body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	font-family:Arial, Helvetica, sans-serif;
	font-size:14px;
}

.main_holder {
	float:left;
	background-color:#eeeeee;
	margin: 0px auto;
	width:500px;
	border: 1px solid #beb9b7;
	padding:20px;
	margin-top:20px;
}

h1 {
	font-size:20px;
}
h2 {
	font-size:16px;
	margin:0 0 0 20px;
	font-weight:bold;
}

.bold {
	font-size:16px;
	margin:0 0 0 20px;
	font-weight:bold;
}
.blue {
	color:#006699;
}
.Green {
	color:#009900
}
.Red {
    color:#990000
}
table td {
	padding:10px;
}
.input {
	float:right;
	margin-top:10px;
}
.clear_both {
	clear:both;
	margin: 0 0 20px 0;
}