    <validationRules>
        <fullName>AmountOnlyForRightSplitType</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   NOT(ISBLANK(Amount__c)),
   NOT(ISPICKVAL(Split_Type__c,&apos;Amount&apos;))
)</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>This field is available only if Split Type = &quot;Amount&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckAcceleratedSettlementOnAccount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   Accelerated_Settlement__c,
   NOT( Account__r.Accelerated_Settlement__c)
)</errorConditionFormula>
        <errorDisplayField>Accelerated_Settlement__c</errorDisplayField>
        <errorMessage>Accelerated settlement is not allowed for this account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckAcceleratedSettlementOnCountry</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   Accelerated_Settlement__c,
   NOT( ICCS_Product_Currency__r.ICCS_Product_Country__r.Accelerated_Settlement__c)
)</errorConditionFormula>
        <errorDisplayField>Accelerated_Settlement__c</errorDisplayField>
        <errorMessage>Accelerated settlement is not allowed for this country</errorMessage>
    </validationRules>
