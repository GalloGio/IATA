        <fullName>First_Call_Letter_required__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(
ISPICKVAL(Security_Type__c , &quot;Financial Guarantee&quot;),
AND( ISPICKVAL(Region__c , &quot;Americas&quot;),ISPICKVAL(Security_Type__c , &quot;Insurance Bond&quot;)),
AND( ISPICKVAL(Region__c , &quot;Americas&quot;),ISPICKVAL(Security_Type__c , &quot;Surety Bond&quot;)),
AND( ISPICKVAL(Region__c , &quot;Asia &amp; Pacific&quot;), OR(ISPICKVAL(Security_Type__c , &quot;Insurance Bond&quot;),ISPICKVAL(Security_Type__c , &quot;TAANZ Bonding Scheme&quot;),ISPICKVAL(Security_Type__c , &quot;DIP&quot;))),

AND(ISPICKVAL(Region__c , &quot;Africa &amp; middle East&quot;), OR( ISPICKVAL(Security_Type__c , &quot;DIP&quot;), ISPICKVAL(Security_Type__c , &quot;CWA Mutual Fund&quot;))),

AND(ISPICKVAL(Region__c , &quot;China &amp; north Asia&quot;), ISPICKVAL(Security_Type__c , &quot;Insurance Bond&quot;))),&quot;Yes&quot;,&quot;No&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>First Call Letter required</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
