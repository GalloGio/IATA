    <validationRules>
        <fullName>Check_Financial_institution_status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISNEW(),ISCHANGED(Financial_Institution__c)), ISPICKVAL(Financial_Institution__r.Institution_Status__c, &quot;Inactive&quot;))</errorConditionFormula>
        <errorDisplayField>Financial_Institution__c</errorDisplayField>
        <errorMessage>Financial institution is not active. Please request agent to submit a financial security from alternative provider.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Financial_Security_Validation_is_mandato</fullName>
        <active>true</active>
        <description>ACR 06749697 - A validation rule to force the user update the field &quot;Financial Security Validation&quot;</description>
        <errorConditionFormula>ISNULL(Financial_Security_Validation__c)==TRUE
&amp;&amp;
ISPICKVAL(Security_Type__c,&quot;Bank Guarantee&quot;)
&amp;&amp;
(Account__r.IATA_ISO_Country__r.Name=&quot;Nigeria&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;South Africa&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Benin&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Senegal&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Mali&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Cote D&apos;ivoire&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Togo&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Niger&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Gabon&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Congo&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Chad&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Mauritania&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Cameroon&quot;||Account__r.IATA_ISO_Country__r.Name=&quot;Burkina Faso&quot;)
&amp;&amp;ISNEW()</errorConditionFormula>
        <errorDisplayField>Financial_Security_Validation__c</errorDisplayField>
        <errorMessage>Please validate the FS and update this field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Financial_institution_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Financial_Institution__c )</errorConditionFormula>
        <errorDisplayField>Financial_Institution__c</errorDisplayField>
        <errorMessage>The financial institution is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Region_must_be_informed_for_FS</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT( ISPICKVAL( Region__c , &quot;Europe&quot;) ),
NOT( ISPICKVAL( Region__c , &quot;Africa &amp; Middle East&quot;) ),
NOT( ISPICKVAL( Region__c , &quot;Asia &amp; Pacific&quot;) ),
NOT( ISPICKVAL( Region__c , &quot;Americas&quot;) ),
NOT( ISPICKVAL( Region__c , &quot;China &amp; North Asia&quot;) )
)</errorConditionFormula>
        <errorDisplayField>Region__c</errorDisplayField>
        <errorMessage>Inform the region</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SARA_Expiry_date_and_Unlimited_Security</fullName>
        <active>true</active>
        <description>Ensures that a financial security is not set as no unlimited security with expiry date</description>
        <errorConditionFormula>and(Unlimited_Security__c ,  NOT(ISNULL(Expiry_Date__c )))</errorConditionFormula>
        <errorMessage>The security is set as unlimited security (with no expiry date) and at the same time as having an expiry date. Please correct this inconsistency before saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SARA_Fill_Release_Encashment_Date</fullName>
        <active>true</active>
        <description>SARA</description>
        <errorConditionFormula>AND(OR (ISPICKVAL(Security_Status__c , &quot;Released&quot;), ISPICKVAL(Security_Status__c , &quot;Encashed&quot;)), ISNULL( Release_Date__c ))</errorConditionFormula>
        <errorDisplayField>Release_Date__c</errorDisplayField>
        <errorMessage>Please fill the date of release or encashment of this guarantee using the field &quot;Release / Encashment Date&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SARA_Fill_Release_reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Security_Status__c , &quot;Released&quot;) , ISPICKVAL(  Release_Reason__c , &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Release_Reason__c</errorDisplayField>
        <errorMessage>Please fill the reason why this security has been released from this picklist</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SARA_Security_expiry_date_mandatory</fullName>
        <active>true</active>
        <description>Request the financial security expiry date or that it is set to &quot;unlimited security&quot;</description>
        <errorConditionFormula>and ( ISNULL(Expiry_Date__c ),  Unlimited_Security__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Expiry_Date__c</errorDisplayField>
        <errorMessage>Please set the expiry date of the security. If it has no expiry, then select the checkbox field &quot;Unlimited Security&quot; below.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SARA_Security_must_be_linked_to_HO</fullName>
        <active>true</active>
        <description>check that the financial security is linked to the top parent account of the agent (except for cross border guarantees and GDI).</description>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Account__r.Location_Type__c , &quot;HO&quot;)),NOT(ISPICKVAL(Account__r.Location_Type__c , &quot;HE&quot;)),NOT(ISPICKVAL(Account__r.Location_Type__c , &quot;GE&quot;)),NOT(ISPICKVAL(Account__r.Location_Type__c , &quot;AO&quot;)) , ISNEW(),  not(ispickval(Security_Type__c , &quot;Cross Border Guarantee&quot;)),not(ispickval(Security_Type__c , &quot;GDI&quot;)))</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>The financial security must be linked to the HO/AO/HE/GE account of the agent, never to a branch. Please classify the security accordingly or contact your administrator if any doubts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SARA_reception_information_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISNEW() ,
OR( ISPICKVAL(Reasonforrequest__c, &quot;&quot;) ,  ISBLANK( Reception_Date__c ) , ISBLANK( Reception_Case__c )  ))</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Please make sure that all the information regarding the reception of the security is filled in: Reception date, request reason and related case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SARA_validity_start_date_mandatory</fullName>
        <active>true</active>
        <description>validity start date mandatory for all securities from 19 July 2012</description>
        <errorConditionFormula>AND( isnull(Validity_Start_Date__c ), ISNEW(), DATEVALUE(CreatedDate ) &gt; DATE(2012,07,18))</errorConditionFormula>
        <errorDisplayField>Validity_Start_Date__c</errorDisplayField>
        <errorMessage>Please enter the validity start date for this security</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Security_Type_Verification</fullName>
