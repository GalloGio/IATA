    <validationRules>
        <fullName>Effective_from_before_effective_To</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TIP_Payment_Provider_Product__r.Provider__r.RecordType.DeveloperName = &quot;TIP_Payment_Provider&quot;, TIP_Effective_From_Date__c &gt; TIP_Effective_To_Date__c )</errorConditionFormula>
        <errorMessage>Effective To Date needs to be after Effective From Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_PP_name_for_IATA_RT</fullName>
        <active>true</active>
        <description>Validates Partner Product&apos;s name if the Bin Range record type is Listed by IATA</description>
        <errorConditionFormula>RecordType.DeveloperName = &apos;Listed_by_IATA&apos; &amp;&amp;  TIP_Payment_Provider_Product__r.Name != $Setup.TIP_Configuration__c.Product_Listed_By_IATA_Name__c</errorConditionFormula>
        <errorMessage>Listed by IATA Bin Range must be linked to the Generic Product</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Range_End_Is_Number</fullName>
        <active>true</active>
        <description>Range End needs to be a number</description>
        <errorConditionFormula>NOT(ISNUMBER(TIP_Range_End__c))</errorConditionFormula>
        <errorDisplayField>TIP_Range_End__c</errorDisplayField>
        <errorMessage>Range To needs to be a number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Range_End_Lower_Than_Start</fullName>
        <active>true</active>
        <description>Range To Should always be higher than Range From</description>
        <errorConditionFormula>AND(ISNUMBER(TIP_Range_Start__c), ISNUMBER(TIP_Range_End__c), VALUE(TIP_Range_Start__c) &gt; VALUE(TIP_Range_End__c))</errorConditionFormula>
        <errorMessage>Range To has to be greater than Range From</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Range_Start_Is_Number</fullName>
        <active>true</active>
        <description>Range Start needs to be a number</description>
        <errorConditionFormula>NOT(ISNUMBER(TIP_Range_Start__c))</errorConditionFormula>
        <errorDisplayField>TIP_Range_Start__c</errorDisplayField>
        <errorMessage>Range From needs to be a number</errorMessage>
    </validationRules>
