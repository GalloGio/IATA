    <validationRules>
        <fullName>Cannot_change_if_inactive_group</fullName>
        <active>true</active>
        <description>It&apos; s not possible to change anything if this local group is inactive</description>
        <errorConditionFormula>AND(
  Active__c=false,
  OR(
    ISCHANGED(Cargo_Passenger__c),
    ISCHANGED(Local_Governance_type__c),
    ISCHANGED(Group_Owner__c),
    ISCHANGED(Country__c),
    ISCHANGED(Cluster__c),

    ISCHANGED(Name),
    ISCHANGED(Division__c),
    ISCHANGED(Abbreviation__c),
    ISCHANGED(Region__c),
    ISCHANGED(Reporting_to__c),
    ISCHANGED(Group_Owner_Text__c),
    ISCHANGED(Restricted_of_Participants__c),
    ISCHANGED(Maximum_Number_of_Participiants__c),
    ISCHANGED(Open_to__c),
    ISCHANGED(Frequency_of_Face_to_Face_Meetings__c),
    ISCHANGED(Mission__c),
    ISCHANGED(Frequency_of_Meetings__c),
    ISCHANGED(Latest_Successes__c),
    ISCHANGED(What_is_Group_Working_on_Now__c)
  )
)</errorConditionFormula>
        <errorMessage>It&apos;s not possible to change this record because it is inactive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delete_group_meetings</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.DeveloperName = &apos;Local_Groups&apos;, 
PRIORVALUE(Number_of_local_group_meetings__c) &gt; Number_of_local_group_meetings__c, 
NOT($Permission.Modify_Local_Group_Owners) 
)</errorConditionFormula>
        <errorMessage>You are not allowed to delete meetings from this group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delete_participants</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(RecordType.DeveloperName = &apos;Reg_Div_Groups&apos;,
PRIORVALUE(Number_of_participants__c) &gt; Number_of_participants__c,
NOT($Permission.Modify_Reg_Div_Group_Owners)
),
AND(RecordType.DeveloperName = &apos;Local_Groups&apos;,
PRIORVALUE(Number_of_participants__c) &gt; Number_of_participants__c,
NOT($Permission.Modify_Local_Group_Owners)
)
)</errorConditionFormula>
        <errorMessage>You are not allowed to delete participants from this group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_a_Division_or_a_Region</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  RecordType.DeveloperName = &quot;Reg_Div_Groups&quot;,

   NOT(ISPICKVAL(Region__c, &quot;N/A&quot;)),
   NOT(ISPICKVAL(Division__c, &quot;N/A&quot;))


)</errorConditionFormula>
        <errorMessage>The group can have either a Division or a Region - but not both at the same time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_of_Reg_Div_Group_cant_be_modified</fullName>
        <active>true</active>
        <description>Name of the group cannot be modified after being approved</description>
        <errorConditionFormula>AND(
  NOT(OR($User.Profile_Name__c =&quot;System Administrator&quot;, $User.Profile_Name__c =&quot;Account management team&quot;)), 
  AND(
  RecordType.DeveloperName = &quot;Reg_Div_Groups&quot;,
  ISCHANGED(Name)
)
)</errorConditionFormula>
        <errorMessage>Name of the group cannot be modified after being approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_edit</fullName>
        <active>true</active>
        <description>Only edit fields if user profile is &apos;Account management team&apos;</description>
        <errorConditionFormula>and(or(ISCHANGED(Cargo_Passenger__c),ISCHANGED(Country__c), 
ISCHANGED(Cluster__c),ISCHANGED(Reporting_to__c), 
ISCHANGED(Local_Governance_type__c),ISCHANGED(Group_Owner__c)), 
!($User.Profile_Name__c=&apos;Account management team&apos;),RecordType.DeveloperName=&apos;Local_Groups&apos;)</errorConditionFormula>
        <errorMessage>Only the Account management team can perform the editing</errorMessage>
    </validationRules>
