    <validationRules>
        <fullName>Cannot_change_if_inactive_group</fullName>
        <active>true</active>
        <description>It&apos; s not possible to change anything if this local group is inactive</description>
        <errorConditionFormula>AND(
  Active__c=false,
  OR(
    ISCHANGED(Cargo_Passenger__c),
    ISCHANGED(Local_Governance_type__c),
    ISCHANGED(Group_Owner__c),
    ISCHANGED(Country__c),
    ISCHANGED(Cluster__c)
  )
)</errorConditionFormula>
        <errorMessage>It&apos;s not possible to change this record because it is inactive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_a_Division_or_a_Region</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
  RecordType.DeveloperName = &quot;Reg_Div_Groups&quot;,

   NOT(ISPICKVAL(Region__c, &quot;N/A&quot;)),
   NOT(ISPICKVAL(Division__c, &quot;N/A&quot;))


)</errorConditionFormula>
        <errorMessage>The group can have either a Division or a Region - but not both at the same time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_of_Reg_Div_Group_cant_be_modified</fullName>
        <active>true</active>
        <description>Name of the group cannot be modified after being approved</description>
        <errorConditionFormula>AND(
  NOT( $User.Profile_Name__c =&quot;System Administrator&quot;), 
  AND(
  RecordType.DeveloperName = &quot;Reg_Div_Groups&quot;,
  ISCHANGED(Name)
)
)</errorConditionFormula>
        <errorMessage>Name of the group cannot be modified after being approved</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Deacitvate</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Deacitvate</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/apex.js&quot;)}

if(&quot;{!$Profile.Name}&quot; != &quot;System Administrator&quot;){
  alert(&quot;The local group can be deactivated only by a system admin&quot;);
}else{


  if(confirm(&quot;Are you sure you want to deactivate this local group? This operation cannot be undone!&quot;)){

    var result = sforce.apex.execute(&quot;NewLocalGroupCtrl&quot;,&quot;deactivateLocalGroup&quot;,{&quot;localgroupid&quot;:&quot;{!LocalGovernance__c.Id}&quot;});

    if(result==&quot;&quot;)
       location.reload();
    else
       alert(result);
  }
}</url>
    </webLinks>
