    <fields>
        <fullName>EF_Payment_Terms_Code__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	ISPICKVAL(EF_Payment_Terms__c, &apos;Payable immediately due net&apos;), &apos;0001&apos;,
	IF(
		ISPICKVAL(EF_Payment_Terms__c, &apos;30 days after invoice date&apos;), &apos;0011&apos;,
		IF(
			ISPICKVAL(EF_Payment_Terms__c, &apos;10 days after invoice date&apos;), &apos;0012&apos;,
			IF(
				ISPICKVAL(EF_Payment_Terms__c, &apos;15 days after invoice date&apos;), &apos;0013&apos;,
				IF(
					ISPICKVAL(EF_Payment_Terms__c, &apos;20 days after invoice date&apos;), &apos;0018&apos;,
					IF(
						ISPICKVAL(EF_Payment_Terms__c, &apos;07 days after invoice date&apos;), &apos;Z007&apos;,
						&apos;&apos;
					)
				)
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>E&amp;F Payment Terms Code</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
