        <description>Used to display a dynamically calculated status: &quot;Expired&quot; status for contracts after their End Date has passed, &quot;Canceled&quot; for canceled contracts or the value of the standard Status field otherwise.</description>
        <externalId>false</externalId>
        <formula>IF (
  ISPICKVAL( Renewal_Status__c, &apos;Canceled&apos;),
  &apos;Canceled&apos;,
  IF (
    AND (
      NOT ( ISBLANK ( Current_Cycle_End_Date__c ) ),
      Current_Cycle_End_Date__c &lt; TODAY()
    ),
    &apos;Expired&apos;,
    TEXT( Status)
  )
)</formula>
        <label>Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
