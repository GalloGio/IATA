    <fields>
        <fullName>IsMine__c</fullName>
        <description>Used for filtering purposes in list views, for the RCRM project. Field is TRUE if the contract &quot;is linked&quot; to the current user, FALSE otherwise. This returns true if the user is either the owner or the RCRM Product Manager of the contract.</description>
        <externalId>false</externalId>
        <formula>AND (
  RecordType.DeveloperName = &apos;RCRM_Contract&apos;,
  OR (
    $User.Id = OwnerId ,
    $User.Id = RCRM_Product_Manager__c 
  )
)</formula>
