    <validationRules>
        <fullName>EF_Contract_Creation_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT($Setup.EF_Skip_Validations__c.Skip_Validation__c),
AND(ISNEW(),  NOT(OR(ISPICKVAL($User.EF_User_Role__c, &apos;Head, Client Services&apos;), ISPICKVAL($User.EF_User_Role__c, &apos;Manager, Business Development&apos;))), RecordType.DeveloperName=&apos;EF_Client_Agreement&apos;))</errorConditionFormula>
        <errorMessage>You are not allowed to create E&amp;F Contracts</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EF_Contract_For_CS_Role_only</fullName>
        <active>true</active>
        <description>Only roles Manager, Business Development, Head, Client Services and Client Services Specialists can modify E&amp;F Contracts.</description>
        <errorConditionFormula>AND
(
    RecordType.DeveloperName = &apos;EF_Client_Agreement&apos;, 
      NOT(ISPICKVAL($User.EF_User_Role__c, &apos;Manager, Business Development&apos;)),
      NOT(ISPICKVAL($User.EF_User_Role__c, &apos;Head, Client Services&apos;)),
      NOT(ISPICKVAL($User.EF_User_Role__c, &apos;Client Services Specialists&apos;)),
	  NOT(BEGINS($User.Profile_Name__c, &apos;System&apos;))
)</errorConditionFormula>
        <errorMessage>Only Client Services Specialists, Client Services Head and Business Development Managers can modify E&amp;F Contracts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EF_Contract_Only_On_Active_Account</fullName>
        <active>true</active>
        <description>EF Contracts can be created just if the account is active.</description>
        <errorConditionFormula>AND( ISNEW(),   RecordType.DeveloperName=&apos;EF_Client_Agreement&apos;,
ISPICKVAL(Account.EF_Status__c, &apos;Inactive&apos;))</errorConditionFormula>
        <errorMessage>E&amp;F Contracts can&apos;t be created if the Account is Inactive for E&amp;F</errorMessage>
    </validationRules>
