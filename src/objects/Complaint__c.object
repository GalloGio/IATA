    <validationRules>
        <fullName>Cannot_close_complaint_in_queue</fullName>
        <active>true</active>
        <description>Prevents from closing the complaint object in the OCIT queue</description>
        <errorConditionFormula>AND( OwnerId = &quot;00G20000000mIiJ&quot;, ISCHANGED( Complaint_Status__c ),  ISPICKVAL(Complaint_Status__c, &quot;Closed&quot;) )</errorConditionFormula>
        <errorMessage>Complaint cannot be closed in the queue, please take ownership of the object before closing it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_Closed_Date_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Complaint_Status__c , &quot;Closed&quot;),  ISNULL( Complaint_Closed_Date__c ))</errorConditionFormula>
        <errorDisplayField>Complaint_Closed_Date__c</errorDisplayField>
        <errorMessage>Please fill the date of closure of the complaint</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_Description_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Complaint_Status__c , &quot;Closed&quot;),  LEN(  Complaint_Description__c  ) = 0)</errorConditionFormula>
        <errorDisplayField>Complaint_Description__c</errorDisplayField>
        <errorMessage>Please fill the description of the complaint</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_Originator_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Complaint_Status__c , &quot;Closed&quot;), ISPICKVAL(     Complaint_Entity_Originator__c   ,&quot;&quot;  ))</errorConditionFormula>
        <errorDisplayField>Complaint_Entity_Originator__c</errorDisplayField>
        <errorMessage>Please fill the originator of the complaint</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_Priority_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Complaint_Status__c , &quot;Closed&quot;),  ISPICKVAL(  Complaint_Priority__c,&quot;&quot;  ))</errorConditionFormula>
        <errorDisplayField>Complaint_Priority__c</errorDisplayField>
        <errorMessage>Please fill the priority of the complaint</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_Reason_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(Complaint_Status__c , &quot;Closed&quot;),  ISPICKVAL(    Complaint_Reason__c  ,&quot;&quot;  ))</errorConditionFormula>
        <errorDisplayField>Complaint_Reason__c</errorDisplayField>
        <errorMessage>Please fill the reason of the complaint</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Complaint_Status_Closed_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(OR(ISPICKVAL(Complaint_Status__c , &quot;Closed&quot;),ISPICKVAL(Complaint_Status__c , &quot;Closed pending follow up&quot;))),  NOT(ISNULL( Complaint_Closed_Date__c )))</errorConditionFormula>
        <errorDisplayField>Complaint_Status__c</errorDisplayField>
        <errorMessage>Please change the status of the complaint to a Closed status</errorMessage>
    </validationRules>
