        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/26.0/connection.js&quot;)}
var prodAccess = new sforce.SObject(&quot;IEC_Product_Access__c&quot;);
prodAccess.id = &apos;{!IEC_Product_Access__c.Id}&apos;;
prodAccess.Active__c = {!NOT(IEC_Product_Access__c.Active__c)};
sforce.connection.update([prodAccess]);
if (prodAccess.Active__c)
	alert(&apos;You have successfully activated the subscription.&apos;);
else
	alert(&apos;You have successfully suspended the subscription.&apos;);
window.location.reload();</url>
