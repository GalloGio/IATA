    <validationRules>
        <fullName>CannotAddInactiveContact</fullName>
        <active>true</active>
        <description>The contact selected is Inactive for E&amp;F and can&apos;t be selected</description>
        <errorConditionFormula>AND(
ISNEW(),
OR( 
ISBLANK(TEXT(EF_Contact__r.EF_Status__c)),
ISPICKVAL( EF_Contact__r.EF_Status__c,&apos;Inactive&apos;)
)
)</errorConditionFormula>
        <errorMessage>The contact selected is Inactive for E&amp;F and can&apos;t be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StatusMustBeConsistentWithEFStatusConta</fullName>
        <active>true</active>
        <description>Status can&apos;t be active if Contact E&amp;F Status is Inactive</description>
        <errorConditionFormula>AND(
ISPICKVAL(EF_Status__c, &apos;Active&apos;),
ISPICKVAL(EF_Contact__r.EF_Status__c, &apos;Inactive&apos;)
)</errorConditionFormula>
        <errorDisplayField>EF_Status__c</errorDisplayField>
        <errorMessage>Status can&apos;t be active if Contact E&amp;F Status is Inactive</errorMessage>
    </validationRules>
