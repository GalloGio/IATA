    <fields>
        <fullName>Report_Date__c</fullName>
        <description>Represents the date when the attachments were reported</description>
        <externalId>false</externalId>
        <label>Posted Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Report_Date_format__c</fullName>
        <description>Represents a text format of the same date. The format is MM/DD/YYYY</description>
        <externalId>false</externalId>
        <formula>CASE(
  MONTH( Report_Date__c),
  1, &quot;January&quot;,
  2, &quot;February&quot;,
  3, &quot;March&quot;,
  4, &quot;April&quot;,
  5, &quot;May&quot;,
  6, &quot;June&quot;,
  7, &quot;July&quot;,
  8, &quot;August&quot;,
  9, &quot;September&quot;,
  10, &quot;October&quot;,
  11, &quot;November&quot;,
  &quot;December&quot;)
+
&apos; &apos;
+
TEXT(Day(Report_Date__c))
+
&apos;, &apos;
+
TEXT(year(Report_Date__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Report Date format</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
