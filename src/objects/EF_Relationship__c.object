    <validationRules>
        <fullName>EF_SIS_Location_ID_format_check</fullName>
        <active>true</active>
        <description>E&amp;F SIS Location ID just can take values 1 to 99 and Main.</description>
        <errorConditionFormula>AND(
       NOT(ISBLANK(EF_SIS_Location_ID__c)), 
       AND( EF_SIS_Location_ID__c&lt;&gt;&quot;Main&quot;,
               NOT(
               OR(
                   REGEX(EF_SIS_Location_ID__c, &quot;[0-9]{1}&quot;),
                   REGEX(EF_SIS_Location_ID__c, &quot;[10-99]{2}&quot;)
                   )
               )
       )
 )</errorConditionFormula>
        <errorDisplayField>EF_SIS_Location_ID__c</errorDisplayField>
        <errorMessage>SIS Location ID just accepts values Main, and numbers from 1 to 99</errorMessage>
    </validationRules>
