    <validationRules>
        <fullName>Status_can_only_be_changed_from_Active</fullName>
        <active>true</active>
        <description>Status value cannot be changed from Completed or Cancelled</description>
        <errorConditionFormula>IF(
  AND(
    NOT(ISNEW()),
    ISCHANGED(Status__c),
    ISPICKVAL(Status__c, &apos;Active&apos;)
  ), 
  TRUE,
  FALSE
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status value cannot be changed from Completed or Cancelled</errorMessage>
    </validationRules>
