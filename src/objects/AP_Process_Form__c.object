    <validationRules>
        <fullName>AP_Check_IINET_Code</fullName>
        <active>true</active>
        <description>If HOT files delivery format is IINET then code must only be 4 chars.</description>
        <errorConditionFormula>(ISPICKVAL(Receive_HOT_files__c, &apos;YES - Daily&apos;) || ISPICKVAL(Receive_HOT_files__c, &apos;YES - Per Period&apos;)) 
&amp;&amp; ISPICKVAL(Hot_files_delivery_format__c,&apos;IINET&apos;)
&amp;&amp;  
(
(ISNULL(IINET_account_number__c) || (LEN(IINET_account_number__c) &lt;&gt; 4))
)</errorConditionFormula>
        <errorDisplayField>IINET_account_number__c</errorDisplayField>
        <errorMessage>The iiNET Account number cannot be empty, and must be 4 characters long, in order to receive HOT files in the iiNET format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AP_HOT_File_Delivery_Format</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(RecordType.DeveloperName==&quot;BSP&quot;, OR(ISPICKVAL(Receive_HOT_files__c, &quot;YES - Daily&quot;), ISPICKVAL(Receive_HOT_files__c, &quot;YES - Per Period&quot;))),
AND(RecordType.DeveloperName==&quot;CASS&quot;, OR(ISPICKVAL(Receive_HOT_files__c, &quot;YES - Billing Period HOT&quot;), ISPICKVAL(Receive_HOT_files__c, &quot;YES - Remittance Period HOT&quot;)))
) &amp;&amp; ISPICKVAL(Hot_files_delivery_format__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Hot_files_delivery_format__c</errorDisplayField>
        <errorMessage>Please select the HOT file delivery format.</errorMessage>
    </validationRules>
