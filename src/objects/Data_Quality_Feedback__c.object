    <validationRules>
        <fullName>EnsureDetailsEntered</fullName>
        <active>true</active>
        <description>If the issue(s) (also) contains other values than &quot;Account or Contact is a duplicate&quot;, make the field Comments /Detailed information mandatory.</description>
        <errorConditionFormula>AND(
OR( 
INCLUDES( Issues__c, &apos;Participation Information is missing&apos;),
INCLUDES( Issues__c, &apos;Address is not complete&apos;),
INCLUDES( Issues__c, &apos;Other&apos;),
ISBLANK( Issues__c)
),
ISBLANK( Comments__c )
)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please provide a detailed description of the issue you are pointing out.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EnsureDuplicateSelected</fullName>
        <active>true</active>
        <description>If the issue(s) contains &quot;Account or Contact is a duplicate&quot;, ensure that a duplicate record is indicated</description>
        <errorConditionFormula>AND( 
INCLUDES( Issues__c, &apos;Account or Contact is a duplicate&apos;),
OR(
AND(NOT(ISBLANK(Account__c)),ISBLANK(Duplicate_Account__c)),
AND(NOT(ISBLANK(Contact__c)),ISBLANK(Duplicate_Contact__c))
)
)</errorConditionFormula>
        <errorMessage>You indicated that the issue is a duplicate Account or Contact. You need to indicate the duplicate record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IdenticalReferenceAndDuplicateAccount</fullName>
        <active>true</active>
        <description>Ensure that the selected duplicate account is not the same as the selected reference account</description>
        <errorConditionFormula>AND (
Account__c = Duplicate_Account__c,
NOT( ISBLANK( Account__c ) )
)</errorConditionFormula>
        <errorDisplayField>Duplicate_Account__c</errorDisplayField>
        <errorMessage>The selected duplicate Account cannot be the same as the reference Account. If there is no duplicate Account, just leave this field empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IdenticalReferenceAndDuplicateContact</fullName>
        <active>true</active>
        <description>Ensure the selected duplicate contact is not the same as the selected reference contact.</description>
        <errorConditionFormula>AND(
Contact__c = Duplicate_Contact__c, 
NOT( ISBLANK( Contact__c ) ) 
)</errorConditionFormula>
        <errorDisplayField>Duplicate_Contact__c</errorDisplayField>
        <errorMessage>The selected duplicate Contact cannot be the same as the reference Contact. If there is no duplicate Contact, just leave this field empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoMoreEditOnClosedItems</fullName>
        <active>true</active>
        <description>Stop the users (except for the System Admin) to edit items marked as &quot;Done&quot;</description>
        <errorConditionFormula>AND (
 ISPICKVAL(PRIORVALUE( Status__c ), &apos;Done&apos;),
 $Profile.Name  &lt;&gt; &apos;System Administrator&apos;
)</errorConditionFormula>
        <errorMessage>Editing of feedback records that are marked as &quot;Done&quot; is not allowed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OnlyOneDuplicatePerRecord</fullName>
        <active>true</active>
        <description>Ensure that either an Account or a Contact is selected, but not both at the same time.</description>
        <errorConditionFormula>ISBLANK( Account__c ) = ISBLANK( Contact__c )</errorConditionFormula>
        <errorMessage>You need to select either a reference Account or a Contact, but not both at the same time.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
