        <fullName>Approve_ID_Card</fullName>
        <availability>online</availability>
        <description>This will change the status of the Case related to this IDCard to Approved</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Approve ID Card</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/apex.js&quot;)} 
sforce.connection.sessionId = &quot;{!$Api.Session_ID}&quot;; 
try{ 

var IDCardAppID = &apos;{!ID_Card_Application__c.Id}&apos;; 
var caseNumber = sforce.apex.execute(&quot;IDCardUtil&quot;,&quot;getIDCardApplicationCase&quot;, {IDCardApplicationID:IDCardAppID}); 
var paymentType = &apos;{!ID_Card_Application__c.Payment_Type__c}&apos;; 
var typeOfApplication = &apos;{!ID_Card_Application__c.Type_of_application__c}&apos;; 

var query = &apos;Select c.ID_Card_Status__c, c.OwnerId, c.ContactId From Case c where CaseNumber = \&apos;&apos;+caseNumber+&apos;\&apos;&apos;; 

var queryresult = sforce.connection.query(query); 
var records = queryresult.getArray(&apos;records&apos;); 
var valid = true; 

if(&apos;{!ID_Card_Application__c.Type_of_application__c}&apos; == &apos;Replacement&apos; ){ 

var queryPAR = &apos;Select Contact__c from Portal_Application_Right__c where contact__r.VER_Number_2__c = \&apos;&apos;+ {!ID_Card_Application__c.VER_Number__c}+&apos;\&apos; and ((( PortalServiceTranslatedName__c=\&apos;bsplink\&apos; or PortalServiceTranslatedName__c=\&apos;casslink\&apos; or PortalServiceTranslatedName__c=\&apos;ifap\&apos;) and Right__c = \&apos;access granted\&apos;) or contact__r.User_Portal_Status__c = \&apos;Approved Admin\&apos; or contact__r.Invoicing_Contact__c = true)&apos;; 
var queryPARresult = sforce.connection.query(queryPAR); 
var recordsPAR = queryPARresult.getArray(&apos;records&apos;); 


if(recordsPAR.length &gt; 0){ 
alert(&apos;Please report to CS team that the contact is applying for change of agency&apos;); 
valid = false;
}
}

if (records.length &gt; 0) { 
var record = records[0]; 
var currentUser = &apos;{!User.Id}&apos;; 

//Validation to check if contact is not IFAP contact with &apos;Financial Assessment Contact&apos; checked 
// for Replacement 
if(typeOfApplication== &apos;Replacement&apos; &amp;&amp; record.ContactId != null) 
{ 
var getContactQuery = &apos;Select c.Financial_Assessment_Contact__c From Contact c where c.Id = \&apos;&apos;+record.ContactId+&apos;\&apos;&apos;; 
var queryGetContact = sforce.connection.query(getContactQuery); 
var ContactRecords = queryGetContact.getArray(&apos;records&apos;); 
if(ContactRecords.length&gt;0) 
{ 
var con = ContactRecords[0]; 
if(con.Financial_Assessment_Contact__c == &apos;true&apos;) 
{	
alert(&quot;The current Contact is a Financial Assessment Contact. It cannot be moved to another Agency. Please check with IFAP admin to make changes regarding this Contact &quot;+record.ContactId); 
valid = false; 
} 

} 
} 

if(valid == true &amp;&amp; record.ID_Card_Status__c == &apos;Approved&apos;) 
{ 
alert(&quot;This application is already approved&quot;); 
valid = false; 
} 
if(valid == true &amp;&amp; record.ID_Card_Status__c == &apos;Pending Manager Approval&apos;) 
{ 
alert(&quot;This application is already submitted for Manager Approval&quot;); 
valid = false; 
} 
// If Current user is not the owner of the case 
if (valid == true &amp;&amp; !((record.OwnerId).indexOf(currentUser) != -1)) 
{ 
alert(&quot;Please, you have to accept the case before approve.&quot;); 
valid = false;	
} 
if(valid == true) 
{ 
if(paymentType == &apos;&apos; || paymentType == null) 
alert(&apos;Please select a payment type&apos;); 

if (paymentType == &apos;Credit Card&apos; || paymentType == &apos;FOC&apos; || paymentType == &apos;Key Account&apos;) 
{ 
var st= sforce.apex.execute(&quot;IDCardUtil&quot;,&quot;UpdateCaseToApproved_Rejected&quot;,{cNum:caseNumber,isApproved:true}); 
if(st == &apos;true&apos;) 
{ 
alert(&quot;The Case has been approved.&quot;); 
window.location.reload(); 
} 
else{ 
alert(&quot;Error! Unable to approve this Application/Case. Issue with Util. \n&quot;+ st); 
} 
} 

if (paymentType == &apos;Invoice&apos;) 
{ 
var getGroupsQuery = &apos;SELECT Id FROM GroupMember where group.name = \&apos;IDCardManager\&apos; and UserOrGroupId = \&apos;&apos;+currentUser+&apos;\&apos;&apos;; 
var queryGetGroups = sforce.connection.query(getGroupsQuery); 
var GroupsRecords = queryGetGroups.getArray(&apos;records&apos;); 

// is part of manager group =&gt; allow to approve right away 
if(GroupsRecords.length&gt;0) 
{ 
var st= sforce.apex.execute(&quot;IDCardUtil&quot;,&quot;UpdateCaseToApproved_Rejected&quot;,{cNum:caseNumber,isApproved:true}); 
if(st == &apos;true&apos;) 
{ 
alert(&quot;The Case has been approved.&quot;); 
window.location.reload(); 
} 
else{ 
alert(&quot;Error! Unable to approve this Application/Case. No Access to groups.\n&quot;+ st); 
}	
} 
else 
{ 
var currentUserManager = &apos;{!User.ManagerId}&apos;; 

if (currentUserManager != null &amp;&amp; currentUserManager != &apos;&apos;) 
{ 

var st = sforce.apex.execute(&quot;IDCardUtil&quot;,&quot;ConfirmChequePayment&quot;, {cNum:caseNumber}); 

if(st == &apos;true&apos;) 
{ 
alert(&quot;This Case is submitted to the manager for approval.&quot;); 
window.location.reload(); 
} 
else { 
alert(&quot;Error! Unable to approve this Application/Case. ConfirmChequePayment. \n&quot;+st); 
} 

} 

else 
{	

alert(&quot;A manager is required to approve this application. Please update your profile with your manager information.&quot;); 
} 
} 

} 
} 
} 
else{	
alert(&quot;No Case is available for this ID Card Application&quot;); 
} 

} 
catch(err) 
{ 
alert(err); 
}</url>
