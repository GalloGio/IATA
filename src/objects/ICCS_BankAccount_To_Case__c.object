    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>BankAccountsBelongToCaseAccount</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(Case__r.AccountId =  ICCS_Bank_Account__r.Account__r.Id)</errorConditionFormula>
        <errorDisplayField>ICCS_Bank_Account__c</errorDisplayField>
        <errorMessage>The bank account chosen doesn&apos;t belong to the case account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CurrencyMatchBankAccount</fullName>
        <active>true</active>
        <description>The currency must match the Bank Account currency.</description>
        <errorConditionFormula>TEXT(CurrencyIsoCode)  &lt;&gt;  
TEXT(ICCS_Bank_Account__r.CurrencyIsoCode)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>The currency must match the Bank Account currency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ICCSNoticeOfAssignmentCheck</fullName>
        <active>true</active>
        <description>The Notice Of Assignment flag can only be set if the selected account has its own Notice of Assignment flag set.</description>
        <errorConditionFormula>AND(
  ICCS_Bank_Account__r.Notice_of_Assignment__c = FALSE,
  Notice_of_Assignment__c = TRUE
)</errorConditionFormula>
        <errorDisplayField>Notice_of_Assignment__c</errorDisplayField>
        <errorMessage>The Notice of Assignment flag cannot be set, because the selected bank account doesn&apos;t have its Notice of Assignment flag set.</errorMessage>
    </validationRules>
</CustomObject>
