    <validationRules>
        <fullName>Check_if_product_is_Active</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   NOT(ISPICKVAL(TIP_Product__r.PP_Status__c, &quot;Active&quot;)),
   ISPICKVAL(TIP_Status__c, &quot;Active&quot;)
)</errorConditionFormula>
        <errorMessage>Payment Product Country Config cannot be activated if related Product is not active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Effective_from_before_effective_To</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TIP_Product__r.Provider__r.RecordType.DeveloperName = &quot;TIP_Payment_Provider&quot;, TIP_Effective_From_Date__c &gt; TIP_Effective_To_Date__c )</errorConditionFormula>
        <errorMessage>Effective to Date needs to be greater than effective from Date</errorMessage>
    </validationRules>
