    <fields>
        <fullName>Attachment_Link__c</fullName>
        <description>This is a link to the last non-image attachment. If there are more than 1 non-image attachment, this will link to the most recently added.</description>
        <externalId>false</externalId>
        <formula>IF (
  NOT( ISBLANK(Last_Attachment_Id__c) ),
  HYPERLINK(&quot;/servlet/servlet.FileDownload?file=&quot; + Last_Attachment_Id__c, &quot;View / Download&quot;),
  &quot;No document attached&quot;
)</formula>
