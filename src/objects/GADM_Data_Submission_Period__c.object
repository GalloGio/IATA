    <validationRules>
        <fullName>DayIsFirstDayInMonth</fullName>
        <active>true</active>
        <description>Date must always be first day of month.</description>
        <errorConditionFormula>DAY(Start_Date__c) &lt;&gt; 1</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Date must always be first day of month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InDueNotification</fullName>
        <active>true</active>
        <errorConditionFormula>In_Due_Notification__c &lt; 0</errorConditionFormula>
        <errorMessage>In due notification must be equal or greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PeriodLenght</fullName>
        <active>true</active>
        <errorConditionFormula>Period_Length__c &lt; 1</errorConditionFormula>
