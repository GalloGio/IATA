    </validationRules>
    <validationRules>
        <fullName>SubmissionCycleEndDate</fullName>
        <active>true</active>
        <errorConditionFormula>Submission_Cycle_End_Day__c &lt;  Submission_Cycle_Start_Day__c</errorConditionFormula>
        <errorMessage>Submission Cycle End Day must be equal or greater than Submission Cycle Start Day</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubmissionCycleEndMonth</fullName>
        <active>true</active>
        <errorConditionFormula>Submission_Cycle_End_Month__c   &lt;  Submission_Cycle_Start_Month__c</errorConditionFormula>
        <errorMessage>Submission Cycle End Month must be equal or greater than Submission Cycle Start Month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubmissionCycleEndtMonth_PeriondLength</fullName>
        <active>true</active>
        <errorConditionFormula>Submission_Cycle_End_Month__c &gt;  Period_Length__c</errorConditionFormula>
        <errorMessage>Submission Cycle End Month must be equal or less than Period Length</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubmissionCycleStartDate</fullName>
        <active>true</active>
