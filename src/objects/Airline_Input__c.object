function getRelatedEmails() {
  try {
		  var topParentId = getTopParentId();
    var query = &quot;SELECT Id, Email&quot;
     + &quot; FROM Contact&quot;
     + &quot; WHERE Accountid IN (&quot;
     + &quot;   SELECT Id&quot;
     + &quot;   FROM Account&quot;
     + &quot;   WHERE Id = &apos;&quot; + topParentId + &quot;&apos; OR Top_parent__c = &apos;&quot; + topParentId + &quot;&apos;&quot;
     + &quot; )&quot;
     + &quot; AND Status__c = &apos;Active&apos;&quot;
     + &quot; AND Airline_Credit_Risk_Manager__c = True&quot;;

    var queryResult = sforce.connection.query(query);
    var records = queryResult.getArray(&quot;records&quot;);
  
    var emails = [];
    for (var i=0; i&lt;records.length; i++) {
      emails.push(encodeURIComponent(records[i].Email));
    }
    return emails;

  } catch (e) {
    console.log(&apos;Error Retrieving emails&apos;);
    return [];
  }
}

function getTopParentId() {
  try {
    var query = &quot;SELECT Id, Top_Parent__c&quot;
     + &quot; FROM Account&quot;
     + &quot; WHERE Id = &apos;{!Airline_Input__c.AirlineId__c}&apos;&quot;;

    var queryResult = sforce.connection.query(query);
    var records = queryResult.getArray(&quot;records&quot;);

    for (var i=0; i&lt;records.length; i++) {
				  var account = records[i];
						if (account.Top_Parent__c==null) {
								return account.Id;
						} else {
						  return account.Top_Parent__c;
						}
    }
    return &apos;&apos;;

  } catch (e) {
    console.log(&apos;Error Retrieving top parent id&apos; + e);
    return &apos;&apos;;
  }
}

// Creating url to send an email
var params = [];
params.push(&quot;retURL=%2F{!Airline_Input__c.Id}&quot;);
params.push(&quot;p3_lkid={!Airline_Input__c.Id}&quot;);
params.push(&quot;template_id=00Xw0000001yTGQ&quot;);
params.push(&quot;p24=&quot; + getRelatedEmails().join(&quot;;&quot;));

window.location.href = &quot;/_ui/core/email/author/EmailAuthor?&quot; + params.join(&quot;&amp;&quot;);</url>
