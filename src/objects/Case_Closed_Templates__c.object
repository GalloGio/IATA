        <externalId>false</externalId>
        <inlineHelpText>values can be: true, false or Default</inlineHelpText>
        <label>CNS Contact</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Language__c</fullName>
        <defaultValue>&quot;Default&quot;</defaultValue>
        <description>language for selected template</description>
