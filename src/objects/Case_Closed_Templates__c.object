        <label>Language</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Source_System__c</fullName>
        <defaultValue>&quot;Default&quot;</defaultValue>
        <externalId>false</externalId>
