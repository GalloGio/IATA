<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Template to use by EmailMessage trigger when closed cases receive emails.
Check class: sendEmailToSenderWhenCaseClosed.cls, method sendEmailToSenderWhenCaseClosed()</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>CNS_Contact__c</fullName>
        <defaultValue>&quot;Default&quot;</defaultValue>
