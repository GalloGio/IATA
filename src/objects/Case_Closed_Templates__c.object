        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Case Closed Templates</label>
    <listViews>
        <fullName>All_Templates</fullName>
        <columns>NAME</columns>
        <columns>Language__c</columns>
