    <validationRules>
        <fullName>Prevent_RE_in_Not_Accreditated_Agencies</fullName>
        <active>true</active>
        <errorConditionFormula>ANG_Manual__c &amp;&amp; ANG_AccountId__r.RecordType.DeveloperName!=&apos;IATA_Agency&apos;</errorConditionFormula>
        <errorMessage>You can only create risk events for Accredited Agencies</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_users_from_changing_issued_date</fullName>
        <active>true</active>
        <description>Prevent users from changing RE&apos;s issue date if the event status is &quot;Active&quot; or &quot;Inactive_Withdrawn&quot; or &quot;Inactive_Revoked&quot; or if RE is &quot;Inactive&quot; and has Expired= true</description>
        <errorConditionFormula>ISCHANGED( ANG_Issued_Date__c )&amp;&amp;( 
ISPICKVAL(PRIORVALUE(ANG_Event_Status__c),&apos;Active&apos; )|| 
ISPICKVAL(PRIORVALUE(ANG_Event_Status__c),&apos;Inactive_Withdrawn&apos;)|| 
ISPICKVAL(PRIORVALUE(ANG_Event_Status__c),&apos;Inactive_Revoked&apos;)|| 
(ISPICKVAL(PRIORVALUE(ANG_Event_Status__c),&apos;Inactive&apos;)&amp;&amp; 
PRIORVALUE(ANG_Expired__c)))</errorConditionFormula>
        <errorDisplayField>ANG_Issued_Date__c</errorDisplayField>
        <errorMessage>You can only change the Issue Date on Risk Events Pending Activation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_users_from_updating_RE_Type</fullName>
        <active>true</active>
        <description>Prevent users from changing the Risk Event Type after inserted</description>
        <errorConditionFormula>!ISNEW() &amp;&amp; ISCHANGED( ANG_Risk_Type__c )</errorConditionFormula>
        <errorDisplayField>ANG_Risk_Type__c</errorDisplayField>
        <errorMessage>You can&apos;t change the Risk Event Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unique_Source_Validation</fullName>
        <active>false</active>
        <description>Only one source field can be filled</description>
        <errorConditionFormula>(
  IF(NOT(ISBLANK(ANG_CaseId__c)) , 1, 0) +
  IF(NOT(ISBLANK(ANG_ChangeCode__c)) , 1, 0) +
  IF(NOT(ISBLANK(ANG_OscarId__c)) , 1, 0)
) &gt; 1</errorConditionFormula>
        <errorMessage>Please, fill only one of the source fields</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
