    <fields>
        <fullName>ANG_Source_Id__c</fullName>
        <description>Shows the Id of the source object or the id of the record itself if no source object is used. This field is used by the ANG_RiskEventGenerator to populate the Unique Key</description>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK(ANG_CaseId__c)), ANG_CaseId__c,
IF(NOT(ISBLANK(ANG_ChangeCode__c)), ANG_ChangeCode__c,
IF(NOT(ISBLANK(ANG_OscarId__c)), ANG_OscarId__c,
Id
)))</formula>
        <label>Source Id</label>
