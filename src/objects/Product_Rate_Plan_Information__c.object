    <validationRules>
        <fullName>IECMonthlyPriceMandatory</fullName>
        <active>true</active>
        <description>Validation rule that makes Monthly Price mandatory when Display as Monthly fee is checked</description>
        <errorConditionFormula>AND(
Display_As_Monthly_Fee__c, 
ISBLANK(Monthly_Price__c))</errorConditionFormula>
        <errorDisplayField>Monthly_Price__c</errorDisplayField>
        <errorMessage>Monthly Price is mandatory when Display as Monthly fee is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IECRatePlansMandatoryFields</fullName>
        <active>true</active>
        <description>Validation Rule that checks for generic Mandatory fields</description>
        <errorConditionFormula>OR( 
ISBLANK(Short_Description__c), 
 LEN(Long_Description__c)=0, 
ISBLANK(Order__c))</errorConditionFormula>
        <errorMessage>The Short , Long Descriptions and Order are Mandatory fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IEC_Triable_Not_Monthly_fee</fullName>
        <active>true</active>
        <description>A triable Rate Plan cannot be displayed as monthly fee</description>
        <errorConditionFormula>AND( Is_Triable__c,  Display_As_Monthly_Fee__c)</errorConditionFormula>
        <errorMessage>A triable Rate Plan cannot be displayed as Monthly fee</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IEC_Triable_Not_Most_Popular</fullName>
        <active>true</active>
        <description>A triable Rate Plan cannot be most popular</description>
        <errorConditionFormula>AND( Is_Triable__c,  Is_Most_Popular__c )</errorConditionFormula>
        <errorMessage>A triable Rate Plan cannot be most popular</errorMessage>
    </validationRules>
