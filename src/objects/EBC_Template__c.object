        <label>Billing Account</label>
        <referenceTo>Zuora__CustomerAccount__c</referenceTo>
        <relationshipLabel>EBC Templates</relationshipLabel>
        <relationshipName>EBC_Templates</relationshipName>
        <required>false</required>
