<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Contains the list of documents (merge templates) in English to be assigned to the Customer Service child cases in the airline suspension process, in function of the country of the child case.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Country_Name__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>false</externalId>
        <inlineHelpText>Type in the participation type (CASS / BSP), a white space, and the country name. 

Example: BSP France.</inlineHelpText>
        <label>Country Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Document_Name__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The Unique name of the template to merge.</inlineHelpText>
        <label>Document Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Airline Suspension Documents</label>
    <visibility>Public</visibility>
</CustomObject>
