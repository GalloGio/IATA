    <fields>
        <fullName>Visible__c</fullName>
        <externalId>false</externalId>
        <formula>OR( 
AND( 
Agency__c = true, 
CONTAINS($User.Customer_type__c,&quot;Agency&quot;) 
), 
AND( 
Not_ICH_Airline__c = true, 
CONTAINS($User.Customer_type__c,&quot;Airline&quot;), 
NOT(CONTAINS($User.Customer_type__c,&quot;ICH&quot;)) 
), 
AND( 
ICH_Airline__c = true, 
CONTAINS($User.Customer_type__c,&quot;Airline&quot;), 
CONTAINS($User.Customer_type__c,&quot;ICH&quot;) 
), 
AND( 
GSA__c = true, 
CONTAINS($User.Customer_type__c,&quot;GSA&quot;) 
), 
AND( 
GSSA__c = true, 
CONTAINS($User.Customer_type__c,&quot;GSSA&quot;) 
), 
AND( 
Name = &quot;Resource Center&quot;, 
CONTAINS($UserRole.Name ,&quot;General Public&quot;) 
), 
Name = &quot;Payment Link&quot;, 
Name = &quot;Resource Center&quot; 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Visible</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
