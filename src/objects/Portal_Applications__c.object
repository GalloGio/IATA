    <fields>
        <fullName>Visible__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(Name = &quot;IFG Production&quot;, Name = &quot;IFG Sandbox&quot;),
$User.IFG_Account_Member__c
,
OR( 
   
    AND( 
    Agency__c = true, 
    $User.Customer_type__c=&quot;Agency&quot; 
    ), 
    AND( 
    VisibleCNS__c= true, 
    $User.Customer_type__c=&quot;CNS Agency&quot; 
    ), 
    AND( 
    Not_ICH_Airline__c = true, 
    CONTAINS($User.Customer_type__c,&quot;Airline&quot;), 
    NOT(CONTAINS($User.Customer_type__c,&quot;ICH&quot;)) 
    ), 
    AND( 
    ICH_Airline__c = true, 
    CONTAINS($User.Customer_type__c,&quot;Airline&quot;), 
    CONTAINS($User.Customer_type__c,&quot;ICH&quot;) 
    ), 
    AND( 
    GSSA__c = true, 
    CONTAINS($User.Customer_type__c,&quot;GSSA&quot;) 
    ), 
    AND( 
    Name = &quot;Resource Center&quot;, 
    CONTAINS($UserRole.Name ,&quot;General Public&quot;) 
    ), 
    AND( 
    Others__c = true, 
    CONTAINS($User.Contact_Account_Record_Type__c  ,&quot;Others&quot;) 
    ), 
    AND(Name = &quot;Payment Link&quot;, NOT($User.Customer_type__c=&quot;CNS Agency&quot;)), 
    AND(Name = &quot;Paypal&quot;,$User.CNS_Account_or_Agency__c),
    AND(Name = &quot;Resource Center&quot;, NOT($User.Customer_type__c=&quot;CNS Agency&quot;)),
    AND(Name = &quot;Transparency in Payments (TIP)&quot;,($User.Is_TIP_User__c))
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Visible</label>
