    <validationRules>
        <fullName>Restrict_by_Accreditation_Type</fullName>
        <active>true</active>
        <description>The sector equals to &quot;Cargo Agent&quot; then the Accreditation Type must be empty.</description>
        <errorConditionFormula>AND(
TEXT(ANG_Restrict_by_Agency_Sector__c) = &apos;Cargo Agent&apos;,
NOT(ISBLANK(ANG_Restrict_by_Accreditation_Type__c))
)</errorConditionFormula>
        <errorMessage>Restriction of visibility by Accreditation Type can only occur for portal services that are also restricted by Agency Sector = Travel Agent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_by_Agency_Category</fullName>
        <active>true</active>
        <description>Only allow to save when Restrict by Agency Category is not empty IF Agency checkbox = TRUE</description>
        <errorConditionFormula>AND(
NOT(Agency__c),
NOT(ISBLANK(ANG_Restrict_by_Agency_Category__c))
)</errorConditionFormula>
        <errorMessage>Please select to restrict visibility by Agency in order to Restrict by Agency Category.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_by_Agency_Sector</fullName>
        <active>true</active>
        <description>Only allow to save when Restrict by Agency Sector is not empty IF Agency checkbox = TRUE</description>
        <errorConditionFormula>AND(
NOT(Agency__c),
TEXT(ANG_Restrict_by_Agency_Sector__c) != &quot;&quot;
)</errorConditionFormula>
        <errorMessage>Please select to restrict visibility by Agency in order to Restrict by Agency Sector.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SCIM_Type_Check</fullName>
        <active>true</active>
        <description>Checks if SCIM_App_Name__c is not empty when Provisioning_Type__c=SCIM</description>
        <errorConditionFormula>AND(ISPICKVAL(Provisioning_Type__c , &apos;SCIM&apos;),  ISBLANK(SCIM_App_Name__c))</errorConditionFormula>
        <errorDisplayField>SCIM_App_Name__c</errorDisplayField>
        <errorMessage>SCIM APP Name field must be provided when Provisioning Type is SCIM</errorMessage>
    </validationRules>
