    <fields>
        <fullName>ANG_Accre_Tp__c</fullName>
        <description>Flag used to filter the Portal Services SOQL Query configured on the custom setting: ISSP Service List View</description>
        <externalId>false</externalId>
        <formula>OR(
OR($User.ANG_Sector__c = &quot;Airline&quot;, $User.ANG_Sector__c = &quot;Cargo Agent&quot;),
IF(INCLUDES(ANG_Restrict_by_Accreditation_Type__c, &apos;Cash&apos;) &amp;&amp; $User.ANG_Accreditation_Model__c = &apos;Cash&apos;, true,
IF(INCLUDES(ANG_Restrict_by_Accreditation_Type__c, &apos;Cashless&apos;) &amp;&amp; $User.ANG_Accreditation_Model__c = &apos;Cashless&apos;, true, false)),
ISBLANK(ANG_Restrict_by_Accreditation_Type__c)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Flag Restrict by Accreditation Type</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
