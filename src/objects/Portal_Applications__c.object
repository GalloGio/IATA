    <fields>
        <fullName>ANG_Flag_Country_Coverage__c</fullName>
        <description>Flag used to filter the Portal Services SOQL Query configured on the custom setting: ISSP Service List View</description>
        <externalId>false</externalId>
        <formula>IF(
            ISPICKVAL(Country_Visibility_Control__c, &quot;SHOW_ALL&quot;), 
            TRUE,
            OR (
            AND(
            IF(
            ISPICKVAL(Country_Visibility_Control__c, &quot;HIDE&quot;),
            AND(
            NOT(CONTAINS(ANG_Country_Coverage__c, $User.ANG_Customer_Country_ISO__c)),
            NOT(CONTAINS(ANG_Country_Coverage_2__c, $User.ANG_Customer_Country_ISO__c))
            ),
            OR(
            CONTAINS(ANG_Country_Coverage__c, $User.ANG_Customer_Country_ISO__c),
            CONTAINS(ANG_Country_Coverage_2__c, $User.ANG_Customer_Country_ISO__c)
            )
            ),
            OR(
            TRIM(ANG_Country_Coverage__c) != &quot;&quot;,
            TRIM(ANG_Country_Coverage_2__c) != &quot;&quot;
            )
            ),
            TRIM(ANG_Country_Coverage__c) = &quot;&quot;,
            TRIM(ANG_Country_Coverage_2__c) = &quot;&quot;
            )
            )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Flag Country Coverage</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ANG_Flag_Restrict_Accredited_Agent__c</fullName>
        <description>Flag used to filter the Portal Services SOQL Query configured on the custom setting: ISSP Service List View</description>
        <externalId>false</externalId>
        <formula>OR(
  (
    AND (
      CONTAINS($User.Customer_type__c,&quot;Agency&quot;),
      $User.ANG_Is_Accredited_Agent__c,
      ANG_Accredited_Agents_Only__c
    )
  ),
  (
    AND (
      CONTAINS($User.Customer_type__c,&quot;Agency&quot;),
      ANG_Accredited_Agents_Only__c = false
    )
  ),
  (
    NOT(CONTAINS($User.Customer_type__c,&quot;Agency&quot;))  
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Flag Restrict Accredited Agent</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ANG_Flag_Restrict_Sector__c</fullName>
        <description>Flag used to filter the Portal Services SOQL Query configured on the custom setting: ISSP Service List View</description>
        <externalId>false</externalId>
        <formula>OR(
  (
    AND($User.ANG_Sector__c != &quot;Travel Agent&quot;, $User.ANG_Sector__c != &quot;Cargo Agent&quot;)
  ),
  (
    AND (OR($User.ANG_Sector__c = &quot;Travel Agent&quot;, $User.ANG_Sector__c = &quot;Cargo Agent&quot;),
           $User.ANG_Sector__c = TEXT(ANG_Restrict_by_Agency_Sector__c))  
  ),
  (
    TEXT(ANG_Restrict_by_Agency_Sector__c) = &quot;&quot;
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Flag Restrict Sector</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ANG_Flag_Restrict_by_Agency_Category__c</fullName>
        <externalId>false</externalId>
        <formula>OR(
ISBLANK(ANG_Restrict_by_Agency_Category__c),
AND(NOT(ISBLANK(ANG_Restrict_by_Agency_Category__c)),
   IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;Airline Point of Sale&apos;) &amp;&amp; $User.ANG_Category__c = &apos;Airline Point of Sale&apos;, true,
    IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;Domestic Agent&apos;) &amp;&amp; $User.ANG_Category__c = &apos;Domestic Agent&apos;, true,
     IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;GSA General Sales Agent&apos;) &amp;&amp; $User.ANG_Category__c = &apos;GSA General Sales Agent&apos;, true,
      IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;Airport Handling Agent&apos;) &amp;&amp; $User.ANG_Category__c = &apos;Airport Handling Agent&apos;, true,
       IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;IATA Passenger Sales Agent&apos;) &amp;&amp; $User.ANG_Category__c = &apos;IATA Passenger Sales Agent&apos;, true,
        IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;MSO Member Sales Office&apos;) &amp;&amp; $User.ANG_Category__c = &apos;MSO Member Sales Office&apos;, true,
         IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;TIDS Agent&apos;) &amp;&amp; $User.ANG_Category__c = &apos;TIDS Agent&apos;, true,
         IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;IATA Cargo Agent&apos;) &amp;&amp; $User.ANG_Category__c = &apos;IATA Cargo Agent&apos;, true,
          IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;CASS Associate&apos;) &amp;&amp; $User.ANG_Category__c = &apos;CASS Associate&apos;, true,
           IF(INCLUDES(ANG_Restrict_by_Agency_Category__c, &apos;Import Agent&apos;) &amp;&amp; $User.ANG_Category__c = &apos;Import Agent&apos;, true, false)
          )
         )
        )
       )
      )
     )
    )
   )
  )
 ),
NOT(CONTAINS($User.Customer_type__c,&quot;Agency&quot;))
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Flag Restrict by Agency Category</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
