        <fullName>VisibleCNS__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Defines if the service should be visible on the CNS portal branding.</description>
        <externalId>false</externalId>
        <inlineHelpText>Defines if the service should be visible on the CNS portal branding.</inlineHelpText>
        <label>Visible on CNS</label>
