    <fields>
        <fullName>Visible__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(Name = &quot;ID Card Online Platform&quot;, $User.ANG_Sector__c = &quot;Airline&quot;),
			false,
IF(OR(
    Name = &quot;IFG Production&quot;, Name = &quot;IFG Sandbox&quot;),
    $User.IFG_Account_Member__c,
				IF(
								AND(Name = &quot;ID Card Online Platform&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;AS&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;GU&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;MP&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;PW&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;UP&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;US&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;PR&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;VI&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;MH&quot;,
                    $User.ANG_Customer_Country_ISO__c &lt;&gt; &quot;FM&quot;),
                true,
                IF( AND(Name = &quot;IATAN Online Services&quot;,
                    OR($User.ANG_Customer_Country_ISO__c = &quot;AS&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;GU&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;MP&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;PW&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;UP&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;US&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;PR&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;VI&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;MH&quot;,
                    $User.ANG_Customer_Country_ISO__c = &quot;FM&quot;)),
                true,
    OR(
        AND(Agency__c = true,
            $User.Customer_type__c=&quot;Agency&quot;,
            NOT(Name = &quot;Access BOP&quot;),
            NOT(Name = &quot;Payment Link&quot;),
	    NOT(Name = &quot;ID Card Online Platform&quot;),
            NOT(Name = &quot;IATAN Online Services&quot;)),
        AND(VisibleCNS__c= true, 
            $User.Customer_type__c=&quot;CNS Agency&quot;), 
        AND(Not_ICH_Airline__c = true,
            Name != &quot;Payment Link&quot;, 
            CONTAINS($User.Customer_type__c,&quot;Airline&quot;), 
            NOT(CONTAINS($User.Customer_type__c,&quot;ICH&quot;))), 
        AND(ICH_Airline__c = true, 
            Name != &quot;Payment Link&quot;, 
            CONTAINS($User.Customer_type__c,&quot;Airline&quot;), 
            CONTAINS($User.Customer_type__c,&quot;ICH&quot;)), 
        AND(GSSA__c = true, 
            CONTAINS($User.Customer_type__c,&quot;GSSA&quot;)), 
        AND(Name = &quot;Resource Center&quot;, 
            CONTAINS($UserRole.Name ,&quot;General Public&quot;)), 
        AND(Others__c = true, 
            CONTAINS($User.Contact_Account_Record_Type__c  ,&quot;Others&quot;)), 
        AND(Name = &quot;Payment Link&quot;,
            NOT($User.Customer_type__c=&quot;CNS Agency&quot;),
            NOT($User.ANG_Category__c=&quot;IATAN Passenger Sales Agent (USA Only)&quot;),
            NOT($User.ANG_Category__c=&quot;Travel Service Intermediaries-TSI (USA Only)&quot;)),
            AND(Name = &quot;Webstar&quot;,
            OR($User.ANG_Category__c=&quot;IATAN Passenger Sales Agent (USA Only)&quot;,
            $User.ANG_Category__c=&quot;Travel Service Intermediaries-TSI (USA Only)&quot;)),
        AND(Name = &quot;Paypal&quot;,
            $User.CNS_Account_or_Agency__c),
        AND(Name = &quot;Resource Center&quot;, 
            NOT($User.Customer_type__c=&quot;CNS Agency&quot;),
            NOT(CONTAINS($User.Customer_type__c,&quot;Airline&quot;))),
        AND(Name = &quot;Transparency in Payments (TIP)&quot;,
            ($User.Is_TIP_User__c)),
        AND(Name = &quot;Access BOP&quot;,
            $User.ANG_Customer_Country_ISO__c = &quot;CN&quot;),
            AND(Name = &quot;IATA Accreditation &amp; Changes&quot;,
            NOT($User.CNS_Account_or_Agency__c),
            NOT($User.ANG_Sector__c=&quot;Cargo Agent&quot;))
))
))
)</formula>
        <label>Visible</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
