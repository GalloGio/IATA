<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Holds lists of email addresses that will receive notifications when a case is assigned to a specific queue.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Email_Adresses__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Include all the email adresses that should receive the notification. Only write the email address and separate multiple addresses with a comma ( , ) .</inlineHelpText>
        <label>Email Adresses</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Queue_Name__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>false</externalId>
        <label>Queue Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>Custom Queue Notification Emails</label>
    <listViews>
        <fullName>All_Notifications</fullName>
        <columns>NAME</columns>
        <columns>Queue_Name__c</columns>
        <columns>Email_Adresses__c</columns>
        <columns>LAST_UPDATE</columns>
        <columns>UPDATEDBY_USER</columns>
        <filterScope>Everything</filterScope>
        <label>All Notifications</label>
        <language>en_US</language>
    </listViews>
    <visibility>Public</visibility>
</CustomObject>
