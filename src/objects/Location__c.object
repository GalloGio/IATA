    <validationRules>
        <fullName>No_Change_On_Account</fullName>
        <active>true</active>
        <description>Forbid update in Account</description>
        <errorConditionFormula>AND(NOT(ISNEW())
  , NOT(ISBLANK(PRIORVALUE(SAP_Id__c)))
  , ISCHANGED(Account__c)
)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Data entry is not allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Change_On_Contact</fullName>
        <active>true</active>
        <description>Forbid entry/update in Contact</description>
        <errorConditionFormula>OR(AND(ISPICKVAL(Type__c, &quot;Sold To&quot;)
     , NOT(ISBLANK(Contact__c))
   )
 , AND(NOT(ISNEW())
     , NOT(ISBLANK(PRIORVALUE(SAP_Id__c)))
     , ISCHANGED(Contact__c)
   )
)</errorConditionFormula>
        <errorDisplayField>Contact__c</errorDisplayField>
        <errorMessage>Data entry is not allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Change_On_SAP_Id</fullName>
        <active>true</active>
        <description>Forbid entry/update in SAP Id</description>
        <errorConditionFormula>OR(AND(NOT(OR(ISPICKVAL(Type__c, &quot;Sold To&quot;)
            , ISPICKVAL(Type__c, &quot;Bill To&quot;)
            , ISPICKVAL(Type__c, &quot;Ship To&quot;)
          ))
     , NOT(ISBLANK(SAP_Id__c))
   )
 , AND(NOT(ISNEW())
     , NOT(ISBLANK(PRIORVALUE(SAP_Id__c)))
     , ISCHANGED(SAP_Id__c)
   )
)</errorConditionFormula>
        <errorDisplayField>SAP_Id__c</errorDisplayField>
        <errorMessage>Data entry is not allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Change_On_Type</fullName>
        <active>true</active>
        <description>Forbid update in Type</description>
        <errorConditionFormula>AND(NOT(ISNEW())
  , NOT(ISBLANK(PRIORVALUE(SAP_Id__c)))
  , ISCHANGED(Type__c)
)</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>Data entry is not allowed</errorMessage>
    </validationRules>
    <validationRules>
