    <validationRules>
        <fullName>Hierarchy_of_comment</fullName>
        <active>true</active>
        <description>Type of comment is mandatory</description>
        <errorConditionFormula>(Achievement__c = TRUE) &amp;&amp; (Parent_Objective__r.Account__c  &lt;&gt;  Null) &amp;&amp;
((ISPICKVAL(Division__c,&quot;&quot;)) &amp;&amp; (ISPICKVAL(Region__c,&quot;&quot;)) &amp;&amp; (ISPICKVAL(Area__c,&quot;&quot;)))</errorConditionFormula>
        <errorDisplayField>Achievement__c</errorDisplayField>
        <errorMessage>Please specify the hierarchy your comment is related to</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RLT_comment_Country</fullName>
        <active>true</active>
        <description>A country or divisional news is to be reported on the RLT report. And the area or division are needed</description>
        <errorConditionFormula>(Country_News__c = TRUE) &amp;&amp; (ISPICKVAL(Area__c,&quot;&quot;))</errorConditionFormula>
        <errorMessage>If this comment is intended to be in the RLT report under the country news, please specify the area</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RLT_comment_Division</fullName>
        <active>true</active>
        <description>A country or divisional news is to be reported on the RLT report. And the area or division are needed</description>
        <errorConditionFormula>(Divisional_News__c = TRUE) &amp;&amp; (ISPICKVAL(Division__c,&quot;&quot;))</errorConditionFormula>
        <errorMessage>If this comment is intended to be in the RLT report under the divisional news, please specify the division</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_of_independent_comment</fullName>
        <active>true</active>
        <description>Type of comment is mandatory</description>
        <errorConditionFormula>(RecordType.DeveloperName = &quot;Objective_Independent&quot;) &amp;&amp; ((Divisional_News__c = FALSE) &amp;&amp; (Country_News__c = FALSE))</errorConditionFormula>
        <errorMessage>Please specify the type of comment</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>New_Comment</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>New News</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/apex/NewComment?parent={!Objective__c.Id}</url>
    </webLinks>
