    </validationRules>
    <validationRules>
        <fullName>Mandatory_Chargeback_on_TIP_Product</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(PP_Chargeback_Airlines__c, &apos;Y&apos;), RecordType.DeveloperName = &apos;TIP_Product&apos;, ISBLANK(PP_Chargeback_airlines_duration__c))</errorConditionFormula>
        <errorDisplayField>PP_Chargeback_airlines_duration__c</errorDisplayField>
        <errorMessage>If subject to chargeback, please indicate the maximun number of days after transaction for which a chargeback can be issued.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Prod_Code_Mandatory_Not_Rejected</fullName>
        <active>true</active>
        <description>Payment Product Code is mandatory for partner products to be approved</description>
        <errorConditionFormula>RecordType.DeveloperName==&apos;TIP_Product&apos; 
&amp;&amp; NOT(ISPICKVAL(PP_Status__c,&apos;REJECTED&apos;) || ISPICKVAL(PP_Status__c,&apos;DRAFT&apos;))
&amp;&amp; ISBLANK(PP_Payment_Product_Code__c)
&amp;&amp; NOT(ISBLANK(PP_Effective_from_Date__c))</errorConditionFormula>
        <errorMessage>Please fill in the Payment Product Code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Provider_should_be_active</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   NOT(ISPICKVAL(Payment_Provider_Account_Role__r.Status__c , &quot;active&quot;)),
   ISPICKVAL(PP_Status__c  , &quot;Active&quot;)
)</errorConditionFormula>
        <errorMessage>Partner Product cannot be activated if Provider Account is not active</errorMessage>
    </validationRules>
