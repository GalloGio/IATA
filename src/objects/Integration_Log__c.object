    <fields>
        <fullName>Context__c</fullName>
        <description>An optional text field used to indicate the context of the interface transaction with a consistent format of &lt;Domain&gt;.&lt;Process&gt;.&lt;Salesforce Entity&gt;.&lt;Operation&gt; For example E_and_F.Account_Creation.Account.Create</description>
        <externalId>false</externalId>
        <label>Context</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>External_Transaction_Id__c</fullName>
        <description>A reference to a transaction held outside of Salesforce e.g. an identifier used by integration in an asynchronous process.</description>
        <externalId>true</externalId>
        <label>External Transaction Id</label>
        <length>255</length>
        <required>false</required>
