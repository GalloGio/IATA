    <fields>
        <fullName>Status__c</fullName>
        <description>Indicates the status of an interface transaction. Possible values are: - Success (Indicates that an interface transaction completed successfully). - Failure (Indicates that an interface transaction completed unsuccessfully). - In Progress (Indicates that an asynchronous process is in progress. Upon completion the Status and Status Detail will be updated accordingly)</description>
        <externalId>false</externalId>
        <label>Status</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Success</fullName>
                    <default>false</default>
                    <label>Success</label>
                </value>
                <value>
                    <fullName>Failure</fullName>
                    <default>false</default>
                    <label>Failure</label>
                </value>
                <value>
                    <fullName>In Progress</fullName>
                    <default>false</default>
                    <label>In Progress</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
