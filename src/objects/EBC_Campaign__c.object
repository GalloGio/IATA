    <fields>
        <fullName>Is_Step1_Valid__c</fullName>
        <description>Verify if the Step 1 is valid and meet the validation.</description>
        <externalId>false</externalId>
        <formula>!ISBLANK(Name) &amp;&amp; !ISBLANK(From_Name__c) &amp;&amp; !ISBLANK(Subject__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Step1 Valid</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Step2_Valid__c</fullName>
        <description>Verify if the Step 2 is valid and meet the validation.</description>
        <externalId>false</externalId>
        <formula>false &amp;&amp;  Is_Step1_Valid__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Step2 Valid</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Step3_Valid__c</fullName>
        <description>Verify if the Step 3 is valid and meet the validation.</description>
        <externalId>false</externalId>
        <formula>true &amp;&amp;  Is_Step2_Valid__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Step3 Valid</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Step4_Valid__c</fullName>
        <description>Verify if the Step 4 is valid and meet the validation.</description>
        <externalId>false</externalId>
        <formula>true &amp;&amp;  Is_Step3_Valid__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Step4 Valid</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Step5_Valid__c</fullName>
        <description>Verify if the Step 5 is valid and meet the validation.</description>
        <externalId>false</externalId>
        <formula>false &amp;&amp;  Is_Step4_Valid__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Step5 Valid</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Step6_Valid__c</fullName>
        <description>Verify if the Step 6 is valid and meet the validation.</description>
        <externalId>false</externalId>
        <formula>false &amp;&amp; Is_Step5_Valid__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Step6 Valid</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Maximum_Allowed_Step__c</fullName>
        <description>Return the furthest allowed step in the wizard based on the Is_StepX_Valid__c formulas</description>
        <externalId>false</externalId>
        <formula>IF(
	Is_Step6_Valid__c,
	7,
	IF(
		Is_Step5_Valid__c,
		6,
		IF(
			Is_Step4_Valid__c,
			5,
			IF(
				Is_Step3_Valid__c,
				4,
				IF(
					Is_Step2_Valid__c,
					3,
					2
				)
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Maximum Allowed Step</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
