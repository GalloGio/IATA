    <fields>
        <fullName>Maximum_Allowed_Step__c</fullName>
        <description>Return the furthest allowed step in the wizard based on the Is_StepX_Valid__c formulas</description>
        <externalId>false</externalId>
        <formula>IF(
	Is_Step6_Valid__c,
	7,
	IF(
		Is_Step5_Valid__c,
		6,
		IF(
			Is_Step4_Valid__c,
			5,
			IF(
				Is_Step3_Valid__c,
				4,
				IF(
					Is_Step2_Valid__c,
					3,
					2
				)
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Return the furthest allowed step in the wizard based on the Is_StepX_Valid__c formulas</inlineHelpText>
        <label>Maximum Allowed Step</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
