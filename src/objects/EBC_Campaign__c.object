    <validationRules>
        <fullName>Design_vs_Status</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISPICKVAL($Profile.UserType, &quot;Standard&quot;)) &amp;&amp;

PRIORVALUE( EBC_Design__c  ) != EBC_Design__c &amp;&amp;

TEXT(Status__c) != &apos;DRAFT&apos;</errorConditionFormula>
        <errorDisplayField>EBC_Design__c</errorDisplayField>
        <errorMessage>The Design can only be changed when the campaign status is Draft</errorMessage>
    </validationRules>
