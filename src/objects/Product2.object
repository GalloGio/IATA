    <validationRules>
        <fullName>ICCS_Product_Family_selection</fullName>
        <active>true</active>
        <description>Meant to ensure the ICCS Admin does not select any other Product Family item than &quot;ICCS&quot;</description>
        <errorConditionFormula>AND(
$Profile.Name = &quot;FDS ICCS Administrator&quot;,
NOT(ISPICKVAL(Family,&quot;ICCS&quot;))
)</errorConditionFormula>
        <errorDisplayField>Family</errorDisplayField>
        <errorMessage>You can&apos;t select any other Product Family than &quot;ICCS&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Manager_mandatory_for_RCRM_prod</fullName>
        <active>true</active>
        <description>The Product Manager field is mandatory for the products that are flagged as RCRM products.</description>
        <errorConditionFormula>AND (
  RCRM_Product__c = true,
  ISBLANK( Product_Manager_lookup__c )
)</errorConditionFormula>
        <errorDisplayField>Product_Manager_lookup__c</errorDisplayField>
        <errorMessage>The Product Manager is mandatory for products within the RCRM project scope.</errorMessage>
    </validationRules>
