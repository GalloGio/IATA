    </validationRules>
    <validationRules>
        <fullName>Open_points_required</fullName>
        <active>true</active>
        <description>Open points are required if status goes to feedback required</description>
        <errorConditionFormula>IF( AND(ISPICKVAL(Status__c, &apos;Feedback required&apos;),ISBLANK( Communication_Comments__c )) , true, false)</errorConditionFormula>
        <errorDisplayField>Communication_Comments__c</errorDisplayField>
        <errorMessage>Cannot be blank if Status = Feedback required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>mandatory_tech_owner</fullName>
