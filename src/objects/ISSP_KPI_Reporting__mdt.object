                    <fullName>Case Escalation</fullName>
                    <default>false</default>
                    <label>Case Escalation</label>
                </value>
                <value>
                    <fullName>Case Reopening</fullName>
                    <default>false</default>
                    <label>Case Reopening</label>
                </value>
                <value>
                    <fullName>Send Email</fullName>
                    <default>false</default>
                    <label>Send Email</label>
                </value>
