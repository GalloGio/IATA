    <validationRules>
        <fullName>N1_Airlines_cannot_be_equal</fullName>
        <active>true</active>
        <errorConditionFormula>Airline_1__c = Airline_2__c</errorConditionFormula>
        <errorDisplayField>Airline_2__c</errorDisplayField>
        <errorMessage>An airline cannot have an agreement with itself. Please change the target Airline.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>N2_No_agreements_for_non_BIETA_account</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISNEW(),
 ISPICKVAL(MITAorBIETA__c, &apos;BIETA&apos;),
 Active__c,
 OR(
  NOT(Airline_1__r.BIETA_Member__c),   
  NOT(Airline_2__r.BIETA_Member__c)
 )
)</errorConditionFormula>
        <errorMessage>A new MITA Agreement cannot be created if the airlines are not BIETA members</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>N2_No_agreements_for_non_MITA_account</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISNEW(),
 ISPICKVAL(MITAorBIETA__c, &apos;MITA&apos;),
 Active__c,
 OR(
  NOT(Airline_1__r.MITA_Member__c),   
  NOT(Airline_2__r.MITA_Member__c)
 )
)</errorConditionFormula>
        <errorMessage>A new MITA Agreement cannot be created if the airlines are not MITA members</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>N3_BIETA_Agreement_type_not_enabled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 Active__c,

 IF(CASE(TEXT(Agreement_type__c),

 &apos;Bilateral, both carriers can issue electronic tickets&apos;,
 IF((OR(ISNULL(Airline_1__r.BIETA_Bilateral_Date__c),ISNULL(Airline_2__r.BIETA_Bilateral_Date__c))),&apos;t&apos;,&apos;f&apos;),
  
 &apos;One Way Passenger Issuing Airline&apos;,
 IF((OR(ISNULL(Airline_1__r.BIETA_One_way_Electronic_Issuing_Airline__c),ISNULL(Airline_2__r.BIETA_One_way_Electronic_Participating__c))),&apos;t&apos;,&apos;f&apos;),
 
 &apos;One Way Passenger Partcipating&apos;,
 IF((OR(ISNULL(Airline_1__r.BIETA_One_way_Electronic_Participating__c),ISNULL(Airline_2__r.BIETA_One_way_Electronic_Issuing_Airline__c))),&apos;t&apos;,&apos;f&apos;),
 
 &apos;One Way Intermodal Issuing Date&apos;,
 IF((OR(ISNULL(Airline_1__r.BIETA_One_way_Intermodal_Issuing_Airline__c),ISNULL(Airline_2__r.BIETA_One_way_Intermodal_Participating__c))),&apos;t&apos;,&apos;f&apos;),
 
 &apos;One Way Intermodal Participating Date&apos;,
 IF((OR(ISNULL(Airline_1__r.BIETA_One_way_Intermodal_Participating__c),ISNULL(Airline_2__r.BIETA_One_way_Intermodal_Issuing_Airline__c))),&apos;t&apos;,&apos;f&apos;),
 
 &apos;f&apos;
)=&apos;t&apos;,true,false)
)</errorConditionFormula>
        <errorMessage>This agreement type is not enabled for the selected airlines</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>N3_MITA_Agreement_type_not_enabled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 Active__c,
 IF(CASE(TEXT(Agreement_type__c),

 &apos;The Passenger and Cargo Interline Traffic Agreement&apos;,
 IF(
  (OR(ISNULL(Airline_1__r.MITA_IATA_Interline_Passenger__c),
  ISNULL(Airline_2__r.MITA_IATA_Interline_Passenger__c),
  ISNULL(Airline_1__r.MITA_IATA_Interline_Cargo__c),
  ISNULL(Airline_2__r.MITA_IATA_Interline_Cargo__c))
  ),&apos;t&apos;,&apos;f&apos;),
 
 &apos;The Passenger Interline Agreement&apos;,
 IF((OR(ISNULL(Airline_1__r.MITA_IATA_Interline_Passenger__c),ISNULL(Airline_2__r.MITA_IATA_Interline_Passenger__c))),&apos;t&apos;,&apos;f&apos;),
 
 &apos;The Cargo Interline Agreement&apos;,
 IF((OR(ISNULL(Airline_1__r.MITA_IATA_Interline_Cargo__c),ISNULL(Airline_2__r.MITA_IATA_Interline_Cargo__c))),&apos;t&apos;,&apos;f&apos;),
 
 &apos;The One Way Issuing Airline&apos;,
 IF((OR(ISNULL(Airline_1__r.MITA_One_way_Pass_Issuing_Airline__c),ISNULL(Airline_2__r.MITA_One_way_Pass_Participating__c))),&apos;t&apos;,&apos;f&apos;),
 
 &apos;The One Way Participating Airline&apos;,
 IF((OR(ISNULL(Airline_1__r.MITA_One_way_Pass_Participating__c),ISNULL(Airline_2__r.MITA_One_way_Pass_Issuing_Airline__c))),&apos;t&apos;,&apos;f&apos;),
 
 &apos;f&apos;
)=&apos;t&apos;,true,false)
)</errorConditionFormula>
        <errorMessage>This agreement type is not enabled for the selected airlines</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>N4_BIETA_Seniority</fullName>
        <active>true</active>
        <description>Prevents &quot;junior&quot; BIETA members from requesting agreements to senior airlines.
The seniorship is calculate on a different date for each agreement type.</description>
        <errorConditionFormula>AND(ISPICKVAL(Cancellation_Reason__c, &quot;&quot;),IF(CASE(TEXT(Agreement_type__c), 

&apos;Bilateral, both carriers can issue electronic tickets&apos;, 
IF(Airline_1__r.BIETA_Bilateral_Date__c &gt; Airline_2__r.BIETA_Bilateral_Date__c,&apos;t&apos;,&apos;f&apos;), 

&apos;One Way Passenger Issuing Airline&apos;, &apos;f&apos;,

&apos;One Way Passenger Partcipating&apos;, &apos;f&apos;,

&apos;One Way Intermodal Issuing Date&apos;, &apos;f&apos;,

&apos;One Way Intermodal Participating Date&apos;, &apos;f&apos;,

&apos;f&apos; )=&apos;t&apos;,true,false))</errorConditionFormula>
        <errorMessage>The selected airline has more seniority than the actual one</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>N4_MITA_Seniority</fullName>
        <active>true</active>
        <description>Prevents &quot;junior&quot; MITA members from requesting agreements to senior airlines.
The seniorship is calculate on a different date for each agreement type.</description>
        <errorConditionFormula>AND(
 Active__c,ISPICKVAL(Cancellation_Reason__c, &quot;&quot;),
IF(CASE(TEXT(Agreement_type__c), 
&apos;The Passenger and Cargo Interline Traffic Agreement&apos;, 
IF(AND(Airline_1__r.MITA_IATA_Interline_Passenger__c &gt; Airline_2__r.MITA_IATA_Interline_Passenger__c, Airline_1__r.MITA_IATA_Interline_Cargo__c &gt; Airline_2__r.MITA_IATA_Interline_Cargo__c),&apos;t&apos;,&apos;f&apos;), 


&apos;The Passenger Interline Agreement&apos;, &apos;f&apos;,

&apos;The Cargo Interline Agreement&apos;, &apos;f&apos;,

&apos;The One Way Issuing Airline&apos;, &apos;f&apos;,

&apos;The One Way Participating Airline&apos;, &apos;f&apos;,

&apos;f&apos; 
)=&apos;t&apos;,true,false)
)</errorConditionFormula>
        <errorMessage>The selected airline has more seniority than the actual one</errorMessage>
    </validationRules>
</CustomObject>
