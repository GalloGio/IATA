    <fields>
        <fullName>Status__c</fullName>
        <description>Describes the situtation of the agreement</description>
        <externalId>false</externalId>
        <formula>IF( ISNULL(Cancellation_processed_date__c),
  IF ( Effective_date__c &lt;= TODAY(), &apos;Active&apos;, &apos;Not Active Yet&apos;),
  IF ( Cancellation_date__c &lt;= TODAY(), &apos;Inactive&apos;, &apos;Pending To Deactivate&apos;)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Possible values are:
 - Not Active Yet,
 - Active
 - Pending To Deactivate
 - Inactive</inlineHelpText>
        <label>Status</label>
