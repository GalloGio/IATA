 OR(
  ISBLANK(Contact__r.FirstName),
  ISBLANK(Contact__r.LastName),
  ISBLANK(Contact__r.Title),
  ISBLANK(Contact__r.Email),
  ISBLANK(Contact__r.AccountId),
  ISBLANK(Contact__r.Account.IATA_ISO_Country__c)
