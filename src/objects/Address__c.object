    <validationRules>
        <fullName>AirportOneRoleShipTo</fullName>
        <active>true</active>
        <errorConditionFormula>and( Count_Roles_Shipping__c &gt;1,
 Account__r.RecordTypeDeveloperName__c =&quot;Airport&quot;)</errorConditionFormula>
        <errorMessage>airport can have only one shipping role per address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BranchAccount_cant_create_AnotherAddress</fullName>
        <active>true</active>
        <description>Branch account can not create another address than the main address.</description>
        <errorConditionFormula>AND( 
     ISPICKVAL( Account__r.Hierarchy_Level__c ,&apos;Branch&apos;),
     (Is_Business_Address__c =False)
)</errorConditionFormula>
        <errorMessage>For Branch account you can&apos;t create another address than the main address (Business address).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BusinessAddressModification</fullName>
        <active>true</active>
        <description>The Main Address must be edit only on Account</description>
        <errorConditionFormula>AND(
    ( TechUpdate_AddressFlow__c =FALSE),
    (Is_Business_Address__c =TRUE),
    or(ISCHANGED( Street__c),
       ISCHANGED( Postal_Code__c ),
       ISCHANGED( City_Name__c ),
       ISCHANGED( Country__c ),
       ISCHANGED( State_Name__c ),
       ISCHANGED( Geo_Coordinates__Latitude__s ),
       ISCHANGED( Geo_Coordinates__Longitude__s )
    )
)</errorConditionFormula>
        <errorMessage>Modification of business account fields needs to be done on the related account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OneRoleBilling</fullName>
        <active>true</active>
        <errorConditionFormula>Count_Roles_Billing__c &gt;1</errorConditionFormula>
        <errorMessage>Only one Billing role per address is allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OneRoleBusiness</fullName>
        <active>true</active>
        <errorConditionFormula>Count_Roles_Business__c &gt;1</errorConditionFormula>
        <errorMessage>Only one Business role per address is allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OneRoleShipping</fullName>
        <active>true</active>
        <errorConditionFormula>Count_Roles_Shipping__c &gt;1</errorConditionFormula>
        <errorMessage>Only one Shipping role per address is allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Street_POnumber</fullName>
        <active>true</active>
        <description>You can not fill Street and PO Number fields at the same time</description>
        <errorConditionFormula>AND(
 Is_Business_Address__c =false,
NOT(ISBLANK(Street__c)),
NOT(ISBLANK(Po_Box__c))
)</errorConditionFormula>
        <errorMessage>You can not fill Street and PO Number fields at the same time</errorMessage>
    </validationRules>
