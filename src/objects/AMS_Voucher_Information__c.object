    <validationRules>
        <fullName>AccountMustBeHO</fullName>
        <active>true</active>
        <description>Only head offices can have a record for Voucher information.</description>
        <errorConditionFormula>NOT(ISPICKVAL(Account__r.Location_Type__c ,&quot;HO&quot;))</errorConditionFormula>
        <errorMessage>Only head offices can have a record for Voucher information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_year_correct_input_value</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(
NOT(ISBLANK ( Year__c ) )
&amp;&amp; 
ISNUMBER ( Year__c )
&amp;&amp;
LEN ( Year__c ) =  4
&amp;&amp;
 ( BEGINS(Year__c , &apos;19&apos;) || BEGINS(Year__c , &apos;20&apos;))
)</errorConditionFormula>
        <errorDisplayField>Year__c</errorDisplayField>
        <errorMessage>Please check the Year format YYYY (&gt; 1900)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Voucher_Information_Only_for_Cargo</fullName>
        <active>true</active>
        <description>Voucher_Information_Only_for_Cargo</description>
        <errorConditionFormula>NOT(ISPICKVAL(Account__r.Location_Class__c,&quot;C&quot;))</errorConditionFormula>
        <errorMessage>Voucher Information is only applied to Cargo Agencies</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
