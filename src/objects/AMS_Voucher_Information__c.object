    <fields>
        <fullName>IsAgencyEligibleForCountryAverage__c</fullName>
        <externalId>false</externalId>
        <formula>YEAR(Account__r.Accreditation_date__c ) &lt; (YEAR(TODAY()) - 1) 
&amp;&amp; 
IsAgencyEligible__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is agency eligible for country average</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>IsAgencyEligible__c</fullName>
        <externalId>false</externalId>
        <formula>YEAR(Account__r.Accreditation_date__c ) &lt;= (YEAR(TODAY()) -1) 
&amp;&amp; 
NOT(ISPICKVAL(Account__r.Status__c ,&quot;Terminated&quot;)) 
&amp;&amp; 
NOT(ISBLANK(AmountLastYearSales__c) || AmountLastYearSales__c == 0) 
&amp;&amp; 
(ISPICKVAL(Account__r.Location_Class__c ,&quot;C&quot;)) 
&amp;&amp; 
NOT(ISBLANK(Account__r.IATA_ISO_Country__r.AMS_Max_Vouchers_Per_Year__c)) 
&amp;&amp; 
NOT(ISBLANK(Account__r.IATA_ISO_Country__r.AMS_Vouchers_Benchmark__c)) 
&amp;&amp; 
ISPICKVAL(Account__r.Location_Type__c ,&quot;HO&quot;)</formula>
        <inlineHelpText>An non eligible Voucher Information:

Sales of that year not found
Agency approval date is after that year
Default status 5, any reason or Default status 6, reasons 61, 62, 68
Submitted international sales of that year is zero or nil</inlineHelpText>
        <label>Is agency eligible to print vouchers</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
