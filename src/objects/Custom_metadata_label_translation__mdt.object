<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Custom metadata label related translations. A record should be inserted per the desired language.</description>
    <fields>
        <fullName>Custom_metadata_label__c</fullName>
        <description>Custom metadata label relation. Maintains the custom label structure. Reunifies all the possible translations for a custom metadata label.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Select the parent custom metadata label. (Name of translation label)</inlineHelpText>
        <label>Custom metadata label</label>
        <referenceTo>Custom_metadata_label__mdt</referenceTo>
        <relationshipLabel>Custom metadata label translations</relationshipLabel>
        <relationshipName>Custom_metadata_label_translations</relationshipName>
        <required>true</required>
        <type>MetadataRelationship</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Language__c</fullName>
        <description>List of available languages for MFA.  (top right corner on login flow)</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>List of available languages for MFA</inlineHelpText>
        <label>Language</label>
        <required>true</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>true</sorted>
                <value>
                    <fullName>ar</fullName>
                    <default>false</default>
                    <label>Arabic</label>
                </value>
                <value>
                    <fullName>zh_CN</fullName>
                    <default>false</default>
                    <label>Chinese</label>
                </value>
                <value>
                    <fullName>en_US</fullName>
                    <default>true</default>
                    <label>English</label>
                </value>
                <value>
                    <fullName>fr</fullName>
                    <default>false</default>
                    <label>French</label>
                </value>
                <value>
                    <fullName>de</fullName>
                    <default>false</default>
                    <label>German</label>
                </value>
                <value>
                    <fullName>in</fullName>
                    <default>false</default>
                    <label>Indonesian</label>
                </value>
                <value>
                    <fullName>it</fullName>
                    <default>false</default>
                    <label>Italian</label>
                </value>
                <value>
                    <fullName>ja</fullName>
                    <default>false</default>
                    <label>Japanese</label>
                </value>
                <value>
                    <fullName>ko</fullName>
                    <default>false</default>
                    <label>Korean</label>
                </value>
                <value>
                    <fullName>pt_BR</fullName>
                    <default>false</default>
                    <label>Portuguese</label>
                </value>
                <value>
                    <fullName>ru</fullName>
                    <default>false</default>
                    <label>Russian</label>
                </value>
                <value>
                    <fullName>es</fullName>
                    <default>false</default>
                    <label>Spanish</label>
                </value>
                <value>
                    <fullName>th</fullName>
                    <default>false</default>
                    <label>Thai</label>
                </value>
                <value>
                    <fullName>tr</fullName>
                    <default>false</default>
                    <label>Turkish</label>
                </value>
                <value>
                    <fullName>vi</fullName>
                    <default>false</default>
                    <label>Vietnamese</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Value__c</fullName>
        <description>Selected language custom metadata label translation.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Selected language custom metadata label translation.</inlineHelpText>
        <label>Value</label>
        <length>500</length>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <label>Custom metadata label translation</label>
    <listViews>
        <fullName>Arabic</fullName>
        <columns>MasterLabel</columns>
        <columns>Custom_metadata_label__c</columns>
        <columns>Language__c</columns>
        <columns>Value__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Language__c</field>
            <operation>equals</operation>
            <value>ar</value>
        </filters>
        <label>Arabic</label>
