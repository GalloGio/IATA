<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Used in ISSP Operational calendar page.
This CS defines a disclaimer to be shown on the calendar page, based on Country and Operation Type</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Country__c</fullName>
        <description>Country ISO code</description>
        <externalId>false</externalId>
        <label>Country</label>
        <length>2</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Label__c</fullName>
        <description>Label name which contains the disclaimer</description>
        <externalId>false</externalId>
        <label>Label</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Operation_Type__c</fullName>
        <description>Type of operation of an operational calendar record</description>
        <externalId>false</externalId>
        <label>Operation Type</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>ISSP Operational Calendar Disclaimer</label>
    <visibility>Public</visibility>
</CustomObject>
