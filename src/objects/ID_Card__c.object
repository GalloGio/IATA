    <webLinks>
        <fullName>Reissue_IDCard</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reissue ID Card</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 


//check if related contact has open/pending cases

	var myquery = &quot;SELECT Id,CaseNumber FROM Case WHERE RecordType.Name = &apos;ID Card Application&apos;  AND Contact.Id= &apos;{!ID_Card__c.Related_ContactId__c}&apos; AND Status != &apos;Closed&apos; limit 1&quot;;
	result = sforce.connection.query(myquery);
	records = result.getArray(&quot;records&quot;);
	if(records[0])
	{	
		alert(&apos;There is already an open case for this Agent: &apos;+records[0].CaseNumber+&apos;. You can not create duplicates.&apos;);
	}
	
	else{
		if({!ID_Card__c.Is_Active_Card__c}==false){
			alert(&apos;You can not reissue an expired card.&apos;);
		}
		else
		{
			try{ 

			var cardType = &quot;Reissue&quot;; 
			var idcardID = &quot;{!ID_Card__c.Id}&quot;; 

			var st = sforce.apex.execute(&quot;IDCardUtil&quot;,&quot;CreateIDCardApplicationAndCase&quot;,{typeofApplication:cardType,IDCardID:idcardID}); 

			var st = String(st);
			if (st.indexOf(&apos;Error&apos;) != -1) 
			{ 
			alert(&apos; An Error occurred &apos;+st); 
			} 
			else 
			{ 
			var baseurl = window.location.protocol+&quot;//&quot;+window.location.host; 
			window.location.href = baseurl+&quot;/&quot;+st; 
			} 

			} 
			catch(err) 
			{ 
			alert(err); 
			}
		}
	}</url>
    </webLinks>
