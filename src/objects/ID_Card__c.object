        <externalId>false</externalId>
        <formula>IF (
       AND(Valid_To_Date__c &gt; TODAY(),
           NOT(OR (
                   ISPICKVAL(Card_Status__c, &apos;Cancelled ID Card&apos;),
                   ISPICKVAL(Card_Status__c, &apos;Cancelled&apos;)
                  )
              )
         )
, True, False)</formula>
