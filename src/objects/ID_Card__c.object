    <fields>
        <fullName>Is_Active_Card__c</fullName>
        <description>Check if the card is active</description>
        <externalId>false</externalId>
        <formula>IF
(
RecordType.DeveloperName = &apos;AIMS&apos; ,
AND
(
Valid_From_Date__c &lt;= TODAY(),
Valid_To_Date__c &gt; TODAY(),
ISNULL(Cancellation_Date__c)
)
,
IF
(
ISPICKVAL(Card_Status__c, &apos;Cancelled ID Card&apos;),
FALSE,
(
IF(TODAY()&lt;= Valid_To_Date__c, TRUE,FALSE)
)
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Active Card</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
