    <webLinks>
        <fullName>Cancel_Card</fullName>
        <availability>online</availability>
        <description>Cancel ID CArd</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel Card</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 

try{ 


var cardStatus = &apos;{!ID_Card__c.Card_Status__c}&apos;; 
var sentToAIMSon = &apos;{!ID_Card__c.Sent_to_AIMS_on__c}&apos;;


if (sentToAIMSon != null &amp;&amp; cardStatus == &apos;Printed/Delivered&apos;) 
{ 
     var cardToUpdate= [];
     var card = new sforce.SObject(&quot;ID_Card__c&quot;); 
     card.id =&quot;{!ID_Card__c.Id}&quot;;
     card.Card_Status__c = &apos;Cancelled&apos;;
     card.Cancellation_Date__c = new Date();
     card.MustSyncWithAIMS__c = &apos;True&apos;;
     cardToUpdate.push(card);
     result = sforce.connection.update(cardToUpdate); 

     alert(&apos;Card has been Cancelled Successfully&apos;);
     window.location.reload();
} 

else if (sentToAIMSon == &apos;&apos; &amp;&amp; cardStatus == &apos;Approved&apos;) 
{ 
     var cardToUpdate= [];
     var card = new sforce.SObject(&quot;ID_Card__c&quot;); 
     card.id =&quot;{!ID_Card__c.Id}&quot;;
     card.Card_Status__c = &apos;Cancelled&apos;;
     card.Cancellation_Date__c = new Date();
     card.MustSyncWithAIMS__c = &apos;False&apos;;
     cardToUpdate.push(card);
     result = sforce.connection.update(cardToUpdate); 

     alert(&apos;Card has been Cancelled Successfully&apos;);
     window.location.reload();
} 
else
{
   alert(&apos;The cancellation of the card cannot be done since we are waiting for AIMS reply related to IDCard printing&apos;);
}

} 
catch(err) 
{ 
alert(err); 
}</url>
    </webLinks>
    <webLinks>
        <fullName>Reissue_IDCard</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reissue IDCard</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 


//check if related contact has open/pending cases

	var myquery = &quot;SELECT Id,CaseNumber FROM Case WHERE RecordType.Name = &apos;ID Card Application&apos;  AND Contact.Id= &apos;{!ID_Card__c.Related_ContactId__c}&apos; AND Status != &apos;Closed&apos; limit 1&quot;;
	result = sforce.connection.query(myquery);
	records = result.getArray(&quot;records&quot;);
	if(records[0])
	{	
		alert(&apos;There is already an open case for this Agent: &apos;+records[0].CaseNumber+&apos;. You can not create duplicates.&apos;);
	}
	
	else{
		if({!ID_Card__c.Is_Active_Card__c}==false){
			alert(&apos;You can not reissue an expired card.&apos;);
		}
		else
		{
			try{ 

			var cardType = &quot;Reissue&quot;; 
			var idcardID = &quot;{!ID_Card__c.Id}&quot;; 

			var st = sforce.apex.execute(&quot;IDCardUtil&quot;,&quot;CreateIDCardApplicationAndCase&quot;,{typeofApplication:cardType,IDCardID:idcardID}); 

			var st = String(st);
			if (st.indexOf(&apos;Error&apos;) != -1) 
			{ 
			alert(&apos; An Error occurred &apos;+st); 
			} 
			else 
			{ 
			var baseurl = window.location.protocol+&quot;//&quot;+window.location.host; 
			window.location.href = baseurl+&quot;/&quot;+st; 
			} 

			} 
			catch(err) 
			{ 
			alert(err); 
			}
		}
	}</url>
    </webLinks>
