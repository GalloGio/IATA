    <webLinks>
        <fullName>Cancel_Card</fullName>
        <availability>online</availability>
        <description>Cancel ID CArd</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel ID Card</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 

try{ 


var cardStatus = &apos;{!ID_Card__c.Card_Status__c}&apos;; 
var sentToAIMSon = &apos;{!ID_Card__c.Sent_to_AIMS_on__c}&apos;;


if (sentToAIMSon != null &amp;&amp; cardStatus == &apos;Valid ID Card&apos;) 
{ 
     var cardToUpdate= [];
     var card = new sforce.SObject(&quot;ID_Card__c&quot;); 
     card.id =&quot;{!ID_Card__c.Id}&quot;;
     card.Card_Status__c = &apos;Cancelled ID Card&apos;;
     card.Cancellation_Date__c = new Date();
     card.MustSyncWithAIMS__c = &apos;False&apos;;
     cardToUpdate.push(card);
     result = sforce.connection.update(cardToUpdate); 

     alert(&apos;Card has been Cancelled Successfully&apos;);
     window.location.reload();
} 

else if (sentToAIMSon == &apos;&apos; &amp;&amp; cardStatus == &apos;Sent ID Card&apos;) 
{ 
     var cardToUpdate= [];
     var card = new sforce.SObject(&quot;ID_Card__c&quot;); 
     card.id =&quot;{!ID_Card__c.Id}&quot;;
     card.Card_Status__c = &apos;Cancelled ID Card&apos;;
     card.Cancellation_Date__c = new Date();
     card.MustSyncWithAIMS__c = &apos;False&apos;;
     cardToUpdate.push(card);
     result = sforce.connection.update(cardToUpdate); 

     alert(&apos;Card has been Cancelled Successfully&apos;);
     window.location.reload();
} 
else
{
   alert(&apos;The cancellation of the card cannot be done since we are waiting for AIMS reply related to IDCard printing&apos;);
}

} 
catch(err) 
{ 
alert(err); 
}</url>
