        <fullName>Icon__c</fullName>
        <description>Define the icon of the recommendation available on the Highlight panel</description>
        <externalId>false</externalId>
        <formula>IF ( NOT(ISBLANK( Icon_URL__c )), IMAGE( Icon_URL__c , Name, 128 , 128 ), 
			IF(RecordType.DeveloperName = &#39;ProductRecommendation&#39;,  
						IMAGE( &quot;/resource/CSPortal/Images/ProductHighlight.JPG&quot;, Name, 130 , 350 ) , 
						IMAGE( &quot;/resource/CSPortal/Images/ServiceHighlight.JPG&quot; , Name, 130 , 350 )) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Icon</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
