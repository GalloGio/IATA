        <availability>online</availability>
        <description>Set the OI in cocluded status, but before ask the user the period between 3 and 12 months</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Conclude</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/37.0/connection.js&apos;)}
{!REQUIRESCRIPT(&apos;/soap/ajax/37.0/apex.js&apos;)}
{!REQUIRESCRIPT(&apos;//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js&apos;)}
{!REQUIRESCRIPT(&apos;//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js&apos;)}


function getOIObject() {
  try {
    var query = &quot;select Id, Conclusion_Date__c, Pending_eff_validation_date__c from Operational_Improvements__c where Id = &apos;{!Operational_Improvements__c.Id}&apos; limit 1&quot;;
    var result = sforce.connection.query(query);
    var records = result.getArray(&quot;records&quot;);
    return records[0];
  } catch (e) {
    alert(&apos;Retrieve Error: &apos; + e);
  }
}

function getToday(){
  return new Date(&quot;{!MONTH(TODAY())}/{!DAY(TODAY())}/{!YEAR(TODAY())}&quot;);
}

function updateOIObject(object, months) {
  try {
    var todayDate = getToday();
    var nextDate = getToday();
    nextDate.setMonth(todayDate.getMonth() + months);
    object.Conclusion_Date__c = todayDate ;
    object.Pending_eff_validation_date__c = nextDate;
    return sforce.connection.update([object])[0];
  } catch (e) {
    alert(&apos;Update Error: &apos; + e);
  }
}

function processConclusion(dialog, OIobject, months) {
  var result = updateOIObject(OIobject, months);
  jQuery(dialog).dialog( &quot;close&quot; );
  if(result.getBoolean(&apos;success&apos;)) {
    location.reload();
  } else {
    alert(result.errors.message);
  }  
}

try{

  var OIobject = getOIObject();

  jQuery(function() {
    /*Append the jQuery CSS CDN Link to the Head tag.*/
    jQuery(&apos;head&apos;).append(&apos;&lt;link rel=&quot;stylesheet&quot; href=&quot;//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/themes/start/jquery-ui.css&quot; type=&quot;text/css&quot; /&gt;&apos;);
   
    /*Create the HTML(DIV Tag) for the Dialog.*/
    var html =
      &apos;&lt;div id=&quot;dialog&quot; title=&quot;Conclude Operational Improvement&quot;&gt;&lt;p&gt;Choose months to conclude?&lt;/p&gt;&lt;/div&gt;&apos;;
   
    /*Check if the Dialog(DIV Tag) already exists if not then Append the same to the Body tag.*/
    if(!jQuery(&apos;[id=dialog]&apos;).size()){
      jQuery(&apos;body&apos;).append(html);
    }    

    /*Open the jQuery Dialog.*/
    jQuery( &quot;#dialog&quot; ).dialog({
      modal: true,
      draggable: false,
      resizable: false,
      position: [&apos;center&apos;, &apos;center&apos;],
      width: 400,
      buttons: {
        &quot;3 Months&quot;: function() {
          processConclusion(this,OIobject,3);
        },
        &quot;6 Months&quot;: function() {
          processConclusion(this,OIobject,6);
        },
        &quot;9 Months&quot;: function() {
          processConclusion(this,OIobject,9);
        },
        &quot;12 Months&quot;: function() {
          processConclusion(this,OIobject,12);
        },
        &quot;Cancel&quot;: function() {
          jQuery( this ).dialog( &quot;close&quot; );
        }
      }
    });
  });
}
catch(e){
  alert(&apos;An Error has Occurred. Error: &apos; + e);
}</url>
