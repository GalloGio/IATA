function htmlDecode(input){
  var e = document.createElement(&apos;div&apos;);
  e.innerHTML = input;
  return e.childNodes.length === 0 ? &quot;&quot; : e.childNodes[0].nodeValue;
}

function getNow(){
  var time;
  try {
    time = &quot;{!NOW()}&quot;.split(&quot; &quot;)[1];
  } catch (e) {
    time = &quot;00:00&quot;;
  }
  return new Date(&quot;{!MONTH(TODAY())}/{!DAY(TODAY())}/{!YEAR(TODAY())} &quot; + time);
}

if ({!ISNULL(Operational_Improvements__c.Date_Time_Closed__c)}) {
  var improvement = new sforce.SObject(&quot;Operational_Improvements__c&quot;);
  improvement.Id = &apos;{!Operational_Improvements__c.Id}&apos;;
  improvement.Date_Time_Closed__c = getNow();
  var result = sforce.connection.update([improvement ]);

  if(result[0].getBoolean(&apos;success&apos;)) {
    location.reload();
  } else {
    alert(htmlDecode(result[0].errors.message));
  }

} else {
  alert(&quot;OI already closed&quot;);
}</url>
