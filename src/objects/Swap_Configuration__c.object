    <fields>
        <fullName>Source_Product_Rate_Plan_Information__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <inlineHelpText>Source rate plan before swapping</inlineHelpText>
        <label>Source Product Rate Plan Information</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>1 OR (2 AND 3)</booleanFilter>
            <filterItems>
                <field>$Source.Relationship_Type__c</field>
                <operation>equals</operation>
                <value>Swap</value>
            </filterItems>
            <filterItems>
                <field>$Source.Relationship_Type__c</field>
                <operation>equals</operation>
                <value>Upgrade</value>
            </filterItems>
            <filterItems>
                <field>Product_Rate_Plan_Information__c.Related_Product_Information__c</field>
                <operation>notEqual</operation>
                <value></value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Product_Rate_Plan_Information__c</referenceTo>
        <relationshipLabel>Swapping Configuration (Source Rate Plan)</relationshipLabel>
        <relationshipName>Swapping_Configuration</relationshipName>
