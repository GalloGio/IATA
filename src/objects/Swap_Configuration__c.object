    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>IECSwappingTargetRatePlanValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(Target_Product_Rate_Plan_Information__r.Related_Product_Information__r.Related_Product_Category__c &lt;&gt;  Source_Product_Rate_Plan_Information__r.Related_Product_Information__r.Related_Product_Category__c,  ISPICKVAL(Target_Product_Rate_Plan_Information__r.Related_Product_Information__r.Product_Audience__c, &apos;Default&apos;))
,  ISPICKVAL(Relationship_Type__c, &apos;Swap&apos;)
)</errorConditionFormula>
        <errorDisplayField>Target_Product_Rate_Plan_Information__c</errorDisplayField>
        <errorMessage>For Swapping: Target Rate Plan must be associated to the Same Product Category of the Source Product Rate Plan and must not be part of any Default Product Audience Rate Plans</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Upgrade_plan_must_be_to_Unlimited</fullName>
        <active>true</active>
        <description>Only upgrade to an Unlimited rate plan is supported.</description>
        <errorConditionFormula>AND (
	ISPICKVAL(Relationship_Type__c, &apos;Upgrade&apos;)
	, OR(
		Target_Product_Rate_Plan_Information__r.Subscription_Usage_Qty__c &gt; 0
		, Target_Product_Rate_Plan_Information__r.Is_Triable__c
	)
)</errorConditionFormula>
        <errorDisplayField>Target_Product_Rate_Plan_Information__c</errorDisplayField>
        <errorMessage>Target upgrade rate plan must only be unlimited.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
