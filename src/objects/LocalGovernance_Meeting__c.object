    <validationRules>
        <fullName>At_least_one_checkbox</fullName>
        <active>true</active>
        <description>One of the items reviewed in the meeting has to be checked to be able to proceed</description>
        <errorConditionFormula>AND(
 Remittance_Frequency_Reviewed__c = FALSE,
 VMFR__c = FALSE,
 Criteria_Reviewed__c = FALSE,
 Other__c = FALSE,
 NOT(ISBLANK( Date_Legal_Requested_to_Sign_Off_Minutes__c ))
)</errorConditionFormula>
        <errorMessage>Please specify the items that were discussed in the meeting (Criteria Reviewed, Remittance Frequency Reviewed, VMFR) or fill &quot;Other&quot; with any other item discussed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_change_if_inactive_group</fullName>
        <active>true</active>
        <description>It&apos; s not possible to insert a new meeting or change the existing one if the participant is related to an inactive local group.</description>
        <errorConditionFormula>AND(
Local_Governance__r.Active__c=false,
OR(
ISNEW(),
ISCHANGED(Name),
ISCHANGED(Local_Governance__c),
ISCHANGED(Comments__c),
ISCHANGED(Criteria_Reviewed__c),
ISCHANGED(Date_Legal_REJECTED_the_minutes__c),
ISCHANGED(Date_Legal_REJECTED_the_agenda__c),
ISCHANGED(Date_Legal_Requested_to_Sign_off_Agenda__c),
ISCHANGED(Date_Legal_Requested_to_Sign_Off_Minutes__c),
ISCHANGED(Date_Legal_Signed_off_Agenda__c),
ISCHANGED(Date_Legal_Signed_Off_the_Minutes__c),
ISCHANGED(Date_of_Meeting__c),
ISCHANGED(Meeting_Responsible__c),
ISCHANGED(Remittance_Frequency_Reviewed__c),
ISCHANGED(VMFR__c)
)
)</errorConditionFormula>
        <errorMessage>It&apos; s not possible to insert a new meeting or change the existing one if the participant is related to an inactive local group.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_required_if_Other_is_true</fullName>
        <active>true</active>
        <description>Comments required to specify what &quot;Other&quot; items were discussed in the meeting</description>
        <errorConditionFormula>AND(
 Other__c = TRUE,
 ISBLANK(  Comments__c  )
)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please fill Comments checklist with the additional items discussed in the meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Create_Edit_Group_Meetings</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Local_Governance__r.RecordType.DeveloperName  = &apos;Local_Groups&apos;,
NOT($Permission.Modify_Local_Group_Owners)
)</errorConditionFormula>
        <errorMessage>You are not allowed to add or edit meetings to this group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Legal_Signed_off_Agenda</fullName>
        <active>true</active>
        <description>The date that legal signed off the agenda can&apos;t be before date legal requested to sign off the agenda</description>
        <errorConditionFormula>AND(
 NOT(ISBLANK(Date_Legal_Requested_to_Sign_off_Agenda__c)),
 NOT(ISBLANK(Date_Legal_Signed_off_Agenda__c)),
 Date_Legal_Requested_to_Sign_off_Agenda__c &gt;Date_Legal_Signed_off_Agenda__c
)</errorConditionFormula>
        <errorMessage>The date when Legal Signed off the Agenda can&apos;t be before Legal Requested to Sign Off the agenda</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Legal_Signed_off_minutes</fullName>
        <active>true</active>
        <description>The date that legal signed off the minutes can&apos;t be before legal requested the sign to sign off the minutes</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Date_Legal_Requested_to_Sign_Off_Minutes__c )),
NOT(ISBLANK(Date_Legal_Signed_Off_the_Minutes__c )),
Date_Legal_Requested_to_Sign_Off_Minutes__c &gt;Date_Legal_Signed_Off_the_Minutes__c 
)</errorConditionFormula>
        <errorMessage>The date when Legal Signed off the Minutes can&apos;t be before Legal Requested to Sign Off the Minutes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_meeting_cant_be_in_the_past</fullName>
        <active>true</active>
        <description>Date of the meeting can&apos;t be a passed date</description>
        <errorConditionFormula>AND(
ISNEW(),
Date_of_Meeting__c &lt; TODAY() ,
 CreatedDate &gt;   DATETIMEVALUE( DATE(2020,01,01) )
)</errorConditionFormula>
        <errorDisplayField>Date_of_Meeting__c</errorDisplayField>
        <errorMessage>Date of the meeting can&apos;t be in the past</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Legal_Remarks_Agenda_mandatory</fullName>
        <active>true</active>
        <description>Legal Remarks Agenda are mandatory if Reason for rejecting agenda = Other</description>
        <errorConditionFormula>AND(
ISPICKVAL(Reason_for_Rejection_Agenda__c , &quot;Other (See comments)&quot;),
ISBLANK( Legal_Remarks_Agenda__c )
)</errorConditionFormula>
        <errorDisplayField>Legal_Remarks_Agenda__c</errorDisplayField>
        <errorMessage>Please fill the Legal Remarks Agenda with the reason for rejecting it</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Legal_remarks_minutes_mandatory</fullName>
        <active>true</active>
        <description>Legal remarks minutes is mandatory if Reason for rejecting the minutes contains Other</description>
        <errorConditionFormula>AND(
ISPICKVAL( Reason_for_Rejection_Minutes__c , &quot;Other&quot;),
ISBLANK( Legal_Remarks_Minutes__c )
)</errorConditionFormula>
        <errorDisplayField>Legal_Remarks_Minutes__c</errorDisplayField>
        <errorMessage>Please fill Legal remarks minutes explaining the Other reasons to reject the minutes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_rejection_minutes_mandatory</fullName>
        <active>true</active>
        <description>Reason for rejecting the minutes is mandatory if the minutes are rejected</description>
        <errorConditionFormula>AND(
 ISBLANK(TEXT(Reason_for_Rejection_Minutes__c )),
 NOT(ISBLANK(Date_Legal_REJECTED_the_minutes__c ))
)</errorConditionFormula>
        <errorDisplayField>Reason_for_Rejection_Minutes__c</errorDisplayField>
        <errorMessage>Please give a reason for rejecting the minutes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_rejection_of_agenda_mandatory</fullName>
        <active>true</active>
        <description>Reason for rejection of agenda needs to be filled if Date Legal Rejected the Agenda isn&apos;t empty</description>
        <errorConditionFormula>AND(
NOT(ISBLANK( Date_Legal_REJECTED_the_agenda__c )),
ISBLANK(TEXT( Reason_for_Rejection_Agenda__c ))
)</errorConditionFormula>
        <errorDisplayField>Reason_for_Rejection_Agenda__c</errorDisplayField>
        <errorMessage>Please give a reason for rejecting the Agenda</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
