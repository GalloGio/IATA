    <fields>
        <fullName>Address__c</fullName>
        <description>Formula address, containing the concatenation of Address_1, Address_2, Address_3</description>
        <externalId>false</externalId>
        <formula>SUBSTITUTE( Address_Line_1__c 
+ IF( ISBLANK(Address_Line_2__c), &apos;&apos;, &apos; &apos; + Address_Line_2__c)
+ IF( ISBLANK(Address_Line_3__c), &apos;&apos;, &apos; &apos; + Address_Line_3__c)
, &apos;|&apos;, &apos;/&apos;)</formula>
