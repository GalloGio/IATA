    <fields>
        <fullName>Address_IATA_Custom_ISO_Country_Code__c</fullName>
        <description>Used as LCtryISO, MCtryISO, RCtryISO by the GDP/MDP table. 
For UP (US Proctectorates), use the State Code, otherwise, it is equals to Address_ISO_Country_Code__c</description>
        <externalId>false</externalId>
        <formula>IF( Address_ISO_Country_Code__c = &apos;UP&apos;
   , Address_State_Code__c
   , Address_ISO_Country_Code__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Used as LCtryISO, MCtryISO, RCtryISO by the GDP/MDP table, based on Address_ISO_Country_Code__c or Address_State_Code__c</inlineHelpText>
        <label>Address IATA Custom ISO Country Code</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
