    <validationRules>
        <fullName>AMS_Expiry_Date_required_on_DGR</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type_Of_Certificate__c, &quot;DGR&quot;),  ISBLANK( Expiry_Date__c ))</errorConditionFormula>
        <errorMessage>DGR certificates must have an expiry date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Uncheck_notification_sent</fullName>
        <active>true</active>
        <description>If expiry date is modified, &quot;Notification sent&quot; field needs to be unchecked.</description>
        <errorConditionFormula>AND(ISCHANGED(Expiry_Date__c),Notification_Sent__c=True)</errorConditionFormula>
        <errorDisplayField>Notification_Sent__c</errorDisplayField>
        <errorMessage>Please un-check this field before updating the Expiry date.</errorMessage>
    </validationRules>
