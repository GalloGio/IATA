    <fields>
        <fullName>CNS_Article__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Used to define the articles that had been created for CNS, and be sent to approval to the CNS president</description>
        <externalId>false</externalId>
        <label>CNS Article</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CS_Portal_link__c</fullName>
        <description>Direct link for the article from CS Portal site, only the english version will contain a shareable link</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(PublishStatus,&apos;Online&apos;),
IF( ISPICKVAL(Language,&apos;en_US&apos;),
 IF($Organization.Id ==&apos;00D2000000008TF&apos;,
   IF(IsVisibleInPkb==true,
     &apos;https://portal.iata.org/pkb/s/article/&apos;+UrlName,
     IF(IsVisibleInPrm,&apos;https://portal.iata.org/s/article/&apos;+UrlName,&apos;Article not available in the Portal&apos;)
   ),
   IF(IsVisibleInPkb==true,
    &apos;pkb/s/article/&apos;+UrlName,
    IF(IsVisibleInPrm,&apos;/s/article/&apos;+UrlName,&apos;Article not available in the Portal&apos;)
   )
 ),
 &apos;Check English version for URL&apos;
),&apos;Only published versions have a shareable link&apos;)</formula>
        <label>CS Portal link</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
