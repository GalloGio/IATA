    <webLinks>
        <fullName>Regenerate_Release</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Update Release</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)} 

if ({!IEC_Event_Release__c.Released__c}) { 
	alert(&quot;Product has been already released.&quot;);
} 
else if ({!IEC_Event_Release__c.Processing_Status__c != null || IEC_Event_Release__c.Processing_Status__c != &apos;&apos;}) { 
	alert(&quot;Please wait!  A process is running for release.&quot;);
} 
else 
{ 
	var retVal = confirm(&quot;Warning!! Are you sure you want to regenerate? &quot;); 
	if(retVal == true){ 
		var result = sforce.apex.execute(&quot;IEC_Evt_Renew_SOAP&quot;,&quot;eventRegenerateRelease&quot;,{ eventReleaseId:&quot;{!IEC_Event_Release__c.Id}&quot; }); 
		alert(&apos;The release process has started.&apos;); 
		document.location.reload(true);
	} 
}</url>
    </webLinks>
    <webLinks>
        <fullName>Release</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Release</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}

if ({!IEC_Event_Release__c.Released__c}) {
	alert(&quot;Product has been already released.&quot;);
}
else if ({!IEC_Event_Release__c.Processing_Status__c != null || IEC_Event_Release__c.Processing_Status__c != &apos;&apos;}) { 
	alert(&quot;Please wait!  A process is running for release.&quot;);
} 
else
{
	var retVal = confirm(&quot;Warning!! Are you sure you want to proceed? If you click &apos;Yes&apos; then Orders will generated and Customers will be charged!\n\nClick OK to proceed or Cancel to stop the process&quot;);
	if(retVal == true){
		var result = sforce.apex.execute(&quot;IEC_Evt_Renew_SOAP&quot;,&quot;Event_Release&quot;,{
		    idEventRelease:&quot;{!IEC_Event_Release__c.Id}&quot;
		});		
		alert(&apos;The release process has started.&apos;);
                document.location.reload(true);
	}
}</url>
    </webLinks>
    <webLinks>
        <fullName>Reprocess_errors</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Reprocess errors</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/apex.js&quot;)}

if ({!IEC_Event_Release__c.Released__c != true}) {
    alert(&quot;This function is only for released event.&quot;);
}
else if ({!IEC_Event_Release__c.Processing_Status__c != null || IEC_Event_Release__c.Processing_Status__c != &apos;&apos;}) { 
    alert(&quot;Please wait!  A process is running for release.&quot;);
} 
else
{
    var retVal = confirm(&quot;Warning!! Are you sure you want to reproceed? If you click &apos;Yes&apos; then Orders will generated and Customers will be charged!\n\nClick OK to proceed or Cancel to stop the process&quot;);
    if(retVal == true){
        var result = sforce.apex.execute(&quot;IEC_Evt_Renew_SOAP&quot;,&quot;reprocessError&quot;,{
            idEventRelease:&quot;{!IEC_Event_Release__c.Id}&quot;
        });
        alert(&apos;The release Reprocess has started.&apos;);
		document.location.reload(true);
    }
}</url>
