    <validationRules>
        <fullName>Forbid_Modification_After_Released</fullName>
        <active>true</active>
        <description>Forbid all UI modification after the event is released.</description>
        <errorConditionFormula>AND(NOT(ISNEW())
  , PRIORVALUE(Released__c) = true
  , OR(ISCHANGED(Name), ISCHANGED(Customer_Type__c), ISCHANGED(Product__c), ISCHANGED(Swapped_Product__c), ISCHANGED(Released__c))
)</errorConditionFormula>
        <errorMessage>No modification is allowed after event released.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Forbid_Modification_On_Running</fullName>
        <active>true</active>
        <description>Forbid modification because a process is running.</description>
        <errorConditionFormula>AND(NOT(ISNEW())
  , NOT(ISBLANK(PRIORVALUE(Processing_Status__c)))
  , OR(ISCHANGED(Name), ISCHANGED(Customer_Type__c), ISCHANGED(Product__c), ISCHANGED(Swapped_Product__c))
)</errorConditionFormula>
        <errorMessage>Please wait!  A process is running for release.</errorMessage>
    </validationRules>
