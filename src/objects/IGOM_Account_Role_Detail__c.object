    <fields>
        <fullName>Account_Role__c</fullName>
        <description>Account Role that the IGOM Account Role Detail is related to.</description>
        <externalId>false</externalId>
        <label>Account Role</label>
        <referenceTo>Account_Role__c</referenceTo>
        <relationshipLabel>IGOM Account Role Details</relationshipLabel>
        <relationshipName>IGOM_Account_Role_Details</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>City__c</fullName>
        <description>City the Station belongs to.</description>
        <externalId>false</externalId>
        <formula>Account_Role__r.Account__r.Business_City_Name__c</formula>
        <label>City</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Country__c</fullName>
        <description>Country the Station belongs to.</description>
        <externalId>false</externalId>
        <formula>Account_Role__r.Account__r.IATA_ISO_Country__r.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Country</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Phone_Number__c</fullName>
        <description>Main Phone Number of the Station.</description>
        <externalId>false</externalId>
        <formula>Account_Role__r.Account__r.Phone</formula>
        <label>Phone Number</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
