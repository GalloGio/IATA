        <description>IEC - Determine if invoice is ready to send to SAP</description>
        <externalId>false</externalId>
        <formula>AND(
	Number_of_Zuora_Items__c &lt;&gt; 0
	, Number_of_Error_SAP_Items__c = 0
	, Number_of_SAP_Items__c = Number_of_Zuora_Items__c 
	, ISPICKVAL(Status__c, &apos;New&apos;)
	, ISBLANK(Subscription_Order_Id__c)
	, Number_of_Use_Contract__c &gt; 0
)</formula>
