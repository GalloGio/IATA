        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>ISSP FAQ2Case Emergencies</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Subtopic__c</columns>
