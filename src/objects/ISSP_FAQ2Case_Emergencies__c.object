<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Extra layer to show topic and subtopics in ISSP_FAQ2Case but only used for Emergency cases.
Only those topic and subtopics in this list are visible for emergencies</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Subtopic__c</fullName>
        <description>Valid subtopic name for emergencies</description>
        <externalId>false</externalId>
        <label>Subtopic</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Topic__c</fullName>
        <description>Valid topic name for emergencies</description>
        <externalId>false</externalId>
        <label>Topic</label>
