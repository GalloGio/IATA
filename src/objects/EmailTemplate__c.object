    <validationRules>
        <fullName>FA_Result_empty_if_FA</fullName>
        <active>true</active>
        <description>The field FA Result must be empty if Template type is FA - Financial Assessment</description>
        <errorConditionFormula>ISPICKVAL( Template_Type__c , &apos;FA - Financial Assessment&apos;)  &amp;&amp;  NOT( ISPICKVAL( FA_Result__c , &apos;&apos;) )</errorConditionFormula>
        <errorMessage>The field FA Result must be null when Template Type is &apos;FA - Financial Assessment&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FA_Result_mandatory_if_FS</fullName>
        <active>true</active>
        <description>The field FA Result is mandatory if the Template Type is Financial Security Request</description>
        <errorConditionFormula>ISPICKVAL(Template_Type__c,&apos;FS - Financial Security&apos;)  &amp;&amp; ISPICKVAL(FA_Result__c,&apos;&apos;)</errorConditionFormula>
        <errorMessage>The field FA Result is mandatory when the Template Type is &apos;FS - Financial Security&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ISO_Country_is_mandatory</fullName>
        <active>false</active>
        <errorConditionFormula>ISNULL( IATA_ISO_Country__c)</errorConditionFormula>
        <errorDisplayField>Agent_Type__c</errorDisplayField>
        <errorMessage>Please select IATA ISO Country.</errorMessage>
    </validationRules>
