    <validationRules>
        <fullName>PIR_Airline_Receiving_NOT_blank</fullName>
        <active>true</active>
        <description>Airline receiving 2-letter Designator Code and/or Airline receiving must be populated</description>
        <errorConditionFormula>AND(
    ISBLANK(X2_letter_Airline_Code__c),
    ISBLANK(Airline_receiving_formula__c)
    )</errorConditionFormula>
        <errorDisplayField>X2_letter_Airline_Code__c</errorDisplayField>
        <errorMessage>Airline receiving 2-letter Designator Code and/or Airline receiving must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PIR_Issuing_NOTequal_Receiving</fullName>
        <active>true</active>
        <description>An Airline Issuing should NOT be able to issue a Prorate Request to itself</description>
        <errorConditionFormula>Airline_issuing__c =   Airline_receiving_name__c</errorConditionFormula>
        <errorDisplayField>Airline_receiving_name__c</errorDisplayField>
        <errorMessage>Airline Issuing and Receiving are the same. You cannot issue a Prorate Request to your Airline. Please modify the Airline receiving 2-letter Designator Code.</errorMessage>
    </validationRules>
