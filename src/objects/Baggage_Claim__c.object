    <fields>
        <fullName>Days_to_Go_vs_60_Day_Limit__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
   ISPICKVAL(Status__c, &quot;Proceed to payment&quot;), 
   0, 

IF (
	AND(
	    NOT(ISBLANK(Tech_Last_Comment_Issuing_Date_Time__c)),
	    ISBLANK(Tech_Last_Comment_Receiving_Date_Time__c)
            ),
	60-(NOW()-Tech_Last_Comment_Issuing_Date_Time__c),

	IF (
	    AND(
		NOT(ISBLANK(Tech_Last_Comment_Issuing_Date_Time__c)),
		NOT(ISBLANK(Tech_Last_Comment_Receiving_Date_Time__c))
		),
	     60-(NOW()-Tech_Last_Comment_Receiving_Date_Time__c),
	     60
	     )
    )
	
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Days to Go vs 60-Day-Limit</label>
        <precision>18</precision>
        <required>false</required>
