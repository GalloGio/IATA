        <fullName>Master</fullName>
        <active>true</active>
        <label>Master</label>
        <picklistValues>
            <picklist>Role_Type__c</picklist>
            <values>
                <fullName>Billing</fullName>
                <default>false</default>
            </values>
            <values>
                <fullName>Business</fullName>
                <default>true</default>
            </values>
            <values>
                <fullName>Shipping</fullName>
                <default>false</default>
            </values>
        </picklistValues>
    </recordTypes>
