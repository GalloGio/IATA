    <fields>
        <fullName>Airline_Code__c</fullName>
        <description>Returns the 2-letter and the 3-digit airline identification codes. To be used for the template merge.</description>
        <externalId>false</externalId>
        <formula>Case__r.Account.Airline_designator__c + &apos; &apos; + Case__r.Account.IATACode__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Airline Code</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Airline_Country__c</fullName>
        <description>Return the country of the airline account on the parent case. To be used for the template merge.</description>
        <externalId>false</externalId>
