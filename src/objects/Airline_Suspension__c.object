    <fields>
        <fullName>IATA_member__c</fullName>
        <externalId>false</externalId>
        <formula>IF (
  ISBLANK( SVP_FDS_Approval__c ),
  IF (
    ISPICKVAL( Case__r.Account.Membership_status__c, &apos;IATA member&apos; ),
    &apos;Yes&apos;,
    &apos;No&apos;
  ),
  IATA_member_locked_value__c 
)</formula>
        <inlineHelpText>Before the suspension approval, this represents the current status of the airline&apos;s IATA membership. After the suspension has been approved, this represents the airline&apos;s IATA membership status at the suspension approval time.</inlineHelpText>
        <label>IATA Member</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
