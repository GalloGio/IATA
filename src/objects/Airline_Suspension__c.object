    <validationRules>
        <fullName>No_IATA_Governance_Groups_if_not_member</fullName>
        <active>true</active>
        <description>Block &quot;Participates in IATA Governance Group&quot; once the field IATA Member is &quot;No&quot;.</description>
        <errorConditionFormula>AND (
  NOT(ISPICKVAL ( ICH_participant__c ,&quot;&quot;)),
  IATA_member__c = &apos;No&apos;,
  NOT( INCLUDES( Participates_in_IATA_Governance_Groups__c, &apos;Not Applicable&apos; ))
)</errorConditionFormula>
        <errorMessage>The field Participates in IATA Governance Groups needs to be set to Not Applicable  if the airline is not an IATA Member.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SVP_Agreement_Rejection_Date_Mandatory</fullName>
        <active>true</active>
        <description>Make the SVP Agreement / Rejection Date field mandatory if the field HQ Region SVP Agree is</description>
        <errorConditionFormula>AND (
  ISBLANK( SVP_Agreement_Rejection_Date__c ), 
  NOT( ISBLANK( TEXT( HQ_Reqion_SVP_Agree__c ) ) )
)</errorConditionFormula>
        <errorMessage>Please select the SVP Agreement / Rejection Date.</errorMessage>
    </validationRules>
