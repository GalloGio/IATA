    <validationRules>
        <fullName>DailyAgencyInfo</fullName>
        <active>true</active>
        <description>Validates Daily Agency Information</description>
        <errorConditionFormula>if( and(RecordType.Name == &apos;Daily&apos;,
      Opt_in__c == true,
      Opt_out_Bulletin__c == false
  ),       
    and(
     RecordType.Name == &apos;Daily&apos;,
     Agency_Info_Irregularities__c == false, 
     Agency_Info_Defaults__c == false, 
     Agency_Info_Termination_Closures__c == false,
     Agency_Info_Reviews__c == false, 
    Agency_Info_Reinstatements__c == false 
  ),false
  )</errorConditionFormula>
        <errorMessage>DAILY Bulletin - Please select at least the Agency Information of interest</errorMessage>
    </validationRules>
