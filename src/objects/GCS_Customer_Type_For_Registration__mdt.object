    <validationRules>
        <fullName>Account_Creatable_Restriction</fullName>
        <active>true</active>
        <description>If Auto Search or No Search is selected, Account cannot be created</description>
        <errorConditionFormula>AND(
					OR(
									ISPICKVAL(Search_Option__c, &apos;No Search&apos;),
									ISPICKVAL(Search_Option__c, &apos;Auto Search&apos;)
					),
     Can_Account_Be_Created__c 				
	)</errorConditionFormula>
        <errorDisplayField>Can_Account_Be_Created__c</errorDisplayField>
        <errorMessage>If Auto Search or No Search is selected, Account cannot be created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Auto_Search_Constraints</fullName>
        <active>true</active>
        <description>If Auto Search is selected, the following fields must be  filled:
- Filter on Category
- Filter on Record Type
- Filter on Sector
And the following fields must be empty:
- Fields Displayed
- Fields Targeted</description>
        <errorConditionFormula>AND(
					ISPICKVAL(Search_Option__c, &apos;Auto Search&apos;),
					OR(
									NOT(ISBLANK(Fields_Displayed__c)),
									NOT(ISBLANK(Fields_Targeted_Partial_Match__c)),
									ISBLANK(Filter_on_Category__c),
									ISBLANK(Filter_on_Record_Type__c),
									ISBLANK(Filter_on_Sector__c)
					)
	)</errorConditionFormula>
        <errorMessage>If Auto Search is selected, the following fields must be  filled:
- Filter on Category
- Filter on Record Type
- Filter on Sector
And the following fields must be empty:
- Fields Displayed
- Fields Targeted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Display_Submit_Restrictions</fullName>
        <active>true</active>
        <description>If Display Submit for Account Creation is true, Created Account Sector and Created Account Category must be filled.
Otherwise, they must be empty.</description>
        <errorConditionFormula>OR(
				AND(
								Display_Submit_for_Account_Creation__c,
								OR(
												ISBLANK(Created_Account_Sector__c),
												ISBLANK(Created_Account_Category__c)
								)
				),
    AND(
								NOT(Display_Submit_for_Account_Creation__c),
								NOT(
												AND(
																ISBLANK(Created_Account_Sector__c),
																ISBLANK(Created_Account_Category__c)
												)
								)
				)
	)</errorConditionFormula>
        <errorDisplayField>Display_Submit_for_Account_Creation__c</errorDisplayField>
        <errorMessage>If Display Submit for Account Creation is true, Created Account Sector and Created Account Category must be filled.
Otherwise, they must be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Search_Constraints</fullName>
        <active>true</active>
        <description>If No Search is selected, the following fields must be  empty:
- Fields Displayed
- Fields Targeted
- Filter on Category
- Filter on Record Type
- Filter on Sector</description>
        <errorConditionFormula>AND(
					ISPICKVAL(Search_Option__c, &apos;No Search&apos;),
					OR(
									NOT(ISBLANK(Fields_Displayed__c)),
									NOT(ISBLANK(Fields_Targeted_Partial_Match__c)),
									NOT(ISBLANK(Filter_on_Category__c)),
									NOT(ISBLANK(Filter_on_Record_Type__c)),
									NOT(ISBLANK(Filter_on_Sector__c))
					)
	)</errorConditionFormula>
        <errorMessage>If No Search is selected, the following fields must be  empty:
- Fields Displayed
- Fields Targeted
- Filter on Category
- Filter on Record Type
- Filter on Sector</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Placeholder_Rule</fullName>
        <active>true</active>
        <description>If Sub-Categorization Custom Label is filled, Placeholder Custom Label must filled as well</description>
        <errorConditionFormula>AND(
    NOT(ISBLANK(Sub_Categorization_Custom_Label__c)),
    ISBLANK(Placeholder_Custom_Label__c)
)</errorConditionFormula>
        <errorDisplayField>Placeholder_Custom_Label__c</errorDisplayField>
        <errorMessage>If Sub-Categorization Custom Label is filled, Placeholder Custom Label must filled as well</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
