<aura:component access="global" implements="lightning:availableForFlowScreens">
    <aura:attribute name="label" type="String" access="global" />
    <aura:attribute name="concatedValues" type="String" access="global" />
    <aura:attribute name="options" type="List" access="private" />
    <aura:attribute name="value" type="String" access="global" />

    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <div>
        <c:Picklist options="{!v.options}" value="{!v.value}" label="{!v.label}" />
    </div>
</aura:component>