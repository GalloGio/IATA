<aura:component implements="forceCommunity:availableForAllPageTypes,force:hasRecordId,force:appHostable" access="global">
	<aura:attribute name="greeting" type="String" default="OneId" access="global" />
	<aura:attribute name="vfHost" type="String" default=""/>
	<aura:attribute name="commName" type="String" default="{!$Label.c.OneId_Community_Name}"/>
	<!--aura:handler name="init" value="{!this}" action="{!c.doInit}" /-->
	
    <div class="oiHeader">
		<a class="homeIcon" href="{!'/' + v.commName}">
			<span class="icon icon-logo"></span>
			<span class="subLogo"></span>
		</a>
		<div id="suiteBar">
			<div id="headerBar">
				&nbsp;
			</div>
                <aura:if isTrue="{!equals(v.serviceName,'FRED')}">                        
                    <a class="homeIcon" href="{!'/'+ $Label.c.OneId_Community_Name+'/login/?language=en_US&amp;serviceName='+v.serviceName}">
                        <span class="icon icon-logo"></span>
                        <span class="{!'subLogo' + v.serviceName}"></span>
                    </a>
                </aura:if>
		</div>	
	</div>
	<div id="miniTopNav">
		<span class="tnContainer centralLimiter">
		</span>
	</div>
</aura:component>