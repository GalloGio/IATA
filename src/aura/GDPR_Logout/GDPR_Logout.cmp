<aura:component access="public" implements="forceCommunity:availableForAllPageTypes"  controller="GDPR_Logout_CTRL">
    <ltng:require scripts="{!$Resource.GDPR_Helper}" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:attribute name="isGuest" type="Boolean" default="false" />
    
    <a class="logout" onclick="{!c.logout}">Logout</a>
</aura:component>