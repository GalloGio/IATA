                function checkForSalutation(){
                  if(!isValidSalutation)
                    $j("[id$=inSalutation]").after("<div class=\"errorMsg\"><strong>Error:</strong> You must enter a value</div>");
                  
                  if( $j("[id$=inSalutation]").val() == '')
                    isValidSalutation = false; 
                  else
