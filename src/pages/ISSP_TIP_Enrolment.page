                function saveContact(isEmailDuplicated){

                  if (jQuery('.errorMsg').length == 0 && isEmailDuplicated == "false"){
                    //cleanModal();
                    closeModal();
                  }
                  else if(isEmailDuplicated == "true"){
                    bootbox.alert("{!$label.ISSP_Application_for_Enlistment_Email_Duplicated}");
