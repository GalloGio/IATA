				                        <apex:commandButton styleclass="btn btn-default"  value="{!$Label.ISSP_AMS_MSO_GSA_MSO_Terminate_MSO}..." disabled="true"  reRender="termination"/>
				                    </apex:outputPanel></apex:facet>
				                </apex:actionStatus>
							</apex:pageBlockButtons>

	                    </apex:pageBlock>

