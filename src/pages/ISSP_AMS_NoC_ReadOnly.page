<apex:page controller="ISSP_AMS_Accreditation"
           cache="false"
           sidebar="false"
           showHeader="false"
           language="{!displayLanguage}"
           standardStylesheets="true"
           applyBodyTag="false"
           applyHtmlTag="false">
    <html>
    <style>
     .col1{
        width:45%;
        padding-right: 15px;
        font-weight: normal;
      }
        
     .colNoC2{
        padding-left:10px;
      }
        
     .colNoC1 {
        width:40%;
        font-weight:bold;
     }
        
     .colNoC1RH {
        width:90%;
        font-weight:bold;
        float:right;
     }
     .col2RH label{
        width:10%;
        float:right;
      }
        
      .colPanel1 {
        width:30%;
        font-weight:bold;
        padding-bottom:10px
      }
        
      .colPanel2 {
        width:65%;
        font-weight:normal !important;
      }
        
        .colInfoSection {
            width:40% !important;
            padding-bottom:10px;
            padding-right:10px;
      }
        
      .headerRow {
            white-space: pre-line !important;
        }
        
        .pdfHeader {
            white-space: pre-line !important;
        }
        .paddedCell {
          padding-left: 3px;
          padding-right: 3px;
        }
    </style>

    <apex:stylesheet value="{!$Resource.CSP_VF_Stylesheet}"/>
    <apex:form >
        <apex:pageBlock >
        <apex:pageBlockSection columns="1" title="{!IF(newlabels,$Label.ISSP_AMS_NoC_Change_Type_and_Location_New,$Label.ISSP_AMS_NoC_Change_Type_and_Location)}" collapsible="false">
                    <apex:panelGrid columns="2" columnClasses="colPanel1, colPanel2" width="100%" rendered="{!OR(con.Account.ANG_Accreditation_Model__c == null, con.Account.ANG_Accreditation_Model__c == 'Legacy')}">
                            <apex:outputText value="{!$Label.ISSP_AMS_NoC_Change_submitted_for}" />
                            <apex:outputText value="{!CASE(newForm.Scope_of_Change__c, 'HO Only', $Label.ISSP_AMS_NoC_Head_Office + ' (' + hqAccount.IATACode__c + ')', 'HO - All Branches', $Label.ISSP_AMS_NoC_HO_All_Branches,'')}"/>
                            <apex:outputtext rendered="{!OR(newForm.Scope_of_Change__c=='HO - Selected Branches',newForm.Scope_of_Change__c=='Selected Branches Only')}">{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.IATACode__c.Label}</apex:outputtext>
                            <apex:outputText rendered="{!OR(newForm.Scope_of_Change__c=='HO - Selected Branches',newForm.Scope_of_Change__c=='Selected Branches Only')}" value="{!IF(newForm.Scope_of_Change__c=='HO - Selected Branches', accountConcerned.IATACode__c + ', ' + agencyCodesConcernedForPDF, agencyCodesConcernedForPDF)}"/>         
                            <apex:outputText value="{!$Label.ISSP_CountryConcerned}"/> 
                            <apex:outputText value="{!amsAllLangCountryLabel}"/> 
                    </apex:panelGrid>
                    <apex:panelGrid columns="2" columnClasses="colPanel1, colPanel2" width="100%" rendered="{!AND(con.Account.ANG_Accreditation_Model__c != null, con.Account.ANG_Accreditation_Model__c != 'Legacy')}">
                            <apex:outputText value="{!$Label.ISSP_AMS_NoC_Change_submitted_for}" />
                            <apex:outputText value="{!CASE(newForm.Scope_of_Change__c, 'HO Only', $Label.ISSP_AMS_NoC_Head_Entity + ' (' + hqAccount.IATACode__c + ')', 'HO - All Branches', $Label.ISSP_AMS_NoC_HE_All_Branches,'')}"/>
                            <apex:outputtext rendered="{!OR(newForm.Scope_of_Change__c=='HO - Selected Branches',newForm.Scope_of_Change__c=='Selected Branches Only')}">{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.IATACode__c.Label}</apex:outputtext>
                            <apex:outputText rendered="{!OR(newForm.Scope_of_Change__c=='HO - Selected Branches',newForm.Scope_of_Change__c=='Selected Branches Only')}" value="{!IF(newForm.Scope_of_Change__c=='HO - Selected Branches', accountConcerned.IATACode__c + ', ' + agencyCodesConcernedForPDF, agencyCodesConcernedForPDF)}"/>         
                            <apex:outputText value="{!$Label.ISSP_CountryConcerned}"/> 
                            <apex:outputText value="{!amsAllLangCountryLabel}" rendered="{!AND(newForm.ANG_Accreditation_Model_to_Change__c != ACCREDITATION_MODEL_MC, con.Account.ANG_Accreditation_Model__c != ACCREDITATION_MODEL_MC)}"/>
                            <apex:outputText value="{!selectedCoutriesForNoCMC}" rendered="{!AND(newForm.ANG_Accreditation_Model_to_Change__c == ACCREDITATION_MODEL_MC, con.Account.ANG_Accreditation_Model__c != ACCREDITATION_MODEL_MC)}"/>
                            <apex:outputText value="{!amsAllLangCountryLabelForMC}" rendered="{!con.Account.ANG_Accreditation_Model__c == ACCREDITATION_MODEL_MC}"/>
                    </apex:panelGrid>
              </apex:pageBlockSection>
        
        <div style="page-break-after:always;margin-top:30px;display:{!IF(newform.Is_change_bank_account_details__c==true && PDFPerRegionCS!=null,'','none')}">
               <apex:pageBlockSection columns="1" collapsible="false">
                    <apex:facet name="header">
                        <apex:outputpanel >
                            {!$Label.ISSP_AMS_NoC_Change_Bank_Details}
                            <apex:commandButton styleclass="iatabtn iatabtn-default" action="{!goToStep3Section1}" value="{!$Label.ISSP_EditSection}" id="Edit-Section1" style="float:right"/>
                        </apex:outputpanel>
                    </apex:facet>
                    <apex:panelGrid columns="1" rendered="{!isocountry.original_bank_details_not_required__c==false}">  
                        <apex:outputText value="{!$Label.ISSP_AMS_NoC_Bank_details_message1}"/>
                        <br/>
                        <apex:outputText escape="false" value="{!PDFPerRegionCS.Address__c}" style="padding-left:26px"/>
                        <apex:outputText value="{!$Label.ISSP_AMS_NoC_Bank_details_message2}"/>
                    </apex:panelGrid>
                    <apex:panelGrid columns="2" rendered="{!isocountry.original_bank_details_not_required__c==true}">  
                        <apex:outputText escape="false" value="{!$Label.ISSP_AMS_NoC_Bank_details_message1}"/>
                    </apex:panelGrid>
                       
                </apex:pageBlockSection>
        </div>
        <!-- NEWGEN-683 -->
        <apex:outputPanel rendered="{!AND(isAccreditationModelNotNullOrLegacy, NOT(newform.Is_change_bank_account_details__c))}">
            <div style="page-break-after:always;margin-top:30px;">
                <apex:pageBlockSection columns="1" collapsible="false">
                    <apex:facet name="header">
                        <apex:outputpanel >
                            {!$Label.ISSP_AMS_NoC_Acreditation_Type_Section_Header}
                            <apex:commandButton styleclass="iatabtn iatabtn-default" action="{!goToStep3Section1}" value="{!$Label.ISSP_EditSection}" id="Edit-Section2" style="float:right"/>
                        </apex:outputpanel>
                    </apex:facet>
                    <apex:panelGrid width="100%" cellpadding="10%" columnClasses="col1, col2" columns="2">
                        <apex:outputPanel layout="block" style="font-weight:bold; padding:5px 10px; font-size:110%;width:100%;background-color:rgb(195,225,240)">{!$Label.ISSP_AMS_NoC_Current_Accreditation_Type}</apex:outputPanel>
                        <apex:outputPanel layout="block" style="font-weight:bold; padding:5px 10px; font-size:110%;width:100%;background-color:rgb(195,225,240)">{!$Label.ISSP_AMS_NoC_Updated_Accreditation_Type}</apex:outputPanel>

                        <apex:panelGrid width="100%" cellpadding="10%" columnClasses="colNoC1RH,colNoC2RH" columns="2">
                            <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Confirm_accreditation_type_are_correct__c.Label}" style="color:black;font-size:110%"/>
                            <apex:outputText ><img src="{!IF(newForm.Confirm_accreditation_type_are_correct__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}"/></apex:outputText>
                        </apex:panelGrid>

                        <apex:panelGrid width="100%" cellpadding="10%" columnClasses="colNoC1RH,colNoC2RH" columns="2">
                            <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Notify_Change_Accreditation_Type__c.Label}" style="color:black;font-size:110%"/>
                            <apex:outputText ><img src="{!IF(newForm.Notify_Change_Accreditation_Type__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}"/></apex:outputText>
                        </apex:panelGrid>

                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                            <apex:outputLabel value="{!$Label.ANG_ISSP_AccreditationType}"/>
                            <apex:outputText value="{!IF(con.Account.ANG_Accreditation_Model__c == 'Cash', $Label.ANG_ISSP_ProductStandardAccreditationWithCash, IF(con.Account.ANG_Accreditation_Model__c == 'Cashless', $Label.ANG_ISSP_ProductStandardAccreditationWithNoCash, $Label.ANG_ISSP_ProductStandardAccreditationWithMultiCountry))}" />
                        </apex:panelGrid>
                        <apex:outputPanel rendered="{!NOT(newForm.Notify_Change_Accreditation_Type__c)}"></apex:outputPanel>
                        <apex:outputPanel rendered="{!newForm.Notify_Change_Accreditation_Type__c}">
                            <apex:panelGrid width="100%" cellpadding="10%" columns="1">
                                <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                                    <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ANG_Accreditation_Model_to_Change__c.Label}"/>
                                    <apex:outputText value="{!IF(newForm.ANG_Accreditation_Model_to_Change__c == 'Cash', $Label.ANG_ISSP_ProductStandardAccreditationWithCash, IF(newForm.ANG_Accreditation_Model_to_Change__c == 'Cashless', $Label.ANG_ISSP_ProductStandardAccreditationWithNoCash, $Label.ANG_ISSP_ProductStandardAccreditationWithMultiCountry))}"></apex:outputText>
                                </apex:panelGrid>
                                <apex:outputPanel rendered="{!newForm.ANG_Accreditation_Model_to_Change__c == ACCREDITATION_MODEL_MC}">
                                    <apex:outputPanel id="accountsToBeConvertedPanel">
                                        <apex:pageBlockTable value="{!accountsToBeConverted}" var="acc" rendered="{!accountsToBeConverted.size > 0}" style="margin-top: 20px;">
                                            <apex:column value="{!acc.Name}"/>
                                            <apex:column value="{!acc.IATACode__c}"/>
                                            <apex:column value="{!acc.BillingCountry}"/>
                                        </apex:pageBlockTable>
                                    </apex:outputPanel>
                                </apex:outputPanel>

                                <apex:outputPanel rendered="{!AND(newForm.ANG_Accreditation_Model_to_Change__c == 'Cash', hqAccount.ANG_Accreditation_Model__c != ACCREDITATION_MODEL_MC)}">
                                    <apex:outputPanel layout="block" style="font-weight:bold; padding:5px 10px; font-size:110%;width:100%;background-color:rgb(195,225,240)">Assessment Contact</apex:outputPanel>
                                    <apex:repeat value="{!List_IFAPAgencyContacts}" var="ifapCon">
                                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                                            <apex:outputLabel style="padding-left: 5px;" value="{!$ObjectType.Contact.fields.FirstName.Label}"/>
                                            <apex:outputText value="{!ifapCon.First_name__c}"></apex:outputText>
                                        </apex:panelGrid>
                                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                                            <apex:outputLabel style="padding-left: 5px;" value="{!$ObjectType.Contact.fields.LastName.Label}"/>
                                            <apex:outputText value="{!ifapCon.Last_name__c}"></apex:outputText>
                                        </apex:panelGrid>
                                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                                            <apex:outputLabel style="padding-left: 5px;" value="{!$ObjectType.Contact.fields.Phone.Label}"/>
                                            <apex:outputText value="{!ifapCon.Phone__c}"></apex:outputText>
                                        </apex:panelGrid>
                                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                                            <apex:outputLabel style="padding-left: 5px;" value="{!$ObjectType.Contact.fields.Email.Label}"/>
                                            <apex:outputText value="{!ifapCon.Email__c}"></apex:outputText>
                                        </apex:panelGrid>
                                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                                            <apex:outputLabel style="padding-left: 5px;" value="{!$ObjectType.AMS_Accreditation_Contact__c.fields.Financial_Assessment_Contact__c.Label}"/>
                                            <apex:outputText ><img src="{!IF(ifapCon.Financial_Assessment_Contact__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}"/></apex:outputText>
                                        </apex:panelGrid>
                                    </apex:repeat>
                                </apex:outputPanel>
                            </apex:panelGrid>
                        </apex:outputPanel>
                    </apex:panelGrid>
                </apex:pageBlockSection>
            </div>
        </apex:outputPanel>
        <!--END NEWGEN-683 -->
        
        <div style="page-break-after:always;margin-top:30px;display:{!IF(newform.Is_change_legal_name__c==true,'','none')}">
              <apex:pageBlockSection columns="1" collapsible="false">
                  <apex:facet name="header">
                            <apex:outputpanel >
                                <apex:outputText rendered="{!!isAccreditationModelNotNullOrLegacy}">{!IF(newlabels, $Label.ISSP_AMS_NoC_Change_of_Legal_Name_New, $Label.ISSP_AMS_NoC_Change_of_Legal_Name)}</apex:outputText>
                                <apex:outputText rendered="{!isAccreditationModelNotNullOrLegacy}">
                                    {!SUBSTITUTE(IF(newlabels, $Label.ISSP_AMS_NoC_Change_of_Legal_Name_New, $Label.ISSP_AMS_NoC_Change_of_Legal_Name), '1', '2')}
                                </apex:outputText>
                                <apex:commandButton styleclass="iatabtn iatabtn-default" action="{!goToStep3Section2}" value="{!$Label.ISSP_EditSection}" id="Edit-Section2" style="float:right"/>
                            </apex:outputpanel>                       
                        </apex:facet>
                    <apex:panelGrid width="100%" cellpadding="10%" columnClasses="col1, col2" columns="2">     
                        <apex:outputPanel layout="block" style="font-weight:bold; padding:5px 10px; font-size:110%;width:100%;background-color:rgb(195,225,240)">{!$Label.ISSP_AMS_NOC_Current_Legal_details}</apex:outputPanel>
                        <apex:outputPanel layout="block" style="font-weight:bold; padding:5px 10px; font-size:110%;width:100%;background-color:rgb(195,225,240)">{!$Label.ISSP_AMS_NoC_Legal_Name_After_Change}</apex:outputPanel>
                        
                    <apex:panelGrid width="100%" cellpadding="10%" columnClasses="colNoC1RH,colNoC2RH" columns="2">
                        <apex:outputLabel style="float:right" value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Confirm_Legal_Details_are_correct__c.Label}" />
                        <apex:outputText ><img src="{!IF(newForm.Confirm_Legal_Details_are_correct__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}"/></apex:outputText>
                    </apex:panelGrid>
                        
                <apex:panelGrid width="100%" cellpadding="10%" columnClasses="colNoC1RH,colNoC2RH" columns="2">
                    <apex:outputLabel style="float:right" value="{!IF(isCNSAgency, $Label.ISSP_AMS_Notify_Change_Legal_Details_CNS, $ObjectType.AMS_Pax_Accreditation_Form__c.fields.Notify_Change_Legal_Details__c.Label)}" />
                    <apex:outputText ><img src="{!IF(newForm.Notify_Change_Legal_Details__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}"/></apex:outputText>
                </apex:panelGrid>
                           
                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                            <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Legally_Registered_Name__c.Label}" style="font-weight:bold; font-size:110%"/>
                            <apex:outputField value="{!accountConcerned.Name}" />
                        </apex:panelGrid>
                        
                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2" rendered="{!newForm.Notify_Change_Legal_Details__c}">
                             <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Legally_Registered_Name__c.Label}" style="font-weight:bold; font-size:110%"/>
                             <apex:outputField value="{!newForm.Account_Name__c}" />
                         </apex:panelGrid>
                        <apex:outputPanel rendered="{!NOT(newForm.Notify_Change_Legal_Details__c)}"></apex:outputPanel>
                        
                       <apex:outputtext rendered="{!AND(isocountry.Local_language_Agency_details_required__c,newForm.Notify_Change_Legal_Details__c)}"><!--empty: move following field to next column -->&nbsp;</apex:outputtext>
                        <apex:panelGrid width="100%" cellpadding="10%" columnClasses="col1, col2" columns="2" rendered="{!AND(isocountry.Local_language_Agency_details_required__c,newForm.Notify_Change_Legal_Details__c)}">
                             <apex:outputLabel value="{!txtLocalAgencyDetailsLegalName}" style="font-weight:bold; font-size:110%"/>
                             <apex:outputField value="{!newForm.Local_Legally_Registered_Name__c}" />
                        </apex:panelGrid>
                        
                            <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                                <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Trade_Name__c.Label}" style="font-weight:bold; font-size:110%"/>
                                <apex:outputField value="{!accountConcerned.TradeName__c}" />
                           </apex:panelGrid>
                          
                           <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2" rendered="{!newForm.Notify_Change_Legal_Details__c}">
                                <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Trade_Name__c.Label}" style="font-weight:bold; font-size:110%"/>
                               <apex:outputField value="{!newForm.Trade_Name__c}"/>
                           </apex:panelGrid>
                        <apex:outputPanel rendered="{!NOT(newForm.Notify_Change_Legal_Details__c)}"></apex:outputPanel>
                        
                        <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                                <apex:outputLabel value="{!IF(isCNSAgency, $Label.ISSP_PAX_Federal_EIN, $ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_VAT_Number_Input__c.Label)}" style="font-weight:bold; font-size:110%"/>
                                <apex:outputField value="{!accountConcerned.VAT_Number__c}" />
                           </apex:panelGrid>
                          
                           <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2" rendered="{!newForm.Notify_Change_Legal_Details__c}">
                                <apex:outputLabel value="{!IF(isCNSAgency, $Label.ISSP_PAX_Federal_EIN, $ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_VAT_Number_Input__c.Label)}" style="font-weight:bold; font-size:110%"/>
                               <apex:outputText value="{!IF(isEUcountry, newCase.IFAP_Country_ISO__c + newForm.ISSP_AMS_VAT_Number_Input__c,newForm.ISSP_AMS_VAT_Number_Input__c)}"/>
                           </apex:panelGrid>
                    </apex:panelGrid>
                                             
              </apex:pageBlockSection>       
                       
        </div>
        
             
        <div style="page-break-after:always;margin-top:30px;display:{!IF(newform.Is_change_location__c==true,'','none')}">
        <apex:pageBlockSection columns="1" collapsible="false">
                <apex:facet name="header">
                            <apex:outputpanel >
                                <apex:outputText rendered="{!!isAccreditationModelNotNullOrLegacy}">{!IF(newlabels, $Label.ISSP_AMS_NoC_Change_of_Location_New, $Label.ISSP_AMS_NoC_Change_of_Location)}</apex:outputText>
                                <apex:outputText rendered="{!isAccreditationModelNotNullOrLegacy}">
                                    {!SUBSTITUTE(IF(newlabels, $Label.ISSP_AMS_NoC_Change_of_Location_New, $Label.ISSP_AMS_NoC_Change_of_Location), '2', '3')}
                                </apex:outputText>
                                <apex:commandButton styleclass="iatabtn iatabtn-default" action="{!goToStep3Section3}" value="{!$Label.ISSP_EditSection}" id="Edit-Section3" style="float:right"/>
                            </apex:outputpanel>                       
                </apex:facet>
              
                <apex:panelGrid width="100%" cellpadding="10%" columnClasses="col1, col2" columns="2">    
                    <apex:outputPanel layout="block" style="font-weight:bold; padding:5px 10px; font-size:110%;width:100%;background-color:rgb(195,225,240)">{!IF(newlabels, $Label.ISSP_AMS_NOC_Current_Location_New, $Label.ISSP_AMS_NOC_Current_Location)}</apex:outputPanel>
                    <apex:outputPanel layout="block" style="font-weight:bold; padding:5px 10px; font-size:110%;width:100%;background-color:rgb(195,225,240)">{!IF(newlabels, $Label.ISSP_AMS_NoC_Location_After_Change_New, $Label.ISSP_AMS_NoC_Location_After_Change)}</apex:outputPanel>   
                
                    <apex:panelGrid width="100%" cellpadding="10%" columnClasses="colNoC1RH,colNoC2RH" columns="2">
                        <apex:outputLabel style="float:right" value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Confirm_Location_Details_are_correct__c.Label}" />
                        <apex:outputText ><img src="{!IF(newForm.Confirm_Location_Details_are_correct__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}"/></apex:outputText>
                    </apex:panelGrid>
                        
                <apex:panelGrid width="100%" cellpadding="10%" columnClasses="colNoC1RH,colNoC2RH" columns="2">
                    <apex:outputLabel style="float:right" value="{!IF(isCNSAgency, $Label.ISSP_AMS_Notify_Change_Legal_Details_CNS, $ObjectType.AMS_Pax_Accreditation_Form__c.fields.Notify_Change_Location__c.Label)}" />
                    <apex:outputText ><img src="{!IF(newForm.Notify_Change_Location__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}"/></apex:outputText>
                </apex:panelGrid>
                    <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2">
                        <apex:outputtext value="{0}
                                    <br />{1}<br />{2}<br /><br />{!$Label.ISSP_Office_Email}: {3}<br />{!$Label.ISSP_Registration_Website}: {4}<br /><br />{!$Label.ISSP_Registration_BusinessPhone}: {5}<br />{!$Label.ISSP_Registration_MobilePhone}: {6}<br />Fax: {7}" escape="false" label="{!$Label.ISSP_AMS_Step3_Office_FullAddress}">
                                    <apex:param value="{!accountConcerned.BillingStreet}" />
                                    <apex:param value="{!accountConcerned.BillingCity},&nbsp;{!accountConcerned.BillingPostalCode}" />
                                    <apex:param value="{!IF(accountConcerned.BillingState!='',accountConcerned.BillingState + ',&nbsp;','')}{!accountConcerned.BillingCountry}" />
                  <apex:param value="{!accountConcerned.Email__c}" />
                  <apex:param value="{!accountConcerned.Website}" />
                                    <apex:param value="{!accountConcerned.Phone}" />
                                    <apex:param value="{!accountConcerned.Mobile__c}" />
                                    <apex:param value="{!accountConcerned.FAX}" />
                                </apex:outputtext>
                    </apex:panelGrid>
                    
                    <apex:panelGrid width="100%" columnClasses="col1, col2" cellpadding="10%" columns="2" rendered="{!newForm.Notify_Change_Location__c}">
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Branch_Office_Street_name_number__c.Label}" style="font-weight:bold; font-size:110%"/>
                            <apex:outputField value="{!newForm.Branch_Office_Street_name_number__c}" />
                        
                            <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Branch_Office_City__c.Label}" style="font-weight:bold; font-size:110%"/>
                            <apex:outputField value="{!newForm.Branch_Office_City__c}" />
                  
                            <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Branch_Office_Postal_code__c.Label}" style="font-weight:bold; font-size:110%"/>
                            <apex:outputField value="{!newForm.Branch_Office_Postal_code__c}" />
                      
                            <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.State_Province__c.Label}" style="font-weight:bold; font-size:110%"/>
                            <apex:outputField value="{!newForm.State_Province__c}" />
                         
                            <apex:outputLabel value="{!$Label.ISSP_Country}" style="font-weight:bold; font-size:110%"/>
