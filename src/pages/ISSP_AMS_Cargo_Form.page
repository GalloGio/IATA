    function checkPercentage(box){
        var thisBox = document.getElementById(box);
        var value = thisBox.value
        value = value.replace(/,/g, ".");
        value = parseFloat(value);
        if (isNaN(value)){
            value = "";
        }
        else if (value < 0){
            value = 0;
        }
        else if (value > 100){
            value = 100;
        }
        thisBox.value = value;
    }

