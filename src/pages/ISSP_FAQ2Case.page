                      <div class="modal-footer">
                          <button type="button" class="btn btn-primary"
                                  data-dismiss="modal"
                                  onclick="ctrlLivAgIntroVidCloseDontRemenber();">{!$Label.LVA_IntroVidCloseDontShowNext}</button>
                          <button type="button" class="btn btn-primary" data-dismiss="modal">{!$Label.LVA_IntroVidCloseShowNext}</button>
                      </div>
                  </div>
              </div>
          </div>
          <script>
              var introVideoSrc='{!livAgentIntroVidUrl}';
              var livAgentIntroVidMode='{!livAgentIntroVidMode}';
              var newwindow;


              function introVideoPopUp(){
                //console.log('basto1p - POP UP INTRO VIDEO INSIDE!');
                if (newWindow!=undefined && !newWindow.closed){

                    //cloes window in order to when open it again the popup came to the front
                    newWindow.close();
                }
                newwindow = window.open('{!livAgentIntroVidUrl}',
                                            '{!$Label.LVA_IntroductionVideo}',
                                            'scrollbars=yes,toolbar=no,status=no,'+
                                            'target=_blank,centerscreen=yes,'+
                                            'width=570, height=350');
                newwindow.focus();
              }

              //program on modal hidden event
              $('#IntroVideoModal').on('hidden.bs.modal', function () {
                    closeIframeVideo();
                })

              function closeIframeVideo(){
                $('#introVideoIFrame').attr('src', '');
              }

              function openIntroVideo(){
                if (livAgentIntroVidMode=='LVA_INTRO_VIDEO_MODE_POP_UP'){
                    introVideoPopUp();
                }
                else{
                    $('#introVideoIFrame').attr('src', introVideoSrc);
                    $('#IntroVideoModal').modal('show');
                }
              }
          </script>
      <apex:outputPanel id="introVideo"
                        rendered="{! IF(isIntroVideoAlreadyShowed==true ,false ,true)}">
          <script>
            openIntroVideo();
          </script>
      </apex:outputpanel>


