                                                <c:ISSP_AMC_Authorized_Areas_Operation participation="{!participation}" rendered="{!ISSP_AMC_Authorized_Areas_Operation}" hasError="{!hasError}" />
                                                <c:ISSP_AMC_PreviousIATACode participation="{!participation}" rendered="{!ISSP_AMC_PreviousIATACode}" hasError="{!hasError}"/>
                                                <c:ISSP_AMC_ICAOCode participation="{!participation}" rendered="{!ISSP_AMC_ICAOCode}" hasError="{!hasError}" />
                                                <c:ISSP_AMC_SchedulesPublication participation="{!participation}" rendered="{!ISSP_AMC_SchedulesPublication}" hasError="{!hasError}"/>
                                                <c:ISSP_AMC_ULD participation="{!participation}" rendered="{!ISSP_AMC_ULD}" hasError="{!hasError}"/>
                                                <c:ISSP_AMC_PreferenceOfCode participation="{!participation}" rendered="{!ISSP_AMC_PreferenceOfCode}" preferencevalidation="designator" hasError="{!hasError}"/>
                                                <c:ISSP_AMC_Upload_Files participation="{!participation}" rendered="{!ISSP_AMC_Upload_Files}"
                                                                         ISSP_AMC_Airline_Issued_AOC="{!ISSP_AMC_Airline_Issued_AOC}"
                                                                         ISSP_AMC_CertificateIncorporationRegistration="{!ISSP_AMC_CertificateIncorporationRegistration}"
                                                                         ISSP_AMC_ULD="{!ISSP_AMC_ULD}"
                                                                         ISSP_AMC_Non_Scheduled="{!ISSP_AMC_Non_Scheduled}"
                                                                         ISSP_AMC_TrafficAgreement_Valid_IATA_Code="{!ISSP_AMC_TrafficAgreement_Valid_IATA_Code}"
                                                                         ISSP_AMC_Authorization_Transportation_Services="{!ISSP_AMC_Authorization_Transportation_Services}"
                                                                         ISSP_AMC_Signed_Letter_CRS="{!ISSP_AMC_Signed_Letter_CRS}"
                                                                         ISSP_AMC_BreakdownOfShareholders="{!ISSP_AMC_BreakdownOfShareholders}"
                                                />
                                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </apex:outputpanel>
                        </div>

                        <div class="tab-pane fade tab-pane1" id="step3">
                            <apex:outputPanel id="step3Panel">
                                <div class="well">
                                    <div class="row">
                                            <div class="col-sm-12">
                                                <div class="col-sm-11">
                                                    <div class="alert alert-warning" role="alert">
                                                        {!$Label.ISSP_AMS_Step2_Instructions}
                                                    </div>
                                                    <apex:outputPanel id="errorMessageStep3">
                                                        <apex:outputPanel rendered="{!displayMessage}">
                                                            <div class="alert alert-{!severity} alert-dismissible">
                                                                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                                                <apex:outputText id="step3message" value="{!message}" escape="false" />
                                                            </div>
                                                        </apex:outputPanel>
                                                    </apex:outputPanel>
                                                    <!--<apex:pageMessages />-->
                                                </div>
                                                <div class="col-sm-1">
                                                    <apex:commandLink id="btnStep3Id" styleClass="btn btn-success" action="{!saveParticipation}" reRender="errorMessageStep1,errorMessageStep2,step3Panel,errorMessageStep4" status="Loading" onclick="startLoading()" oncomplete="stopLoading()" value="Save" >
                                                        <apex:param assignTo="{!step}" value="3"/>
                                                    </apex:commandLink>
                                                </div>
                                            </div>
                                        <div class="col-sm-12">
                                            <div class="panel-body">
                                                <div class="form-group">
                                                    <div class="panel panel-primary" style="overflow-x:auto;">
                                                        <table class="table panel-primary">
                                                            <thead class="panel-heading">
                                                                <tr>
                                                                    <td class="col-md-9 col-sm-1">{!$Label.ISSP_AMS_Step2_Check_List}</td>
                                                                    <td class="col-md-2 col-sm-1" style="text-align: center;">Document Required?</td>
                                                                    <td class="col-md-1 col-sm-1" style="text-align: center;">Status</td>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <apex:repeat value="{!appRequirementList}" var="requirementItem">
                                                                    <tr style="display: {!IF(requirementItem.isToShow, '','none')}">
                                                                        <th scope="row">{!requirementItem.requirementDescription}</th>
                                                                        <td style="text-align: center;">
                                                                            <span class="{!IF(requirementItem.requirementType == 'Attachment', 'glyphicon glyphicon-open', '')}" aria-hidden="true" style=" color: #003B74; font-size: 20px;" />
                                                                        </td>
                                                                        <td style="text-align: center;">
                                                                            <span class="{!IF(requirementItem.requirementStatus == 'Done', 'glyphicon glyphicon-ok', 'glyphicon glyphicon-exclamation-sign')}" aria-hidden="true" style="color:{!IF(requirementItem.requirementStatus == 'Done', 'green', 'red')}; font-size: 20px;"/>
                                                                        </td>
                                                                    </tr>
                                                                </apex:repeat>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </apex:outputPanel>
                        </div>

                        <div class="tab-pane fade tab-pane1" id="step4">
                            <apex:outputPanel id="step4Panel">
                                <div class="well">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="col-sm-12">
                                                <apex:outputPanel id="errorMessageStep4">
                                                    <apex:outputPanel rendered="{!displayMessage}">
                                                        <div class="alert alert-{!severity} alert-dismissible">
                                                            <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                                            <apex:outputText id="step4message" value="{!message}" escape="false" />
                                                        </div>
                                                    </apex:outputPanel>
                                                </apex:outputPanel>
                                            </div>

                                            <div class="col-sm-12">
                                                <c:ISSP_AMC_TermsAndConditions participation="{!participation}" rendered="{!ISSP_AMC_TermsAndConditions}" hasError="{!hasError}" />
                                            </div>
                                            <div class="col-sm-11">
                                                &nbsp;
                                            </div>
                                            <div class="col-sm-1">
                                                <apex:commandLink id="btnStep4Id" styleClass="btn btn-success" action="{!saveParticipation}" reRender="errorMessageStep1,errorMessageStep2,step3Panel,errorMessageStep4,step2Panel,step1Panel,step4Panel" status="Redirect" value="Submit">
                                                    <apex:param assignTo="{!step}" value="Submit"/>
                                                </apex:commandLink>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </apex:outputPanel>
                        </div>

                    </apex:form>  
                </div>
                
                <c:ISSP_CustomFooter communityName="{!communityName}"/>
                
            </div>
        </body>
    </html>
</apex:page>