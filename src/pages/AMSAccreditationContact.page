        function setTabTitle() {
            //Set the current tab's title
            if({!isCertificationOSCAR})
                sforce.console.setTabTitle('Agency trained staff');
            else
                sforce.console.setTabTitle('Agency owners');
        }
        var pageLoad = window.onload;
        window.onload = function()
        {
            if (pageLoad)
            {
                pageLoad();
            }
            setTabTitle();
            checkCorrection(); //FM AMSU-1
        }

        //FM AMSU-1
        function checkCorrection(){
            if({!isCorrection} && !({!isAllHierarchySelected})){
                
                alert('Only able to manage Owners when the hierarchy is all selected!');
                var saveBtn = document.getElementById('saveButton');
                saveBtn.disabled = true;
                closeTab();
            }
            
        }

        //FM AMSU-132
        var $j = jQuery.noConflict();

        function showNewTrainedStaffDialog() {
            $j(function() {
                //$j( '#importNewTrainedStaffModal').modal('show');
                //alert('aqui');
                j$('#backdrop').addClass('slds-backdrop--open');
                j$('#modal').addClass('slds-fade-in-open');
          });
        }

        function closeTrainedStaffModal() {

            $j(function() {
                //$j( '#importNewTrainedStaffModal' ).modal('hide');
                j$('#modal').removeClass('slds-fade-in-open');
                j$('#backdrop').removeClass('slds-backdrop--open');
                //refreshTab();
          });

        }
        //FM AMSU-132 - END
    </script>
    <script>
        function saveServerJs()
        {
            var saveBtn = document.getElementById('saveButton');
            saveBtn.disabled = true;
            saveServer();
        }
        
        //AMSU-132
        function saveServerImportJs()
        {
            var saveBtn = document.getElementById('saveButtonImport');
