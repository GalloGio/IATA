  	function onlyNumbers(event) {
   		var event = event || window.event; // for trans-browser compatibility
        var charCode = event.which || event.keyCode;
     
        if (charCode === 44 || charCode === 46){
            return true;
        } else if ( charCode > 31 && (charCode < 48 || charCode > 57) ){
            return false;
        }

        return true;                
    }
  
