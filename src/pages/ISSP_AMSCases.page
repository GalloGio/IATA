                      <button type="button" onclick="modalModalOptOutClose()" class="btn btn-danger btn-block">{!$label.LAQ_Close}</button>
                    </div>
                    <div class="col-md-6">
                      <button type="button" onclick="doOptOut()" class="btn btn-success btn-block">{!$label.IDCard_Continue_Button}</button>                
