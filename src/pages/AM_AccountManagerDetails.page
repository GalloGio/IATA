<apex:page standardController="Account"  showHeader="false" sidebar="false" standardStylesheets="true">
	
	<div class="bPageBlock bDetailBlock" style="margin-left: -10px;">
		<div class="pbBody">
			<div class="pbSubsection">

				<table class="detailList" border="0" cellpadding="0" cellspacing="0">
					<tr>
						<td class="labelCol">Account Manager</td>
						<td class="dataCol col02 inlineEditLock"><apex:outputLink target="_blank" value="/{!Account.AM_Account_Manager__c}" >{!Account.AM_Account_Manager__r.Name}</apex:outputLink></td>
						<td class="labelCol">Email</td>
						<td class="dataCol inlineEditLock"><apex:outputField value="{!Account.AM_Account_Manager__r.Email}" /></td>
					</tr>
					<tr>
						<td class="labelCol">Title</td>
						<td class="dataCol col02 inlineEditLock"><apex:outputField value="{!Account.AM_Account_Manager__r.PPD_Tittle__c}" /></td>
						<td class="labelCol">Phone</td>
						<td class="dataCol inlineEditLock"><apex:outputField value="{!Account.AM_Account_Manager__r.Phone}" /></td>
					</tr>
					<tr>
						<td class="labelCol">Office</td>
						<td class="dataCol col02 inlineEditLock"><apex:outputField value="{!Account.AM_Account_Manager__r.PPD_Office__c}" /></td>
						<td class="labelCol">Mobile</td>
						<td class="dataCol inlineEditLock"><apex:outputField value="{!Account.AM_Account_Manager__r.MobilePhone}" /></td>
					</tr>
				</table>
			
			</div>
		</div>
	</div>


</apex:page>