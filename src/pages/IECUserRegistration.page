        <script>
        function registerNewAccount() {
        
            var btnBack2ToSearchAccount = this.document.getElementById('{!$Component.activeSubscriptionForm.back2ToSearchAccount}');
            var btnRegisterNewAccount = this.document.getElementById('{!$Component.activeSubscriptionForm.registerNewAccount}');
                
            changeCursorProgress(btnBack2ToSearchAccount);
            changeCursorProgress(btnRegisterNewAccount);

            disableControl(btnBack2ToSearchAccount);
            disableControl(btnRegisterNewAccount);
            
            proceedToRregisteNewAccount();
        }
    
        function registerExistingAccount() {
        
            var btnRegisterContact = this.document.getElementById('{!$Component.activeSubscriptionForm.registerContact}');
            var btnCreateAccount = this.document.getElementById('{!$Component.activeSubscriptionForm.createAccount}');
                
            changeCursorProgress(btnRegisterContact);
            changeCursorProgress(btnCreateAccount);

            disableControl(btnRegisterContact);
            disableControl(btnCreateAccount);
            
            proceedToRregisterPortalUser();
        }
    
        
        
        function registerWithEmailChange() {
        
        
            var btnBackToSearchAccount = this.document.getElementById('{!$Component.activeSubscriptionForm.backToSearchAccount}');
            var btnRegisterContactWithNewEmail = this.document.getElementById('{!$Component.activeSubscriptionForm.registerContactWithNewEmail}');
                
            changeCursorProgress(btnBackToSearchAccount);
            changeCursorProgress(btnRegisterContactWithNewEmail);

            disableControl(btnBackToSearchAccount);
            disableControl(btnRegisterContactWithNewEmail);
            
            proceedToRegisterUserWithEmailChange();
        }
        
    
    </script>
