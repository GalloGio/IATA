            closeModal();
            setPortalAdmin('{!$Component.AMSForm.PortalAdminId}');
            $( '.datePicker').css('position','absolute');
        }

        var natphone = $("input[name='{!$Component.newContact.contactBlock.contactphoneinput}']");
        var phone = $("input[name='{!$Component.newContact.contactBlock.contactphone}']");

        natphone.intlTelInput({
            nationalMode: true,
            defaultCountry: defaultHQCountry,
            numberType: "FIXED_LINE",
            utilsScript: "{!URLFOR($Resource.InternationalPhoneNumberFormat, '/intl-tel-input-master/lib/libphonenumber/build/utils.js')}"
        });

        natphone.intlTelInput("setNumber", phone.val());

        var natmobile = $("input[name='{!$Component.newContact.contactBlock.contactmobileinput}']");
        var mobile = $("input[name='{!$Component.newContact.contactBlock.contactmobile}']");
        
         natmobile.intlTelInput({
            nationalMode: true,
            defaultCountry: defaultHQCountry,
            numberType: "MOBILE",
            utilsScript: "{!URLFOR($Resource.InternationalPhoneNumberFormat, '/intl-tel-input-master/lib/libphonenumber/build/utils.js')}"
        });
