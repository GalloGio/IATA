                                </div>
                                
                                <div class="row">
                                    <apex:outputLabel styleClass="lbl col-lg-5" value="{!$ObjectType.Contact.fields.Membership_Function__c.Label}" />
                                    <div class="col-lg-7"><apex:outPutField value="{!Contact.Membership_Function__c }"/></div>
                                </div>
                                 <div class="row">
