<apex:page controller="ISSP_APProcess"  renderAs="pdf" applyHtmlTag="false" showHeader="false">
    
    <apex:stylesheet value="{!URLFOR($Resource.ISSP_AP_CSS)}"/>
    
    <div class="header">
        <div><apex:image width="100%" value="{!URLFOR($Resource.AP_PDF_Header)}"/></div>
    </div>
    
    <div class="footer">
        <table border="0" width="100%">
            <tr>
                <td width="33%" align="left">
                    {!CASE(MONTH(TODAY()),
                    1, "January",
                    2, "February",
                    3, "March",
                    4, "April",
                    5, "May",
                    6, "June",
                    7, "July",
                    8, "August",
                    9, "September",
                    10, "October",
                    11, "November",
                    12, "December",
