        if (country == 'Canada' || country == 'United States' || country == 'Venezuela' || country == 'Australia' || country == 'Brazil') {
            $j("#shippingstate").find("input").attr('readonly', true);
            $j("#billingstate").find("input").attr('readonly', true);
        
