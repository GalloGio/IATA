                    //alert("change");
                    var shippingc = $j("#shippingcity").find("input").val();
                    var shippings = $j("#shippingstate").find("input").val();
                    if ((!shippingc || 0 === shippingc.length) && shippings.length>0) {
                        //alert("Delete sstate");
                        $j("#shippingstate").find("input").val("");
                }
