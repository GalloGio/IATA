                if(!$j(this).val() || ($j(this).hasClass('len3') && $j(this).val().length<3)){
                    flag = false;
                    $j(this).parents('.form-group').addClass('has-error');
                }
            })
            if(flag == false){
                $j('#recFields').show();
            }
            return flag;
        }
        
