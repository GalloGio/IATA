              			natmobile.intlTelInput("setNumber", mobile.val());
              
              			var natfax = $j("input[name='{!$Component.newContact.contactBlock.contactfaxinput}']");
              			var fax = $j("input[name='{!$Component.newContact.contactBlock.contactfax}']");
          
               			natfax.intlTelInput({
