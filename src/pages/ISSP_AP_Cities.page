<apex:page standardController="AP_Process_Form__c" extensions="ISSP_AP_Cities" >

	<apex:pageBlock >
	<apex:pageBlockTable value="{!cityList}" var="city" >
        <apex:column > 
            <apex:facet name="header">
               City
            </apex:facet>
            {!city.city}
        </apex:column>
        <apex:column > 
            <apex:facet name="header">
               Percentage Value
            </apex:facet>
            {!city.percentage}
        </apex:column>
	</apex:pageBlockTable>
	</apex:pageBlock>
	
</apex:page>