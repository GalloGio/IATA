          //document.getElementById("yearend").value,
          document.getElementById("exclOpenCases").checked,
          document.getElementById("exclClosedCases").checked
          );
      }
      
      function getAgentType()
      {
        var namelist = "";
        
        var ob = document.getElementById("agentType");
        
        for (var i = 0; i < ob.options.length; i++)
        {
            if (ob.options[ i ].selected)
                    namelist += ob.options[ i ].value + "-";
        }

           return namelist;
      
      }
      function getAgentStatus() 
      { 
        return "Approved-Listed-Changes processed-Under review-Cash basis/with commission";
        

     }
      

      

      function validateOpenCases(sendEmail){
  
        doIt = true;
        
       
         var oc = getInputEndingWith("OpenCase").value;

          if (oc == "true")
          {
            // now we block if open cases
            //doIt = confirm('Some of the selected agents already have open IATA Financial Review cases. Do you want to continue?');
            alert('Some of the selected agents already have open IATA Financial Review cases.');
            return false;
          }  

          if (doIt && document.getElementById('ClosedCase').value == "true"  &&  $("*[id*=otherCRE]").val() == 'false') {
            alert('Some of the selected agents have closed IATA Financial Review cases for the selected Financial Year.');
            return false;
          }
        
          if(doIt){
                // run Mass case creation
                if (sendEmail == 'true'){
                  MassCaseCreateSendEmail();
                   }
                else{
               MassCaseCreate();
                    }
          }
         return false;
       }
       
       //used to get OpenCase flag from controller which is saved in a hidden field
        function getInputEndingWith(endsWith)
        {
            var r = new RegExp("(.*)"+endsWith+"$");  
            var inputs = document.getElementById('OpenCase');
            var target;
            return inputs;
        }
      
   function openLookup(baseURL, width, modified, searchParam){
   
   
    var originalbaseURL = baseURL;
    var originalwidth = width;
    var originalmodified = modified;
    var originalsearchParam = searchParam;
 
    var lookupType = baseURL.substr(baseURL.length-3, 3);
    if (modified == '1') baseURL = baseURL + searchParam;

    var isCustomLookup = false;
 
    // Following "001" is the lookup type for Account object so change this as per your standard or custom object
    if(lookupType == "{!GetEmailTemplateKeyPrefix}"){
 
      var urlArr = baseURL.split("&");
      var txtId = '';
      if(urlArr.length > 2) {
        urlArr = urlArr[1].split('=');
        txtId = urlArr[1];
      }
 
      // Following is the url of Custom Lookup page. You need to change that accordingly
      baseURL = "/apex/EmailTemplateCustomLookupPage?txt=" + txtId;
 
      // Following is the id of apex:form control "myForm". You need to change that accordingly
      baseURL = baseURL + "&frm=" + escapeUTF("{!$Component.myForm}");
      if (modified == '1') {
        // the Email Templates will be filtered by country
        //searchParam = document.getElementById('country').value;
        searchParam = document.getElementById('{!$Component.ISOcountry}_lkid').value;
        console.log(searchParam);
        //console.log(getElementByIdCS('massCaseCreationPage:massCaseCreationForm:j_id0:criteria:ISOcountry_lkid').value);
        //searchParam = $('!$Component.ISOcountry').attr('id');
        //alert(searchParam);
        //searchParam = encodeURI(searchParam);
        //searchParam = searchParam.replace("'", "%27");
        //searchParam = searchParam.replace("(", "%28");
        //searchParam = searchParam.replace(")", "%29");
        baseURL = baseURL + "&Country=" + searchParam + "&IsReminder=false&TemplateType=FA%20-%20Financial%20Assessment";
        
      }
 
      // Following is the ID of inputField that is the lookup to be customized as custom lookup
      if(txtId.indexOf('template') > -1 ){
        isCustomLookup = true;
      }
    }
 
 
    if(isCustomLookup == true){
      openPopup(baseURL, "lookup", 350, 480, "width=640,height=480,toolbar=no,status=no,directories=no,menubar=no,resizable=yes,scrollable=no", true);
    }
    else {
      if (modified == '1') originalbaseURL = originalbaseURL + originalsearchParam;
      openPopup(originalbaseURL, "lookup", 350, 480, "width="+originalwidth+",height=480,toolbar=no,status=no,directories=no,menubar=no,resizable=yes,scrollable=no", true);
    } 
  }
      
     
      </script>
