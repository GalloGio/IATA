    }

    
    function showNewTrainedStaffDialog() {
        $(function() {
            $( '#addNewTrainedStaffModal').modal('show');
            $( '.datePicker').css('position','fixed');
      });
    }


    
   

    function toggleContactDetails(s) {
        if($(s).hasClass("glyphicon-plus")) {
            $(s).switchClass("glyphicon-plus", "glyphicon-minus");
        } else {
            $(s).switchClass("glyphicon-minus", "glyphicon-plus");
        }
        $("." + s.id).toggle();
    }

    function togglePageItem(v, fldId) {
        var el = document.getElementById(fldId)
        if(v=='Yes') {
            el.style.display='block';
        } else {
            el.style.display='none';
        }
    }

    function toggleItemByCheckbox(currId,fldId) {
        if ($('[id$='+currId+']').is(":checked")) {
            $('[id$='+fldId+']').css('display', 'block');
        } else {
            $('[id$='+fldId+']').css('display', 'none');
        }
    }

    function setPortalAdmin(f) {
        var adminid = document.getElementById(f).value;
        $( "#" + adminid ).prop( "checked", true );
