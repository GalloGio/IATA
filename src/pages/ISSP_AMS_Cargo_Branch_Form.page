<apex:form id="newTrainedStaff">
<div class="modal fade" id="addNewTrainedStaffModal" role="dialog" tabindex="-1" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close"  onclick="cancelTrainedStaffUpdate(); return false;" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Add Trained Staff Member</h4>
      </div>
      
      <apex:pageMessages id="trainedStaffMessages" />
       
      <div class="modal-body">
          <apex:pageBlock id="trainedStaffBlock">
              <table>
                  <tr>
                      <td style="width:25%">
                        <table>
                            <tr><td class="modalrow">
                                <apex:outputText value="First Name" /><br/>
                                <apex:inputField value="{!trainedStaffContact.First_name__c}" />
                                </td></tr>
                            <tr><td class="modalrow">
                                <apex:outputText value="Last Name" /><br/>
                                <apex:inputField value="{!trainedStaffContact.Last_name__c}" />
                                </td></tr>
                            <tr><td class="modalrow">
                                <apex:outputText value="Position" /><br/>
                                <apex:inputField required="true" value="{!trainedStaffContact.Position_trained_staff__c}" />
                                </td></tr>
                            <tr><td class="modalrow">
                                <apex:outputLabel value="Experience in air cargo transportation business"/><br/>
                                <apex:outputText value="(a) IATA/FIATA (or equivalent industry) introductory  training courses completed" /><br/>
                                <apex:inputField value="{!trainedStaffContact.IATA_FIATA_training_history__c}" />
                                </td></tr>
                            <tr><td class="modalrow">
                                <apex:outputText value="b) Training course on IATA Dangerous Goods  Regulations successfully completed   " /><br/>
                                    <apex:selectRadio id="validUntilDate" required="true"  style="width:100px;{!IF(trainedStaffContact.Training_dangerous_goods_completed__c==null, 'border:2px red solid;', '')}" value="{!trainedStaffContact.Training_dangerous_goods_completed__c}" onchange="togglePageItem(this.value,'validUntil')" >
                                        <apex:selectOption itemValue="Yes" itemLabel="{!$Label.ISSP_Yes}"/>
                                        <apex:selectOption itemValue="No" itemLabel="{!$Label.ISSP_No}"/>
                                    </apex:selectRadio>
                                </td></tr>
                             
                                
                                <tr><td colspan="1" class="modalrow" id="validUntil" style="{!IF(trainedStaffContact.Training_dangerous_goods_completed__c == 'Yes', 'border: 0px !important;', 'display:none')}">
