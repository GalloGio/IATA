            reRender="trainedstafflist, trainedStaffMessages">
            <apex:param value="Trained Staff" name="trainedStafftype" assignTo="{!contactType}" />
         </apex:commandButton>&nbsp;
        <button type="button" class="btn btn-default" onclick="cancelTrainedStaffUpdate(); $( '.datePicker').css('position','absolute'); return false;" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">{!$Label.ISSP_Cancel}</span></button>
      </div>
    </div>
  </div>
</div>
    
</apex:form>
        
