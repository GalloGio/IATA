	                    	<apex:param value="{!newForm.ISSP_AMS_Date_license_granted__c}"/>
	                    </apex:outputText>
	                
	                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Type_of_License__c.Label}" rendered="{!countryCheckList.License_required__c==true}"/>           
	                <apex:outputText value="{!newForm.ISSP_AMS_Type_of_License__c}" rendered="{!countryCheckList.License_required__c==true}" />
	              </apex:panelGrid>
                   
            	<apex:panelGrid columns="2" width="100%" cellpadding="10%" columnClasses="col1, col2" rendered="{!OR(isTerminatedAgent, isNewHOforAccredAgent)}">
	                <apex:outputText value="{!$ObjectType.Account.fields.Legal_name__c.Label}" />
	                <apex:outputfield value="{!newForm.Account_Name__c}" />
                    <apex:outputText value="{!$ObjectType.Account.fields.TradeName__c.Label}" />
                    <apex:outputfield value="{!newForm.Trade_Name__c}"/>
	                <apex:outputText value="{!$Label.ISSP_AMS_CGO_ReadOnly_LBL2}" />
	                <apex:outputtext value="{0}
	                            <br />{1}<br />{2}" escape="false">
	                            <apex:param value="{!newForm.Branch_Office_Street_name_number__c}" />
	                            <apex:param value="{!newForm.Branch_Office_City__c},&nbsp;{!newForm.Branch_Office_Postal_code__c}" />
	                            <apex:param value="{!IF(newForm.Billing_State__c!='',newForm.Billing_State__c + ',&nbsp;','')}{!isocountry.Name}" />
	                </apex:outputtext>
	                 <apex:outputText value="{!$Label.ISSP_AMS_CGO_ReadOnly_LBL3}" />
	                <apex:outputtext value="{0}
	                            <br />{1}<br />{2}" escape="false">
	                            <apex:param value="{!newForm.Shipping_Street__c}" />
	                            <apex:param value="{!newForm.Shipping_City__c},&nbsp;{!newForm.Shipping_Postal_Code__c}" />
	                            <apex:param value="{!IF(newForm.Shipping_State__c!='',newForm.Shipping_State__c + ',&nbsp;','')}{!isocountry.Name}" />
	                 </apex:outputtext>
	                 <apex:outputText value="{!$ObjectType.Account.fields.Website.Label}" />
	                 <apex:outputfield value="{!newForm.Website__c}" />
	                <apex:outputText value="{!$ObjectType.Account.fields.Phone.Label}" />
	                 <apex:outputfield value="{!newForm.Branch_Office_Phone__c}" />
	                <apex:outputText value="{!$ObjectType.Account.fields.Email__c.Label}" />
	                 <apex:outputfield value="{!newForm.Branch_Office_Email__c}" />
	                <apex:outputText value="{!$ObjectType.Account.fields.Fax.Label}" />
	                 <apex:outputfield value="{!newForm.Branch_Office_FAX__c}" />
	                                   
	                <apex:outputText value="{!IF(newForm.Agent_Type__c!='EACP Intermediary', IF(newlabels, $Label.ISSP_AMS_Cargo_Office_Open_Date_New, $ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Cargo_Office_Open_Date__c.Label),
                                                 'Date office opened as an Intermediary')}" />           
	                <apex:outputText value="{0,date, MMMM d,  yyyy}">
	                    <apex:param value="{!newForm.ISSP_AMS_Cargo_Office_Open_Date__c}"/>
	                </apex:outputText>
	                
	                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Date_license_granted__c.Label}" rendered="{!countryCheckList.License_required__c==true}"/>           
	                    <apex:outputText value="{0,date, MMMM d,  yyyy}" rendered="{!countryCheckList.License_required__c==true}">
	                    	<apex:param value="{!newForm.ISSP_AMS_Date_license_granted__c}"/>
	                    </apex:outputText>
	                
	                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Type_of_License__c.Label}" rendered="{!countryCheckList.License_required__c==true}"/>           
	                <apex:outputText value="{!newForm.ISSP_AMS_Type_of_License__c}" rendered="{!countryCheckList.License_required__c==true}" />
	                
	  			</apex:panelGrid>
	        </apex:pageBlockSection>
	    <div style="page-break-after:always;margin-top:30px;">
	    
	    <!-- ************************* section Local Details (China) ************************* -->
	    			<apex:pageBlockSection columns="1" collapsible="false" rendered="{!isocountry.Local_language_Agency_details_required__c}">
	                    <apex:facet name="header">
	                        <apex:outputpanel >
	                             {!txtLocalAgencyDetailsTitle}
	                            <apex:commandButton styleclass="btn btn-default" action="{!goToStep3Section2}" value="{!$Label.ISSP_EditSection}" id="Edit-Section3" style="float:right"/>
	                        </apex:outputpanel>
	                    </apex:facet>
	                    <apex:outputText value="{!txtLocalAgencyDetailsPleaseEnter}"/>
