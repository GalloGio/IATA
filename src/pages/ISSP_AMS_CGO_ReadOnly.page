	                        </apex:outputpanel>                       
	       		</apex:facet>
	    		<apex:panelGrid columns="2" width="100%" cellpadding="10%" columnClasses="col1, col2" rendered="{!AND(NOT(isTerminatedAgent), NOT(isNewHOforAccredAgent))}">
	                <apex:outputText value="{!IF(isocountry.Local_language_Agency_details_required__c, txtLocalAgencyDetailsLegalNameENG, $ObjectType.Account.fields.Legal_name__c.Label)}" />
	                <apex:outputfield value="{!hqAccount.Name}" />
	                <apex:outputText value="{!$Label.ISSP_AMS_CGO_ReadOnly_LBL2}" />
	                <apex:outputtext value="{0}
	                            <br />{1}<br />{2}" escape="false">
	                            <apex:param value="{!hqAccount.BillingStreet}" />
	                            <apex:param value="{!hqAccount.BillingCity},&nbsp;{!hqAccount.BillingPostalCode}" />
	                            <apex:param value="{!IF(hqAccount.BillingState!='',hqAccount.BillingState + ',&nbsp;','')}{!hqAccount.BillingCountry}" />
	                </apex:outputtext>
