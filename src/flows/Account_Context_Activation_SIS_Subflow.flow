                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Wait_for_SIS_case_to_be_closed</targetReference>
            </connector>
            <label>case already created</label>
        </rules>
    </decisions>
    <description>Search for a completed sis case, else create one if it doesn&apos;t exist and notify account creator.</description>
    <formulas>
        <name>EmailBody</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(SUBSTITUTE(SUBSTITUTE({!$Label.SisProcessEmailBody}, 
&quot;{0}&quot;, {!UserFlowLauncher.Name}),
&quot;{1}&quot;, {!SisCase.CaseNumber}),
&quot;{2}&quot;, {!Account.Name})</expression>
    </formulas>
    <formulas>
        <name>EmailSubject</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!$Label.SisProcessEmailSubject}, &quot;{0}&quot;, {!Account.Name})</expression>
    </formulas>
    <formulas>
        <name>SisCaseSubject</name>
        <dataType>String</dataType>
        <expression>{!$Label.SisProcessCaseSubject}</expression>
    </formulas>
