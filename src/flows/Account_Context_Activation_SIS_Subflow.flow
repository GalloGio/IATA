    <decisions>
        <name>Account_has_SIS_ID</name>
        <label>Account has SIS ID ?</label>
        <locationX>204</locationX>
        <locationY>248</locationY>
        <defaultConnector>
            <targetReference>Get_SIS_Case</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>SIS KO</defaultConnectorLabel>
        <rules>
            <name>SIS_fields_filled</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Account.SIS_ID__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Account.EF_SIS_Code__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Account.EF_SIS_Designator__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Output_Success</targetReference>
            </connector>
            <label>SIS fields filled</label>
        </rules>
    </decisions>
