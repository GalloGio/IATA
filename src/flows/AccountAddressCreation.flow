    <description>launch a flow to create an address record based on account business address</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(

[Account].RecordTypeDeveloperName__c!=&quot;IATA_Agency&quot;, 

OR( 
  AND(
    NOT(ISBLANK([Account].Business_City_Name__c)),
    OR(ISNEW(),ISCHANGED([Account].Business_City_Name__c))
  ),
  AND(
    NOT(ISBLANK([Account].Business_Street__c)),
    OR(ISNEW(),ISCHANGED([Account].Business_Street__c))
  ),
  AND(
    NOT(ISBLANK([Account].Business_City_Name__c)),
    OR(ISNEW(),ISCHANGED([Account].Business_City_Name__c))
  ),
  AND(
    NOT(ISBLANK([Account].Business_City_Name__c)),
    OR(ISNEW(),ISCHANGED([Account].Business_City_Name__c))
  ),
  AND(
    NOT(ISBLANK([Account].Business_State_Name__c)),
    OR(ISNEW(),ISCHANGED([Account].Business_State_Name__c))
  ),
  AND(
    NOT(ISBLANK([Account].Business_Postal_Code__c)),
    OR(ISNEW(),ISCHANGED([Account].Business_Postal_Code__c))
  ),
  AND(
    NOT(ISBLANK([Account].RecordTypeId )),
    ISCHANGED([Account].RecordTypeId ),
	NOT(ISBLANK([Account].IATA_ISO_Country__c)),
	NOT(ISBLANK([Account].Business_State_Name__c)),
	NOT(ISBLANK([Account].Business_City_Name__c)),
	NOT(ISBLANK([Account].Business_Postal_Code__c)),
	NOT(ISBLANK([Account].Business_Street__c))
  ),
  AND(
    NOT(ISBLANK([Account].Business_Geo_Coordinates__Latitude__s)),
    OR(ISNEW(),ISCHANGED([Account].Business_Geo_Coordinates__Latitude__s))
  ),
  AND(
    NOT(ISBLANK([Account].Business_Geo_Coordinates__Longitude__s)),
    OR(ISNEW(),ISCHANGED([Account].Business_Geo_Coordinates__Longitude__s))
  ),
  AND(
    NOT(ISBLANK([Account].Business_PO_Box__c)),
    OR(ISNEW(),ISCHANGED([Account].Business_PO_Box__c))
  )
)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(

