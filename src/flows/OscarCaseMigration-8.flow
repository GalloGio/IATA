        <defaultConnector>
            <targetReference>Provide_Concerned_Agency_Codes</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Selected Branches</defaultConnectorLabel>
        <rules>
            <name>None_All_Branches</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Scope_of_Change</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>HO_All_Branches</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Scope_of_Change</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>HO_Only</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>OscarProcessTypeAssignment</targetReference>
            </connector>
            <label>None/All Branches</label>
        </rules>
    </decisions>
