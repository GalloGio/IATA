    <decisions>
        <name>Is_Change_of_Bank_Details</name>
        <label>Is Change of Bank Details ?</label>
        <locationX>140</locationX>
        <locationY>126</locationY>
        <defaultConnector>
            <targetReference>OscarProcessTypeAssignment</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>All Other Changes</defaultConnectorLabel>
        <rules>
            <name>Change_of_Bank_Details</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Please_choose_the_process_to_convert</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>Change_of_Bank_Account_Details</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Type_of_Bank_Details_Change</targetReference>
            </connector>
            <label>Change of Bank Details</label>
        </rules>
    </decisions>
    <decisions>
        <name>MigrationResultDecision</name>
        <label>MigrationResultDecision</label>
        <locationX>134</locationX>
        <locationY>540</locationY>
        <defaultConnector>
            <targetReference>ResultPage_0</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>MigrationIsOk</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>errorMessage</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ResultPage</targetReference>
            </connector>
            <label>MigrationIsOk</label>
        </rules>
    </decisions>
    <decisions>
        <name>What_is_the_Scope_of_Change</name>
