            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Process_Reconsideration</name>
        <label>Process Reconsideration?</label>
        <locationX>687</locationX>
        <locationY>553</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes2</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>oscar.Disapproval_date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Reconsideration_date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Disapproval_date__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>oscar.Bank_Guarantee_requested__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Bank_Guarantee_requested__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Subtract_Reconsideration_Time</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <formulas>
        <name>AgreementTime</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!oscar.Agreement_requested__c}),
	0,
	IF(ISNULL({!oscar.PSAA_EACP_Agreement_received__c}),
		TODAY(),
		{!oscar.PSAA_EACP_Agreement_received__c}
	) - {!oscar.Agreement_requested__c}
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>BankGuaranteeTime</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!oscar.Bank_Guarantee_requested__c}),
	0,
	IF(ISNULL({!oscar.Bank_Guarantee_received__c}),
		TODAY(),
		{!oscar.Bank_Guarantee_received__c}
	) - {!oscar.Bank_Guarantee_requested__c}
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>BankGuaranteetOrAgreementTime</name>
        <dataType>Number</dataType>
        <expression>IF({!BankGuaranteeTime} &gt; {!AgreementTime}, {!BankGuaranteeTime}, {!AgreementTime})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>PaymentTime</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!oscar.Proof_of_payment_received__c}),
	TODAY(),
	{!oscar.Proof_of_payment_received__c}
) - {!oscar.Payment_requested__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ReconsiderationTime</name>
        <dataType>Number</dataType>
        <expression>{!oscar.Reconsideration_date__c} - {!oscar.Disapproval_date__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>StartDate</name>
        <dataType>Date</dataType>
        <expression>IF(ISNULL({!oscar.Change_effective_as_of__c}),
	{!oscar.Dossier_Reception_Date__c},
	IF({!oscar.Dossier_Reception_Date__c} &gt; {!oscar.Change_effective_as_of__c},
		{!oscar.Dossier_Reception_Date__c},
		{!oscar.Change_effective_as_of__c}
	)
)</expression>
    </formulas>
    <formulas>
        <name>TotalTime</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!case_record.ClosedDate}),
	TODAY(),
	DATEVALUE({!case_record.ClosedDate})
) - {!StartDate}</expression>
        <scale>2</scale>
    </formulas>
    <interviewLabel>AMS OSCAR Effective Age Calculator {!$Flow.CurrentDateTime}</interviewLabel>
