        <label>Subtract Reconsideration Time</label>
        <locationX>1045</locationX>
        <locationY>551</locationY>
        <assignmentItems>
            <assignToReference>EffectiveAge</assignToReference>
            <operator>Subtract</operator>
            <value>
                <elementReference>ReconsiderationTime</elementReference>
            </value>
        </assignmentItems>
    </assignments>
    <decisions>
        <name>Bank_Guarantee_or_Agreement_Requested</name>
        <label>Bank Guarantee or Agreement Requested?</label>
        <locationX>730</locationX>
        <locationY>296</locationY>
        <defaultConnector>
            <targetReference>Process_Reconsideration</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes1</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>oscar.Bank_Guarantee_requested__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Agreement_requested__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Dossier_Reception_Date__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>BankGuaranteeOrAgreementDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Subtract_Bank_Guarantee_Or_Agreement_Time</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_CLO_Closure</name>
        <label>Is CLO - Closure?</label>
        <locationX>86</locationX>
        <locationY>266</locationY>
        <defaultConnector>
            <targetReference>Set_Total_Time</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>oscar.Case_Reason__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>CLO - Closure</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Was_Rejected_Not_Accepted</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Payment_Requested</name>
        <label>Payment Requested?</label>
        <locationX>730</locationX>
        <locationY>52</locationY>
        <defaultConnector>
            <targetReference>Bank_Guarantee_or_Agreement_Requested</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes0</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>oscar.Payment_requested__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Dossier_Reception_Date__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>oscar.Payment_requested__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Subtract_Payment_Time</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Process_Reconsideration</name>
        <label>Process Reconsideration?</label>
        <locationX>730</locationX>
        <locationY>553</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes2</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>oscar.Disapproval_date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Reconsideration_date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Dossier_Reception_Date__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>oscar.Reconsideration_date__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Bank_Guarantee_requested__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Disapproval_date__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>oscar.Bank_Guarantee_requested__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Subtract_Reconsideration_Time</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Was_Rejected_Not_Accepted</name>
        <label>Was Rejected/Not Accepted?</label>
        <locationX>88</locationX>
        <locationY>496</locationY>
        <defaultConnector>
            <targetReference>Calculate_Eff_Age</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes3</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>oscar.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed_ Not Accepted</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>oscar.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed_Rejected</stringValue>
                </rightValue>
            </conditions>
            <label>Yes</label>
        </rules>
    </decisions>
    <formulas>
        <name>AgreementTime</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!oscar.Agreement_requested__c}),
	0,
	IF(ISNULL({!oscar.PSAA_EACP_Agreement_received__c}),
		TODAY(),
		{!oscar.PSAA_EACP_Agreement_received__c}
	) - {!oscar.Agreement_requested__c}
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>BankGuaranteeOrAgreementDate</name>
        <dataType>Date</dataType>
        <expression>IF({!oscar.Bank_Guarantee_requested__c} &gt; {!oscar.Agreement_requested__c}, {!oscar.Bank_Guarantee_requested__c} , {!oscar.Agreement_requested__c})</expression>
    </formulas>
    <formulas>
        <name>BankGuaranteeTime</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!oscar.Bank_Guarantee_requested__c}),
	0,
	IF(ISNULL({!oscar.Bank_Guarantee_received__c}),
		TODAY(),
		{!oscar.Bank_Guarantee_received__c}
	) - {!oscar.Bank_Guarantee_requested__c}
)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>BankGuaranteetOrAgreementTime</name>
        <dataType>Number</dataType>
        <expression>IF({!BankGuaranteeTime} &gt; {!AgreementTime}, {!BankGuaranteeTime}, {!AgreementTime})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CLO_Age</name>
        <dataType>Number</dataType>
        <expression>IF({!oscar.Form_Effective_Date__c} == {!oscar.Suspended_in_BSPLink_CASSLink__c}, 1, 2)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>PaymentTime</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!oscar.Proof_of_payment_received__c}),
	TODAY(),
	{!oscar.Proof_of_payment_received__c}
) - {!oscar.Payment_requested__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ReconsiderationTime</name>
        <dataType>Number</dataType>
        <expression>IF(
    ISNULL({!oscar.Reconsideration_date__c}),
    TODAY(),
    {!oscar.Reconsideration_date__c}
)  - {!oscar.Disapproval_date__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>StartDate</name>
        <dataType>Date</dataType>
        <expression>IF(OR(ISPICKVAL({!oscar.Process__c}, &apos;ANG.MAJOR.CHANGES.CHINA.1.0&apos;),
            ISPICKVAL({!oscar.Process__c}, &apos;ANG.MAJOR.CHANGES.1.0&apos;),
            ISPICKVAL({!oscar.Process__c}, &apos;MAJOR.CHANGES&apos;),
            ISPICKVAL({!oscar.Process__c}, &apos;CHANGE.CHINESE.1.1&apos;),
            ISPICKVAL({!oscar.Process__c}, &apos;CHANGE.1.0&apos;)),
            IF(ISNULL({!oscar.Change_effective_as_of__c}),
                {!oscar.Dossier_Reception_Date__c},
                IF({!oscar.Dossier_Reception_Date__c} &gt; {!oscar.Change_effective_as_of__c},
                    {!oscar.Dossier_Reception_Date__c},
                    {!oscar.Change_effective_as_of__c}
                )
            ),
            {!oscar.Dossier_Reception_Date__c}
        )</expression>
    </formulas>
    <formulas>
        <name>TotalTime</name>
        <dataType>Number</dataType>
        <expression>IF(ISNULL({!case_record.ClosedDate}),
	TODAY(),
	DATEVALUE({!case_record.ClosedDate})
) - {!StartDate}</expression>
        <scale>2</scale>
    </formulas>
    <interviewLabel>AMS OSCAR Effective Age Calculator {!$Flow.CurrentDateTime}</interviewLabel>
    <label>AMS OSCAR Effective Age Calculator</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>50</locationX>
        <locationY>50</locationY>
        <connector>
            <targetReference>Is_CLO_Closure</targetReference>
        </connector>
    </start>
    <status>Active</status>
    <variables>
        <name>case_record</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
        <objectType>Case</objectType>
    </variables>
