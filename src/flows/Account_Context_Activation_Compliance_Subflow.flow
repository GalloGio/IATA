            </value>
        </filters>
        <filters>
            <field>CaseArea__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Corporate Compliance</stringValue>
            </value>
        </filters>
        <filters>
            <field>IsClosed</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Reason1__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>New E&amp;F participant</stringValue>
            </value>
        </filters>
        <filters>
            <field>RecordTypeDeveloperName__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>RECORDTYPE_CASE_INTERNAL</elementReference>
            </value>
        </filters>
        <filters>
            <field>Region__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>Account.Region_formula__c</elementReference>
            </value>
        </filters>
        <object>Case</object>
        <outputReference>ComplianceCase</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>CaseNumber</queriedFields>
    </recordLookups>
    <recordLookups>
        <name>Get_Due_Diligence_Queue</name>
        <label>Get Due Diligence Queue</label>
        <locationX>1080</locationX>
        <locationY>256</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
