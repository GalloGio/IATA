            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ComplianceCase.BSPCountry__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Account.IATA_Iso_Country_Name__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ComplianceCase.AccountId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Account.Id</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ComplianceCase.OwnerId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>DueDiligenceQueue.Id</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ComplianceCase.CreatedById</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>UserFlowLauncher.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Insert_Compliant_Case</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Wait_for_compliance_case_to_be_closed</name>
        <label>Wait for compliance case to be closed</label>
        <locationX>740</locationX>
        <locationY>485</locationY>
        <assignmentItems>
            <assignToReference>WaitForNextAction</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </assignmentItems>
    </assignments>
    <constants>
        <description>DeveloperName of the queue Due Diligence</description>
        <name>QUEUE_DEVELOPERNAME_DUE_DILIGENCE</name>
        <dataType>String</dataType>
        <value>
            <stringValue>Due_Diligence</stringValue>
        </value>
    </constants>
    <constants>
        <description>Case RecordType DeveloperName for Internal Case</description>
        <name>RECORDTYPE_CASE_INTERNAL</name>
        <dataType>String</dataType>
        <value>
            <stringValue>InternalCasesEuropeSCE</stringValue>
        </value>
    </constants>
    <decisions>
        <name>has_been_relaunched</name>
        <label>has been relaunched ?</label>
        <locationX>544</locationX>
        <locationY>264</locationY>
        <defaultConnector>
            <targetReference>Is_Case_Created</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>first execution</defaultConnectorLabel>
        <rules>
            <name>second_execution</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FlowHasBeenRelaunched</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Notify_Flow_Launcher</targetReference>
            </connector>
            <label>second execution</label>
        </rules>
    </decisions>
    <decisions>
        <description>Account is currently compliant are:
Due Diligence Status (Due_Diligence_Status__c) = “Cleared”, AND
Date of last clearance (Date_of_last_clearance__c) within the last 12 months</description>
        <name>Is_Account_compliant</name>
        <label>Is Account compliant ?</label>
        <locationX>180</locationX>
        <locationY>264</locationY>
        <defaultConnector>
            <targetReference>Get_Compliance_Case</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>not compliant</defaultConnectorLabel>
        <rules>
            <name>is_compliant</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Flow.CurrentDate</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>MaxLastClearanceDate</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Account.Due_Diligence_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Cleared</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Output_Success</targetReference>
            </connector>
            <label>is compliant</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Case_Created</name>
        <label>Is Case Created ?</label>
        <locationX>732</locationX>
        <locationY>262</locationY>
        <defaultConnector>
            <targetReference>Get_Case_RT_Internal_Case</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>not created</defaultConnectorLabel>
        <rules>
            <name>case_already_created</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ComplianceCase</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Wait_for_compliance_case_to_be_closed</targetReference>
            </connector>
            <label>case already created</label>
        </rules>
    </decisions>
    <description>Search for a completed compliant case, else create one if it doesn&apos;t exist and notify account creator.</description>
    <formulas>
        <name>CaseCompliantSubject</name>
        <dataType>String</dataType>
        <expression>{!$Label.EFComplianceReview} &amp; &apos; - &apos; &amp; {!Account.Name}</expression>
    </formulas>
    <formulas>
        <name>EmailBody</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE(SUBSTITUTE(SUBSTITUTE({!$Label.ComplianceProcessEmailBody}, 
&quot;{0}&quot;, {!UserFlowLauncher.Name}),
&quot;{1}&quot;, {!ComplianceCase.CaseNumber}),
&quot;{2}&quot;, {!Account.Name})</expression>
    </formulas>
    <formulas>
        <name>EmailSubject</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!$Label.ComplianceProcessEmailSubject}, &quot;{0}&quot;, {!Account.Name})</expression>
    </formulas>
    <formulas>
        <name>MaxLastClearanceDate</name>
        <dataType>Date</dataType>
        <expression>ADDMONTHS({!Account.Date_of_last_clearance__c}, 12)</expression>
    </formulas>
    <interviewLabel>Account Context Activation Compliance Subflow {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Account Context Activation Compliance Subflow</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
        <name>Insert_Compliant_Case</name>
        <label>Insert Compliant Case</label>
        <locationX>925</locationX>
        <locationY>485</locationY>
        <connector>
            <targetReference>Wait_for_compliance_case_to_be_closed</targetReference>
        </connector>
        <inputReference>ComplianceCase</inputReference>
    </recordCreates>
    <recordLookups>
        <name>Get_Case_RT_Internal_Case</name>
        <label>Get Case RT Internal Case</label>
        <locationX>922</locationX>
        <locationY>256</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Get_Due_Diligence_Queue</targetReference>
        </connector>
        <filters>
            <field>DeveloperName</field>
            <operator>EqualTo</operator>
            <value>
