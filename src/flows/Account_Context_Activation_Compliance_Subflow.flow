            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ComplianceCase.CaseArea__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Corporate Compliance</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ComplianceCase.Reason1__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>SIS Client</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ComplianceCase.Subject</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>CaseCompliantSubject</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>ComplianceCase.Region__c</assignToReference>
            <operator>Assign</operator>
