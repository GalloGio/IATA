        </value>
    </constants>
    <decisions>
        <name>Operation</name>
        <label>Operation</label>
        <locationX>590</locationX>
        <locationY>134</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Create</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UserProvisioningRequest.Operation</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Create</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SCIM_Create_User_Generator</targetReference>
            </connector>
            <label>Create</label>
        </rules>
        <rules>
            <name>Update</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UserProvisioningRequest.Operation</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Update</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SCIM_Update_User_Generator</targetReference>
            </connector>
            <label>Update</label>
        </rules>
        <rules>
            <name>Collect</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UserProvisioningRequest.Operation</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Reconcile</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SCIM_Collect_Users</targetReference>
            </connector>
            <label>Collect</label>
        </rules>
        <rules>
            <name>Toggle</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>UserProvisioningRequest.Operation</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Activate</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserProvisioningRequest.Operation</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Freeze</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserProvisioningRequest.Operation</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Unfreeze</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SCIM_Toggle_User</targetReference>
            </connector>
            <label>Toggle</label>
        </rules>
    </decisions>
    <formulas>
        <name>CallParams</name>
        <dataType>String</dataType>
        <expression>&apos;{&quot;externalSystems&quot; : &quot;Global Default Insurance&quot;}&apos;</expression>
    </formulas>
    <formulas>
        <description>Formula to determine if a UserProvisioningRequest indicates enabling a user.</description>
        <name>EnableUser</name>
        <dataType>Boolean</dataType>
        <expression>TEXT({!UserProvisioningRequest.Operation}) == &apos;Activate&apos; || TEXT({!UserProvisioningRequest.Operation}) == &apos;Unfreeze&apos;</expression>
    </formulas>
    <formulas>
        <name>EntitlementFormula</name>
        <dataType>String</dataType>
        <expression>&apos;{&apos;+
&apos;&quot;language&quot;:&quot;&apos; + UPPER(LEFT(TEXT({!UserFields.LanguageLocaleKey}), 2)) + &apos;&quot;,&apos; +
&apos;&quot;iataCode&quot;:&quot;&apos; + {!UserFields.Contact_Account_IATA_Code__c} + &apos;&quot;,&apos; +
&apos;&quot;bspCountry&quot;:&quot;&apos; + {!UserFields.Contact.Account.Country_ISO_Code__c} + &apos;&quot;,&apos; +
&apos;&quot;externalSystems&quot;:&quot;&apos; + &quot;Global Default Insurance (GDI)&quot; + &apos;&quot;,&apos; +
&apos;&quot;accountId&quot;:&quot;&apos; + {!UserFields.Contact.AccountId} + &apos;&quot;,&apos; +
&apos;&quot;phone&quot;:&quot;&apos; + {!UserFields.Contact.Phone} + &apos;&quot;,&apos; +
&apos;&quot;ifapContact&quot;:&quot;&apos; + IF({!UserFields.Contact.Financial_Assessment_Contact__c}, &apos;true&apos;, &apos;false&apos;) + &apos;&quot;&apos; +
&apos;}&apos;</expression>
    </formulas>
    <formulas>
        <name>PortalName</name>
        <dataType>String</dataType>
        <expression>{!UserFields.Contact.Account.IATA_ISO_Country__r.ANG_Portal_Service__r.Name}</expression>
    </formulas>
    <formulas>
        <name>ProjectFormula</name>
        <dataType>String</dataType>
        <expression>&apos;GDI&apos;</expression>
    </formulas>
    <formulas>
        <name>UserNameFormula</name>
        <dataType>String</dataType>
        <expression>{!User.Username}</expression>
    </formulas>
