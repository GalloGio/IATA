            <assignedTo>Cases_Emergency_BJS_GDC</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>AND(
  OR(
  ISPICKVAL(Priority , &quot;Emergency&quot;),
AND(
OR(
CONTAINS(Subject,&quot;Emergency&quot;),
CONTAINS(Subject,&quot;emergency&quot;),
CONTAINS(Subject,&quot;EMERGENCY&quot;),
CONTAINS(Subject,&quot;Very important&quot;),
CONTAINS(Subject,&quot;Very Important&quot;),
CONTAINS(Subject,&quot;VERY IMPORTANT&quot;),
CONTAINS(Subject,&quot;very important&quot;),
CONTAINS(Subject,&quot;IMPORTANT&quot;),
CONTAINS(Subject,&quot;Critical&quot;),
CONTAINS(Subject,&quot;critical&quot;),
CONTAINS(Subject,&quot;CRITICAL&quot;)
),
 ISPICKVAL(Origin,&quot;Portal&quot;)
),
  AND(
  ISPICKVAL(Origin,&quot;Escalation&quot;),
  OR(
  Key_Customer_Case__c &lt;&gt; &quot; &quot;,
  ISPICKVAL(Priority,&quot;High&quot;)
  )
  )
  ),
  OR(RecordType.DeveloperName = &quot;Cases_China_North_Asia&quot;,
  AND(RecordType.DeveloperName =&quot;Cases_Global&quot;,ISPICKVAL(Region__c,&quot;China &amp; North Asia&quot;))),
  TIMEVALUE(CreatedDate)&gt;=TIMEVALUE(&quot;00:00:00.000&quot;),
  TIMEVALUE(CreatedDate)&lt;TIMEVALUE(&quot;09:00:00.000&quot;)
  )</formula>
        </ruleEntry>
