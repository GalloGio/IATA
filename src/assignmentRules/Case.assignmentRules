AND( 
ISPICKVAL(Country_concerned_by_the_query__c , &quot;Russian Federation&quot;), 
ISPICKVAL(Account.Location_Class__c, &quot;P&quot;), 
NOT(CONTAINS( TEXT(Reason) ,&quot;Certificate&quot;)),
!(RecordType.DeveloperName=&apos;ID_Card_Application&apos;),
ISPICKVAL( Status , &quot;Open&quot;),
NOT(CONTAINS(Subject ,&quot;IEP Model&quot;)),
NOT(CONTAINS(TEXT(CaseArea__c),&quot;Corporate Compliance&quot;))
),
AND(
RecordType.DeveloperName = &apos;OSCAR_Communication&apos;,
ISPICKVAL( Origin,&quot;PCI automatic renewal&quot;),
ISPICKVAL(BSPCountry__c,&quot;Russian Federation&quot;)
)
)</formula>
