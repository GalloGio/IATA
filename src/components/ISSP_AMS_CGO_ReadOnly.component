<apex:component controller="ISSP_AMS_Accreditation">
    <apex:pageBlockSection columns="1" title="{!$Label.ISSP_AMS_First_Page_Summary_Header}" collapsible="false">
    			<apex:panelGrid columns="2" width="100%" cellpadding="10%" columnClasses="col1, col2">
                    	<apex:outputText value="{!$Label.ISSP_AMS_Step1_Operation_Type}"/>
                        <apex:outputText value="{!$Label.ISSP_Cargo_Agent}"/>
                    	<apex:outputText value="{!$Label.ISSP_AMS_Step1_Approval_Sought}" />
                    	<apex:outputText value="{!newForm.Agent_Type__c + ' - ' + IF(newCase.Reason1__c=='New HO', $Label.ISSP_AMS_Reason_New_HO, $Label.ISSP_AMS_Reason_New_Branch)}"/>
                    	<apex:outputText value="{!$Label.ISSP_CountryConcerned}"/> 
                    	<apex:outputText value="{!amsAllLangCountryLabel}"/> 
    			</apex:panelGrid>
          </apex:pageBlockSection>
    
    <div style="page-break-after:always;margin-top:30px;">
          <apex:pageBlockSection columns="1" collapsible="false">
              <apex:facet name="header">
                        <apex:outputpanel >
                            {!CASE(amsOfficeType,'HO', $Label.ISSP_AMS_STEP_3_1_HO_Title, $Label.ISSP_AMS_STEP_3_1_Branch_Title)}
                            <apex:commandButton styleclass="btn btn-default" action="{!goToStep3Section1}" value="{!$Label.ISSP_EditSection}" id="Edit-Section1" style="float:right"/>
                        </apex:outputpanel>                       
                    </apex:facet>
    			<apex:panelGrid columns="2" width="100%" cellpadding="10%" columnClasses="col1, col2" rowClasses="cgo-readonly">
                    
                <apex:outputText value="* Legal Status "/>
                <apex:outPutText value="{!newForm.ISSP_AMS_Legal_Status__c}"/>
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Date_legal_establishment__c.Label}"/>
                <apex:outputText value="{0,date, MMMM d,  yyyy}">
                    <apex:param value="{!newForm.ISSP_AMS_Date_legal_establishment__c}" />
                </apex:outputText>           
              	<apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Date_of_incorporation__c.Label}" rendered="{!newForm.ISSP_AMS_Legal_Status__c == 'Corporation'}"/>
                <apex:outputText value="{0,date, MMMM d,  yyyy}" rendered="{!newForm.ISSP_AMS_Legal_Status__c == 'Corporation'}">
                	<apex:param value="{!newForm.ISSP_AMS_Date_of_incorporation__c}" />    
                </apex:outputText>
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Nominee_Trustee__c.Label}" rendered="{!newForm.ISSP_AMS_Legal_Status__c == 'Corporation'}"/>
                <apex:outputText value="{!newForm.ISSP_AMS_Nominee_Trustee__c}" rendered="{!newForm.ISSP_AMS_Legal_Status__c == 'Corporation'}"/>
                    
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Date_of_Association__c.Label}" rendered="{!newForm.ISSP_AMS_Legal_Status__c == 'Association'}"/>           
                <apex:outputText value="{!newForm.ISSP_AMS_Date_of_Association__c}" rendered="{!newForm.ISSP_AMS_Legal_Status__c == 'Association'}"/>
              	
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_For_whom_Nominee__c.Label}" rendered="{!AND(AND(newForm.ISSP_AMS_Nominee_Trustee__c != '', newForm.ISSP_AMS_Nominee_Trustee__c == 'Yes'),newForm.ISSP_AMS_Legal_Status__c == 'Corporation')}"/>           
                <apex:outputText value="{!newForm.ISSP_AMS_For_whom_Nominee__c}" rendered="{!AND(AND(newForm.ISSP_AMS_Nominee_Trustee__c != '', newForm.ISSP_AMS_Nominee_Trustee__c == 'Yes'),newForm.ISSP_AMS_Legal_Status__c == 'Corporation')}"/>
                
                <apex:outputText value="* Give details of the parent business activities of your firm (customs house broker, ocean freight forwarder, travel agent, etc.)"/>
                <apex:outputText value="{!newForm.ISSP_AMS_Business_Activities__c}" />
                
                <apex:outputText value="* Is the applicant a parent, subsidiary, affiliate, owner, officer, director,  employee, partner,  or stockholder of any other business concern(s)? "/>
                <apex:outputText value="{!newForm.ISSP_AMS_Is_Applicant_Parent__c}" />
                
                 <apex:outputText value="* State the name(s) address(es) and nature of the business activities involved." rendered="{!newForm.ISSP_AMS_Is_Applicant_Parent__c == 'Yes'}"/>
                 <apex:outputText value="{!newForm.ISSP_AMS_State_Names_Addresses__c}" rendered="{!newForm.ISSP_AMS_Is_Applicant_Parent__c == 'Yes'}"/>
                    
                 <apex:outputText value="* Number of years for which you have successfully traded?" />
                 <apex:outputText value="{!newForm.ISSP_AMS_Number_of_Years_Traded__c}" />
                    
                 <apex:outputText value="Is your firm authorized to make customs declarations?"/>
                 <apex:outputText value="{!newForm.ISSP_AMS_Is_Make_Declarations__c}" />
                    
                </apex:panelGrid>
              </apex:pageBlockSection>
    </div>
    <apex:pageBlockSection columns="1" collapsible="false">
        	<apex:facet name="header">
                        <apex:outputpanel >
                            {!$Label.ISSP_AMS_STEP_3_2_Title}
                            <apex:commandButton styleclass="btn btn-default" action="{!goToStep3Section2}" value="{!$Label.ISSP_EditSection}" id="Edit-Section2" style="float:right"/>
                        </apex:outputpanel>                       
       		</apex:facet>
    		<apex:panelGrid columns="2" width="100%" cellpadding="10%" columnClasses="col1, col2">
            
                <apex:outputText value="{!$ObjectType.Account.fields.Legal_name__c.Label}" />
                <apex:outputfield value="{!hqAccount.Name}" />
                <apex:outputText value="Full billing Address of the Office for which Application for Approval is made" />
                <apex:outputtext value="{0}
                            <br />{1}<br />{2}" escape="false">
                            <apex:param value="{!hqAccount.BillingStreet}" />
                            <apex:param value="{!hqAccount.BillingCity},&nbsp;{!hqAccount.BillingPostalCode}" />
                            <apex:param value="{!IF(hqAccount.BillingState!='',hqAccount.BillingState + ',&nbsp;','')}{!hqAccount.BillingCountry}" />
                </apex:outputtext>
                 <apex:outputText value="Full shipping Address of the Office for which Application for Approval is made" />
                <apex:outputtext value="{0}
                            <br />{1}<br />{2}" escape="false">
                            <apex:param value="{!hqAccount.ShippingStreet}" />
                            <apex:param value="{!hqAccount.ShippingCity},&nbsp;{!hqAccount.ShippingPostalCode}" />
                            <apex:param value="{!IF(hqAccount.ShippingState!='',hqAccount.ShippingState + ',&nbsp;','')}{!hqAccount.ShippingCountry}" />
                 </apex:outputtext>
                 <apex:outputText value="{!$ObjectType.Account.fields.Website.Label}" />
                 <apex:outputfield value="{!hqAccount.Website}" />
                <apex:outputText value="{!$ObjectType.Account.fields.Phone.Label}" />
                 <apex:outputfield value="{!hqAccount.Phone}" />
                <apex:outputText value="{!$ObjectType.Account.fields.Email__c.Label}" />
                 <apex:outputfield value="{!hqAccount.Email__c}" />
                <apex:outputText value="{!$ObjectType.Account.fields.Mobile__c.Label}" />
                 <apex:outputfield value="{!hqAccount.Mobile__c}" />
                <apex:outputText value="{!$ObjectType.Account.fields.Fax.Label}" />
                 <apex:outputfield value="{!hqAccount.Fax}" />
                
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Cargo_Office_Open_Date__c.Label}" />           
                <apex:outputText value="{0,date, MMMM d,  yyyy}">
                    <apex:param value="{!newForm.ISSP_AMS_Cargo_Office_Open_Date__c}"/>
                </apex:outputText>
                
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Date_license_granted__c.Label}" rendered="{!countryCheckList.License_required__c==true}"/>           
                    <apex:outputText value="{0,date, MMMM d,  yyyy}" rendered="{!countryCheckList.License_required__c==true}">
                    	<apex:param value="{!newForm.ISSP_AMS_Date_license_granted__c}"/>
                    </apex:outputText>
                
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.ISSP_AMS_Type_of_License__c.Label}" rendered="{!countryCheckList.License_required__c==true}"/>           
                <apex:outputText value="{!newForm.ISSP_AMS_Type_of_License__c}" rendered="{!countryCheckList.License_required__c==true}" />
                
  			</apex:panelGrid>
        </apex:pageBlockSection>
    <div style="page-break-after:always;margin-top:30px;">
    
    <!-- ************************* section Local Details (China) ************************* -->
    			<apex:pageBlockSection columns="1" collapsible="false" rendered="{!isocountry.ISO_Code__c=='CN'}">
                    <apex:facet name="header">
                        <apex:outputpanel >
                            Agency Details in Local language
                            <apex:commandButton styleclass="btn btn-default" action="{!goToStep3Section2}" value="{!$Label.ISSP_EditSection}" id="Edit-Section3" style="float:right"/>
                        </apex:outputpanel>
                    </apex:facet>
                    <apex:outputText value="Please enter the Legal Name and Physical & Correspondence address of the Agency to be accredited in your local language."/>
                    <apex:panelGrid columns="4" width="100%" cellpadding="10%" columnClasses="col1_regular, col2_regular">
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Legally_Registered_Name__c.Label}"/>
                       	<apex:outputField value="{!newForm.Local_Legally_Registered_Name__c}" />
                        <apex:outputtext ><!--empty: move following field to next row -->&nbsp;</apex:outputtext>
                    	<apex:outputtext ><!--empty: move following field to next row -->&nbsp;</apex:outputtext>
                        
                        <apex:outputText value="Physical Address" style="font-weight:bold; font-size:110%"/>
                        <apex:outputtext ><!--empty: move following field to next row -->&nbsp;</apex:outputtext>
                    	<apex:outputText value="Correspondence Address" style="font-weight:bold; font-size:110%"/>
                        <apex:outputtext ><!--empty: move following field to next row -->&nbsp;</apex:outputtext>
                        
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Physical_Address_Street__c.Label}"/>
                       	<apex:outputField value="{!newForm.Local_Physical_Address_Street__c}" />
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Postal_Address_Street__c.Label}"/>
                       	<apex:outputField value="{!newForm.Local_Postal_Address_Street__c}" styleClass="PostalStreet"/>
                      	
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Physical_Address_City__c.Label}"/>
                       	<apex:outputField value="{!newForm.Local_Physical_Address_City__c}" />
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Postal_Address_City__c.Label}"/>
                      	<apex:outputField value="{!newForm.Local_Postal_Address_City__c}"/>
                        
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Physical_Address_Postal_Code__c.Label}"/>
                       <apex:outputField value="{!newForm.Local_Physical_Address_Postal_Code__c}" />
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Postal_Address_Postal_Code__c.Label}"/>
                        <apex:outputField value="{!newForm.Local_Postal_Address_Postal_Code__c}" styleClass="PostalPostalCode"/>
                        
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Physical_Address_Province__c.Label}"/>
                       	<apex:outputField value="{!newForm.Local_Physical_Address_Province__c}" />
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Postal_Address_Province__c.Label}"/>
                        <apex:outputField value="{!newForm.Local_Postal_Address_Province__c}" styleClass="PostalProvince"/>
                        
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Physical_Address_Area__c.Label}"/>
                       	<apex:outputField value="{!newForm.Local_Physical_Address_Area__c}" />
                        <apex:outputLabel value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Local_Postal_Address_Area__c.Label}"/>
                        <apex:outputField value="{!newForm.Local_Postal_Address_Area__c}" styleClass="PostalArea"/>
                        
                        <apex:outputLabel value="Country"/>
                       	<apex:outputText value="{!isocountry.IATA_Country_ZH__c}" label="Country"/>
                        <apex:outputLabel value="Country"/>
                       	<apex:outputText value="{!isocountry.IATA_Country_ZH__c}" label="Country"/>
                    </apex:panelGrid>
                </apex:pageBlockSection>
    </div>
    <div style="page-break-after:always;margin-top:30px;">
    <apex:pageBlockSection columns="1" collapsible="false">
    <apex:facet name="header">
                        <apex:outputpanel >
                            SECTION 3 - Agency Contacts
                            <apex:commandButton styleclass="btn btn-default" action="{!goToStep3Section3}" value="{!$Label.ISSP_EditSection}" id="Edit-Section3" style="float:right"/>
                        </apex:outputpanel>                       
       		</apex:facet>
    <apex:panelGrid columns="1" width="100%" cellpadding="10%">
        <apex:dataTable value="{!List_AgencyContacts}" var="agencycon" width="100%" columnsWidth="15%,30%,25%,30%">
            <apex:facet name="header">3.1 PEOPLE</apex:facet>
            <apex:column style="vertical-align: text-top;" headerClass="pdfHeader">
            	<apex:facet name="header">Name</apex:facet>
            	<apex:outputText value="{!agencycon.First_name__c} {!agencycon.Last_name__c} {!IF(isocountry.ISO_Code__c=='CN','('+agencycon.Local_First_name__c + ' ' + agencycon.Local_Last_Name__c + ')','')}"/>
        	</apex:column>
            <apex:column style="vertical-align: text-top;" headerClass="pdfHeader">
                <apex:facet name="header">Contact Details</apex:facet>
            	{!$ObjectType.Contact.fields.Phone.Label}: {!agencycon.Phone__c}<br/>
       			{!$ObjectType.Contact.fields.MobilePhone.Label}: {!agencycon.Mobile__c}<br/>
  				{!$ObjectType.Contact.fields.Email.Label}: {!agencycon.Email__c}<br/>
 				{!$ObjectType.Contact.fields.FAX.Label}: {!agencycon.FAX__c}<br/>
				{!$Label.ISSP_Address}: {!agencycon.AddressStreet__c}
				<span style="display:{!IF(agencycon.AddressPostcode__c == '','none','')}">, </span>
 				{!agencycon.AddressPostcode__c}
    			<span style="display:{!IF(agencycon.AddressCity__c == '','none','')}">, </span>
  				{!agencycon.AddressCity__c}
 				<span style="display:{!IF(agencycon.AddressCountry__c == '','none','')}">, </span>
				{!agencycon.AddressCountry__c}
			</apex:column>
        	<apex:column style="vertical-align: text-top;" headerClass="pdfHeader">
  				<apex:facet name="header">{!$Label.ISSP_AMS_Additional_Data}</apex:facet>
   				{!$ObjectType.AMS_Accreditation_Contact__c.fields.Financial_interest__c.Label}: {!TEXT(agencycon.Financial_interest__c)}<br/>
				{!$ObjectType.AMS_Accreditation_Contact__c.fields.Time_devoted_to_Agency_business__c.Label}: {!TEXT(agencycon.Time_devoted_to_Agency_business__c)}<br/>
            </apex:column>
            <apex:column style="vertical-align: text-top;" headerClass="pdfHeader" width="20%" >
                <apex:facet name="header">Roles &amp; Responsibilities</apex:facet>
                <apex:outputText rendered="{!agencycon.Agency_owner__c}" value="{!CASE(ownerLabelType, 'shareholder', $Label.ISSP_AMS_Shareholder, 'partner', $Label.ISSP_AMS_Partner, $Label.ISSP_AMS_Owner)}:"/><apex:image value="/img/checkbox_checked.gif" rendered="{!agencycon.Agency_owner__c}"/><br/>
                <apex:outputText rendered="{!agencycon.Agency_role__c}" value="{!CASE(roleLabelType, 'director', $Label.ISSP_AMS_Director, $Label.ISSP_AMS_Manager)}:"/><apex:image value="/img/checkbox_checked.gif" rendered="{!agencycon.Agency_role__c}"/><br/>
                <apex:outputText rendered="{!agencycon.Authorised_signatory__c}" value="{!$Label.ISSP_AMS_Authorised_Sig}:" escape="false"/><apex:image value="/img/checkbox_checked.gif" rendered="{!agencycon.Authorised_signatory__c}"/><br/>
                <apex:outputText rendered="{!agencycon.Financial_Assessment_Contact__c}" value="{!$Label.ISSP_AMS_Fin_Contact}:" escape="false"/><apex:image value="/img/checkbox_checked.gif" rendered="{!agencycon.Financial_Assessment_Contact__c}"/><br/>
                <apex:outputText rendered="{!agencycon.id == newForm.ISSP_AMS_Portal_admin__c}" value="{!$Label.ISSP_AMS_Portal_Admin}:" escape="false"/><apex:image value="/img/checkbox_checked.gif" rendered="{!agencycon.id == newForm.ISSP_AMS_Portal_admin__c}"/>
            </apex:column>                                                      
        </apex:dataTable>
        <br/>
        <apex:dataTable value="{!List_AgencyCompanies}" var="agencycon" width="100%" columnsWidth="30%,30%,20%,20%" rendered="{!List_AgencyCompanies.size != 0}">
            <apex:facet name="header">3.2 COMPANIES</apex:facet>
            <apex:column style="vertical-align: text-top;" headerClass="pdfHeader">
            	<apex:facet name="header">Name</apex:facet>
            	<apex:outputText value="{!agencycon.Name}"/>
        	</apex:column>
            <apex:column style="vertical-align: text-top;" headerClass="pdfHeader">
            	<apex:facet name="header">Registration Number</apex:facet>
            	<apex:outputText value="{!agencycon.Registration_number__c}"/>
        	</apex:column>
            <apex:column style="vertical-align: text-top;" headerClass="pdfHeader">
            	<apex:facet name="header">IATA Code</apex:facet>
            	<apex:outputText value="{!agencycon.IATACode__c}"/>
        	</apex:column>
            <apex:column style="vertical-align: text-top;" headerClass="pdfHeader">
            	<apex:facet name="header">% Financial Interest</apex:facet>
            	<apex:outputText value="{!agencycon.Financial_interest__c}"/>
        	</apex:column>
         </apex:dataTable>
        </apex:panelGrid>
      </apex:pageBlockSection>
    </div>
    
    <div style="page-break-after:always;">
    <apex:pageBlockSection columns="1" collapsible="false">
    <apex:facet name="header">
                        <apex:outputpanel >
                            SECTION 4 - Additional Agent Details
                            <apex:commandButton styleclass="btn btn-default" action="{!goToStep3Section4}" value="{!$Label.ISSP_EditSection}" id="Edit-Section4" style="float:right"/>
                        </apex:outputpanel>                       
       		</apex:facet>
    <apex:panelGrid columns="3" width="100%" headerClass="bg-primary" cellpadding="10%" columnClasses="col1, col2_4, col3">
        <apex:outputText value="* Has your firm ever held a Cargo Agency Agreement executed by IATA?"/>
        <apex:outputText value="{!newForm.Has_your_firm_ever_held_a_Cargo_Agency__c}"/>
        <apex:outputText >
            <apex:panelGrid columns="4" rendered="{!newForm.Has_your_firm_ever_held_a_Cargo_Agency__c=='Yes'}">
            	<apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.IATA_Agents_Numeric_Code__c.Label}"/>
                <apex:outputText value="{!newForm.IATA_Agents_Numeric_Code__c}"/>
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Date_issued__c.Label}" />
                <apex:outputText value="{!newForm.Date_issued__c}"/>
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Name_under_which_it_was_issued__c.Label}" />
                <apex:outputText value="{!newForm.Name_under_which_it_was_issued__c}"/>
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Reason_for_cancellation__c.Label}" />
                <apex:outputText value="{!newForm.Reason_for_cancellation__c}"/>
            </apex:panelGrid>
        </apex:outputText>
        
        <apex:outputText value="* Has your firm ever held a CASS Associate Agreement executed by IATA?"/>
        <apex:outputText value="{!newForm.Has_your_form_ever_held_CASS_Assoc_etc__c}"/>
        <apex:outputText >
            <apex:panelGrid columns="4" rendered="{!newForm.Has_your_form_ever_held_CASS_Assoc_etc__c=='Yes'}">
            	<apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.CASS_Assoc_Code__c.Label}"/>
                <apex:outputText value="{!newForm.CASS_Assoc_Code__c}"/>
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.CASS_Date_issued__c.Label}" />
                <apex:outputText value="{!newForm.CASS_Date_issued__c}"/>
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.CASS_Name_under_which_it_was_issued__c.Label}" />
                <apex:outputText value="{!newForm.CASS_Name_under_which_it_was_issued__c}"/>
                <apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.CASS_Reason_for_cancellation__c.Label}" />
                <apex:outputText value="{!newForm.CASS_Reason_for_cancellation__c}"/>
            </apex:panelGrid>
        </apex:outputText>
        
        <apex:outputText value="* Does your firm intend to request authorization to deliver consignments for transportation to air carrier(s) at airport(s) in any country(ies) adjacent to the country for which this application is being made?"/>
        <apex:outputText value="{!newForm.Does_your_firm_intend_to_request__c}"/>
         <apex:outputText >
        <apex:outputText rendered="{!newForm.Does_your_firm_intend_to_request__c=='Yes'}">
        	<apex:outputText value="* List the airport(s) to be authorized and to be reflected on your Cargo Agency Agreement. " />
            <apex:outputField value="{!newForm.List_the_airport_s_to_be_authorized__c}"/>
        </apex:outputText>
        </apex:outputText>
        
        <apex:outputText value="* Is your firm an approved IATA Passenger Sales Agent?"/>
        <apex:outputText value="{!newForm.Is_your_firm_an_approved_IATA_Passenger__c}" />
        <apex:outputText >
        <apex:outputText rendered="{!newForm.Is_your_firm_an_approved_IATA_Passenger__c=='Yes'}">
        	<apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.Agents_Numeric_Code_of_your_Head_Office__c.Label}"/>
            <apex:outputfield value="{!newForm.Agents_Numeric_Code_of_your_Head_Office__c}"/>
        </apex:outputText>
        </apex:outputText>
       
        <apex:outputText value="Does the air transportation which your firm sells/handles include goods owned by your firm? "/>
        <apex:outputText value="{!newForm.Does_the_air_transportation_which_your__c}"/>
         <apex:outputText >
        <apex:outputText rendered="{!newForm.Does_the_air_transportation_which_your__c=='Yes'}">
        	<apex:outputText value="* State the percentage of your gross monetary volume of yearly air cargo transportation business represented by the transportation of such goods"/>
            <apex:outputfield value="{!newForm.State_the_percentage_of_your_gross__c}" />
        </apex:outputText>
        </apex:outputText>
        
        <apex:outputText value="Does your firm sell air cargo transportation to the general public?"/>
        <apex:outputText value="{!newForm.Does_your_firm_sell_air_cargo__c}" />
        <apex:outputText >
        <apex:outputText rendered="{!newForm.Does_your_firm_sell_air_cargo__c=='Yes'}">
        	<apex:outputText value="* What percentage of your sales is to the general public?"/>
          	<apex:outputfield value="{!newForm.What_percentage_of_your_sales__c}"/>	
        </apex:outputText>
        </apex:outputText>
               
        <apex:outputText value="Is your firm a General Sales and Service Agent (GSSA) for any air carrier(s)?"/>
        <apex:outputText value="{!newForm.Is_your_firm_a_General_Sales_Agent_GSA__c}"/>
        <apex:outputText >
        <apex:outputText rendered="{!newForm.Is_your_firm_a_General_Sales_Agent_GSA__c=='Yes'}">
        	<apex:outputText value="{!$ObjectType.AMS_Pax_Accreditation_Form__c.fields.List_carriers__c.Label}" />
            <apex:outputfield value="{!newForm.List_carriers__c}" />
