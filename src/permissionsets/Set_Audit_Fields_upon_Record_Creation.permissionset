<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Permission set is used to enable &quot;Set Audit Fields upon Record Creation&quot; [Set audit fields, such as Created By and Last Modified By, when you create a record (API only).] for standard profiles</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Set Audit Fields upon Record Creation</label>
    <userPermissions>
        <enabled>true</enabled>
        <name>CreateAuditFields</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>UpdateWithInactiveOwner</name>
    </userPermissions>
</PermissionSet>
