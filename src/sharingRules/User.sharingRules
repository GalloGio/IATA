<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingGuestRules>
        <fullName>Share_users_with_CS_Portal_Guest</fullName>
        <accessLevel>Read</accessLevel>
        <description>share all users with CS Portal guest user for login</description>
        <label>Share users with CS Portal Guest</label>
        <sharedTo>
            <guestUser>CS_Portal</guestUser>
        </sharedTo>
        <criteriaItems>
            <field>LastName</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
